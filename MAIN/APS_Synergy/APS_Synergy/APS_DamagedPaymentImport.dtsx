              SQLTask:SqlStatementSource="UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Site &lt;' + LTRIM(RTRIM(SP.CampusID)) + '&gt;' + ' not found in TIPWeb-IT'&#xA;ELSE Note + '; ' +  'Site &lt;' + LTRIM(RTRIM(SP.CampusID)) + '&gt;' + ' not found in TIPWeb-IT' END&#xA;FROM _ETL_StagingChargePayments SP&#xA;LEFT JOIN tblTechSites SD ON LTRIM(RTRIM(SP.CampusID)) = SD.SiteID&#xA;WHERE SD.SiteID IS NULL&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'StudentID missing'&#xA;ELSE Note + '; ' +  'StudentID missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL(StudentID, ''))) = ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'ISBN missing'&#xA;ELSE Note + '; ' +  'ISBN missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL(ISBN, ''))) = ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Accession missing'&#xA;ELSE Note + '; ' +  'Accession missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL(Accession, ''))) = ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'TransactionDate missing'&#xA;ELSE Note + '; ' +  'TransactionDate missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL([TransactionDate], ''))) = ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid TransactionDate'&#xA;ELSE Note + '; ' +  'Invalid TransactionDate' END&#xA;WHERE ISDATE(LTRIM(RTRIM([TransactionDate]))) = 0&#xA;AND LTRIM(RTRIM(ISNULL([TransactionDate], ''))) &lt;&gt; ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'TransReceiptID missing'&#xA;ELSE Note + '; ' +  'TransReceiptID missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL([TransReceiptID], ''))) = ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'PaymentAmount missing'&#xA;ELSE Note + '; ' +  'PaymentAmount missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL([PaymentAmount], ''))) = ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid PaymentAmount'&#xA;ELSE Note + '; ' +  'Invalid PaymentAmount' END&#xA;WHERE ISNUMERIC(LTRIM(RTRIM([PaymentAmount]))) = 0&#xA;AND LTRIM(RTRIM(ISNULL([PaymentAmount], ''))) &lt;&gt; ''&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingChargePayments&#xA;SET [Notes]  = ''&#xA;WHERE [Notes] IS NULL&#xA;OR [Notes] = 'NULL'&#xA;GO&#xA;&#xA;UPDATE  dbo._ETL_StagingChargePayments&#xA;SET Accepted = 1&#xA;WHERE Accepted IS NULL&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
