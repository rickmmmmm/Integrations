              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo._ETL_ChargePayments', 'U') IS NOT NULL DROP TABLE dbo._ETL_ChargePayments;&#xA;&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;&#xA;GO&#xA;&#xA;SET QUOTED_IDENTIFIER ON&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[_ETL_ChargePayments](&#xA;&#x9;[ETLChargePaymentUID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[CampusID] [int] NULL,&#xA;&#x9;[StudentID] [varchar](50) NULL,&#xA;&#x9;[ISBN] [varchar](50) NULL,&#xA;&#x9;[Accession] [varchar](50) NULL,&#xA;&#x9;[Copies] [int] NULL,&#xA;&#x9;[ChargeAmount] [money] NULL,&#xA;&#x9;[TotalPaid] [money] NULL,&#xA;&#x9;[TotalOwed] [money] NULL,&#xA;&#x9;[ModifiedDate] [datetime] NOT NULL,&#xA;&#x9;[Notes] [varchar](7500) NULL,&#xA;&#x9;[DateSatisfied] [datetime] NULL,&#xA;&#x9;[ChargeUID] [int] NULL,&#xA;&#x9;[ChargePaymentUID] [int] NULL,&#xA;&#x9;[TempOwed] [money] NULL,&#xA;&#x9;[TempChargeUID] [int] NULL,&#xA; CONSTRAINT [PK__ETL_ChargePayments] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[ETLChargePaymentUID] ASC&#xA;)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
