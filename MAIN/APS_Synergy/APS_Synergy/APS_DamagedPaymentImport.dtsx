          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="UPDATE _ETL_StagingChargePayments&#xA;SET PaymentNotes = 'Existing'&#xA;FROM _ETL_StagingChargePayments SCP&#xA;LEFT JOIN tblUnvChargePayments UCP&#xA;ON 'Synergy Transaction ReceiptID: ' + LTRIM(RTRIM(SCP.TransReceiptID)) LIKE '%' + UCP.Notes + '%'&#xA;WHERE Accepted = 1&#xA;AND UCP.ChargePaymentUID IS NOT NULL&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE _ETL_StagingChargePayments&#xA;SET PaymentNotes = 'Synergy Transaction ReceiptID: ' + LTRIM(RTRIM(TransReceiptID))&#xA;WHERE ETLStagingChargePaymentUID IN&#xA;(SELECT MAX(ETLStagingChargePaymentUID) AS ID&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE Accepted = 1&#xA;AND PaymentNotes IS NULL&#xA;GROUP BY StudentID, Accession)&#xA;&#xA;GO&#xA;&#xA;&#xA;WHILE (SELECT COUNT(*)&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE PaymentNotes IS NULL) &gt; 0&#xA;BEGIN&#xA;&#x9;UPDATE _ETL_StagingChargePayments&#xA;&#x9;SET PaymentNotes = 'Process'&#xA;&#x9;FROM _ETL_StagingChargePayments SCP&#xA;&#x9;JOIN (SELECT MAX(ETLStagingChargePaymentUID) ETLStagingChargePaymentUID,&#xA;&#x9;StudentID, Accession&#xA;&#x9;FROM _ETL_StagingChargePayments SCPJ&#xA;&#x9;WHERE SCPJ.PaymentNotes IS NULL&#xA;&#x9;GROUP BY SCPJ.StudentID, SCPJ.Accession) SCPJ&#xA;&#x9;ON SCP.ETLStagingChargePaymentUID = SCPJ.ETLStagingChargePaymentUID&#xA;&#x9;AND SCP.StudentID = SCPJ.StudentID&#xA;&#x9;AND SCP.Accession = SCPJ.Accession&#xA;&#x9;&#xA;&#x9;UPDATE SCP&#xA;&#x9;SET SCP.PaymentNotes = SCP.PaymentNotes + '; ' + 'Synergy Transaction ReceiptID: ' + LTRIM(RTRIM(SCPJ.TransReceiptID))&#xA;&#x9;FROM _ETL_StagingChargePayments SCP&#xA;&#x9;JOIN _ETL_StagingChargePayments SCPJ&#xA;&#x9;ON SCP.StudentID = SCPJ.StudentID&#xA;&#x9;AND SCP.Accession = SCPJ.Accession&#xA;&#x9;WHERE SCP.PaymentNotes IS NOT NULL&#xA;&#x9;AND SCP.PaymentNotes &lt;&gt; 'Existing'&#xA;&#x9;AND SCP.PaymentNotes &lt;&gt; 'Process'&#xA;&#x9;AND SCP.PaymentNotes &lt;&gt; 'Processed'&#xA;&#x9;AND SCPJ.PaymentNotes IS NOT NULL&#xA;&#x9;AND SCPJ.PaymentNotes = 'Process'&#xA;&#x9;&#xA;&#x9;UPDATE _ETL_StagingChargePayments&#xA;&#x9;SET PaymentNotes = 'Processed'&#xA;&#x9;WHERE PaymentNotes = 'Process'&#xA;END&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE _ETL_StagingChargePayments&#xA;SET PaymentAmount = CONVERT(VARCHAR, SJ.Amount)&#xA;FROM _ETL_StagingChargePayments SP&#xA;JOIN (SELECT MAX(ETLStagingChargePaymentUID) AS ID,&#xA;SUM(CONVERT(MONEY, LTRIM(RTRIM(PaymentAmount)))) AS Amount&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE Accepted = 1&#xA;AND PaymentNotes &lt;&gt; 'Existing'&#xA;GROUP BY StudentID, Accession) SJ ON SP.ETLStagingChargePaymentUID = SJ.ID&#xA;&#xA;GO&#xA;&#xA;&#xA;INSERT INTO dbo._ETL_ChargePayments&#xA;(CampusID,StudentID ,ISBN, Accession, Copies, ChargeAmount,&#xA;TotalPaid,TotalOwed, ModifiedDate,Notes,DateSatisfied)&#xA;SELECT LTRIM(RTRIM(CampusID)), LTRIM(RTRIM(StudentID)), LTRIM(RTRIM(ISBN)), LTRIM(RTRIM(Accession)),&#xA;1, NULL, CONVERT(MONEY, LTRIM(RTRIM(PaymentAmount))), NULL,&#xA;CONVERT(DATETIME, LTRIM(RTRIM([TransactionDate]))),&#xA;CASE WHEN LTRIM(RTRIM(ISNULL(_ETL_StagingChargePayments.Notes, ''))) = '' THEN&#xA;PaymentNotes ELSE&#xA;PaymentNotes + '; Notes: ' + _ETL_StagingChargePayments.Notes END,&#xA;NULL&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE ETLStagingChargePaymentUID IN&#xA;(SELECT MAX(ETLStagingChargePaymentUID) AS ID&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE Accepted = 1&#xA;AND PaymentNotes &lt;&gt; 'Existing'&#xA;GROUP BY StudentID, Accession)&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
