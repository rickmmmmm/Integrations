          DTS:Description="Execute SQL Task"
          DTS:DTSID="{DB7F807F-61BB-4E7D-9339-10ED105EFCC1}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Data Preparation"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="--Change location name to 'Music Warehouse' if the location is 'Retired'&#xA;UPDATE dbo._ETL_Inventory&#xA;SET [Site] = (SELECT Top 1 SiteID FROM tblTechSites WHERE SiteName = 'MusicWarehouse')&#xA;WHERE [Site] = (SELECT Top 1 SiteID FROM tblTechSites WHERE SiteName = 'Retired')&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Inventory&#xA;SET [Status] = CASE WHEN [Site] = (SELECT Top 1 SiteID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   FROM tblTechSites &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   WHERE SiteName = 'MusicWarehouse')&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;THEN  'Disposed' &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Available' END,&#xA;&#x9;[Site] = CASE WHEN [Site] = (SELECT Top 1 SiteID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM tblTechSites &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHERE SiteName = 'Retired')&#xA;&#x9;&#x9;&#x9;&#x9;  THEN (SELECT Top 1 SiteID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM tblTechSites &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE SiteName = 'MusicWarehouse') &#xA;&#x9;&#x9;&#x9;&#x9;  ELSE [Site] END,&#xA;&#x9; Location = CASE WHEN [Site] = (SELECT Top 1 SiteID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM tblTechSites &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  WHERE SiteName = 'MusicWarehouse')&#xA;&#x9;&#x9;&#x9;&#x9;   THEN 'MusicWarehouse' &#xA;WHEN [Site] = '999'&#x9;&#x9;&#x9;&#x9;&#x9; THEN 'VPA'&#xA;ELSE 'BandHallCharms' END&#x9;&#x9;&#x9;&#x9;    &#x9;&#x9;&#x9;&#x9;   &#x9;&#xA;FROM dbo._ETL_Inventory&#xA;&#xA;GO&#xA;&#xA;INSERT INTO tblUnvManufacturers(ManufacturerName,&#xA;CreatedByUserID,&#xA;CreatedDate,&#xA;LastModifiedByUserID,&#xA;LastModifiedDate)&#xA;SELECT DISTINCT Manufacturer,0,GETDATE(),0,GETDATE()&#xA;FROM _ETL_Inventory&#xA;WHERE Manufacturer NOT IN (SELECT ManufacturerName&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;      FROM tblUnvManufacturers)&#x9;&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Import Data into TIPWeb-IM\INSERT Inventory"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{89CAFBDF-CA64-4738-BA73-93F0E32A237A}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="INSERT Inventory"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="--Insert asset into TIPWebIT when Barcode == Tag &amp; Category (in Charms) &lt;&gt; Product Type (in TIPWebIT)&#xA;&#xA;INSERT INTO tblTechInventory(&#xA;InventoryTypeUID,&#xA;ItemUID,&#xA;SiteUID,&#xA;EntityUID,&#xA;EntityTypeUID,&#xA;StatusUID,&#xA;TechDepartmentUID,&#xA;Tag,&#xA;Serial,&#xA;FundingSourceUID,&#xA;InventoryNotes,&#xA;CreatedByUserID,&#xA;CreatedDate,&#xA;LastModifiedByUserID,&#xA;LastModifiedDate,&#xA;ArchiveUID,&#xA;InventorySourceUID,&#xA;ContainerUID)&#xA;SELECT 1,G.ItemUID,I.SiteUID,E.RoomUID,2,C.statusID,D.TechDepartmentUID,B.Tag + 'BND',B.Serial,F.FundingSourceUID,&#xA;'Charms Import',0,GETDATE(),0,GETDATE(),0,4,0&#xA;FROM _ETL_Inventory B &#xA;LEFT JOIN tblStatus C ON B.Status = C.Statusdesc AND C.StatusTypeUID = 6&#xA;LEFT JOIN tblTechDepartments D ON D.DepartmentName = B.Department&#xA;LEFT JOIN tblUnvRooms E ON E.RoomNumber = B.Location &#xA;LEFT JOIN tblFundingSources F ON F.FundingSource = B.FundingSource&#xA;LEFT JOIN tblTechItems G ON G.ItemName = B.Product&#x9;&#xA;LEFT JOIN tblUnvManufacturers H ON H.ManufacturerUID = G.ManufacturerUID&#x9;&#x9;&#x9;&#x9;&#x9; &#xA;LEFT JOIN tblTechSites I ON I.SiteID = B.Site&#xA;LEFT JOIN tblTechItemTypes J ON J.ItemTypeUID = B.ItemTypeUID&#xA;LEFT JOIN tblTechInventory K ON B.Tag = K.Tag &#x9;&#xA;WHERE K.InventoryUID IS NOT NULL&#xA;AND B.ItemUID IS NOT NULL&#xA;AND B.ItemTypeUID &lt;&gt; (SELECT ItemTypeUID FROM tblTechItems WHERE ItemUID = K.ItemUID)  &#xA;AND J.ItemTypeName like '%MusicalInstrument%'&#x9;&#x9;&#xA;GO&#xA;&#xA;&#xA;--Insert asset into TIPWebIT when the Tag is not found&#xA;INSERT INTO tblTechInventory(&#xA;InventoryTypeUID,&#xA;ItemUID,&#xA;SiteUID,&#xA;EntityUID,&#xA;EntityTypeUID,&#xA;StatusUID,&#xA;TechDepartmentUID,&#xA;Tag,&#xA;Serial,&#xA;FundingSourceUID,&#xA;InventoryNotes,&#xA;CreatedByUserID,&#xA;CreatedDate,&#xA;LastModifiedByUserID,&#xA;LastModifiedDate,&#xA;ArchiveUID,&#xA;InventorySourceUID,&#xA;ContainerUID)&#xA;SELECT 1,G.ItemUID,I.SiteUID,E.RoomUID,2,C.statusID,D.TechDepartmentUID,B.Tag,B.Serial,F.FundingSourceUID,&#xA;'Charms Import',0,GETDATE(),0,GETDATE(),0,4,0&#xA;FROM _ETL_Inventory B &#xA;LEFT JOIN tblStatus C ON B.Status = C.Statusdesc AND C.StatusTypeUID = 6&#xA;LEFT JOIN tblTechDepartments D ON D.DepartmentName = B.Department&#xA;LEFT JOIN tblUnvRooms E ON E.RoomNumber = B.Location &#xA;LEFT JOIN tblFundingSources F ON F.FundingSource = B.FundingSource&#xA;LEFT JOIN tblTechItems G ON G.ItemName = B.Product&#x9;&#xA;LEFT JOIN tblUnvManufacturers H ON H.ManufacturerUID = G.ManufacturerUID&#x9;&#x9;&#x9;&#x9;&#x9; &#xA;LEFT JOIN tblTechSites I ON I.SiteID = B.Site&#xA;LEFT JOIN tblTechItemTypes J ON J.ItemTypeUID = B.ItemTypeUID&#xA;LEFT JOIN tblTechInventory K ON B.Tag = K.Tag &#xA;WHERE K.InventoryUID IS NULL&#xA;AND B.ItemUID IS NOT NULL&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Import Data into TIPWeb-IM\UPDATE Inventory"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{1D61A4F0-CF28-4EF2-A5DF-836B28D92B59}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="UPDATE Inventory"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="UPDATE tblTechInventory&#xA;SET StatusUID = C.StatusID,&#xA;SiteUID = G.SiteUID,&#xA;TechDepartmentUID = D.TechDepartmentUID,&#xA;InventoryNotes = CASE WHEN A.InventoryNotes IS NULL THEN 'Charms Import'&#xA;ELSE A.InventoryNotes + '; Charms Import' END,&#xA;EntityUID = E.RoomUID,&#xA;EntityTypeUID = 2,&#xA;FundingSourceUID = F.FundingSourceUID,&#xA;LastModifiedByUserID = 0,&#xA;LastModifiedDate = GETDATE()&#xA;FROM tblTechInventory A&#xA;INNER JOIN _ETL_Inventory B ON A.Tag = B.Tag&#xA;INNER JOIN tblStatus C ON B.Status = C.Statusdesc AND C.StatusTypeUID = 6&#xA;INNER JOIN tblTechDepartments D ON D.DepartmentName = B.Department&#xA;INNER JOIN tblUnvRooms E ON E.RoomNumber = B.Location &#xA;INNER JOIN tblFundingSources F ON F.FundingSource = B.FundingSource&#xA;INNER JOIN tblTechSites G ON G.SiteID = B.Site  &#xA;INNER JOIN tblTechItems H ON H.ItemUID = A.ItemUID  &#xA;INNER JOIN tblTechItemTypes I ON I.ItemTypeUID = H.ItemTypeUID&#xA;WHERE I.ItemTypeName like '%MusicalInstrument%'&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
