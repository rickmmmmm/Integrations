            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="UPDATE dbo._ETL_StagingInventory&#xA;SET Category = ''&#xA;WHERE Category IS NULL&#xA;&#xA;&#xA;&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET Barcode = ''&#xA;WHERE Barcode IS NULL&#xA;&#xA;&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET [Description] = ''&#xA;WHERE [Description] IS NULL&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET [Brand] = ''&#xA;WHERE [Brand] IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET [Model] = ''&#xA;WHERE [Model] IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET [Serial] = ''&#xA;WHERE [Serial] IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET [Location] = '999'&#xA;WHERE [Location] IS NULL OR [Location] =  ''&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET [FundingSource] = 199&#xA;WHERE ISNULL(FundingSource,'') = ''&#xA;&#xA;GO&#xA;&#xA;--Logs error if Site is not found in TIPWeb-IT&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Site &lt;' + SI.Location + '&gt;' + ' not found in TIPWeb-IT'&#xA;ELSE Note + '; ' +  'Site &lt;' + SI.Location + '&gt;' + ' not found in TIPWeb-IT' END&#xA;FROM _ETL_StagingInventory SI&#xA;LEFT JOIN tblTechSites SD ON SI.Location = SD.SiteID&#xA;WHERE SD.SiteID IS NULL&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;--Logs error if a Barcode has value 'NA' or 'N/A'&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Tag &lt;' + SI.Barcode + '&gt;' + ' cannot be imported into TIPWeb-IT'&#xA;ELSE Note + '; ' +  'Tag &lt;' + SI.Barcode + '&gt;' + ' cannot be imported into TIPWeb-IT' END&#xA;FROM _ETL_StagingInventory SI&#xA;WHERE SI.Barcode IN ('Top &amp; Bottom','Tom 1 &amp; 2','Snare Drum w/Stand','Set 1 - One Pair','Set 1',&#xA;'Set 01','Set 003','Set 002','Set 001','Set #1','S40c','Q066','None','No Case','N/A','NA')&#xA;&#xA;GO&#xA;&#xA;--Logs error if Product Type is not found in TIPWeb-IT&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Product Type &lt;' + LTRIM(RTRIM(SI.Category)) + '&gt; not found in TIPWeb-IT'&#xA;ELSE Note + '; Product Type &lt;' + LTRIM(RTRIM(SI.Category)) + '&gt; not found in TIPWeb-IT' END&#xA;FROM _ETL_StagingInventory SI&#xA;LEFT JOIN tblTechItemTypes TI ON UPPER(LTRIM(RTRIM(SI.Category))) = UPPER(TI.ItemTypeName)&#xA;WHERE TI.ItemTypeUID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE  dbo._ETL_StagingInventory&#xA;SET Accepted = 1&#xA;WHERE Accepted IS NULL" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
