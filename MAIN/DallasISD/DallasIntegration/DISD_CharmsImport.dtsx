          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="INSERT INTO _ETL_Inventory(&#xA;ItemTypeUID,&#xA;Tag,&#xA;Product,&#xA;Manufacturer,&#xA;Model,&#xA;Serial,&#xA;[Site],&#xA;FundingSource,&#xA;[Status],&#xA;Location,&#xA;Department)&#xA;SELECT (SELECT TOP 1 ItemTypeUID FROM tblTechItemTypes&#xA;&#x9;   WHERE ItemTypeName = _ETL_StagingInventory.Category),&#xA;Barcode,[Description],Brand,model,Serial,Location,FundingSource,'Available','BandHallCharms','VisualandPerformingArts'&#xA;FROM _ETL_StagingInventory&#xA;WHERE Accepted = 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
