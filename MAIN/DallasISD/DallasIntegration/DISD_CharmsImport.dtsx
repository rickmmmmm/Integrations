              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo._ETL_StagingInventory', 'U') IS NOT NULL DROP TABLE dbo._ETL_StagingInventory;&#xA;&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;&#xA;GO&#xA;&#xA;SET QUOTED_IDENTIFIER ON&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[_ETL_StagingInventory](&#xA;&#x9;[ETLStagingInventoryUID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[Category] [varchar](50) NULL,&#xA;&#x9;[Barcode] [varchar](50) NULL,&#xA;&#x9;[Description] [varchar](100) NULL,&#xA;&#x9;[Brand] [varchar](100) NULL,&#xA;&#x9;[Model] [varchar](50) NULL,&#xA;&#x9;[Serial] [varchar](50) NULL,&#xA;&#x9;[Location] [varchar](100) NULL,&#xA;&#x9;[FundingSource] [varchar] (50) NULL,&#xA;&#x9;[Accepted] [bit] NULL,&#xA;&#x9;[Note] [varchar](max) NULL,&#xA;&#x9;[Imported] [datetime] NULL&#xA; CONSTRAINT [PK__ETL_StagingInventory] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[ETLStagingInventoryUID] ASC&#xA;)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
