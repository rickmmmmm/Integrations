                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
vth.TransferNumber, vth.Name, convert(varchar(10), vth.CreatedDate, 101) as DateCreated, vth.CreatedBy, 
trSite.SiteName as OriginSite, vth.DestinationSite, vth.Status as TransferStatus, lts.statusDesc as LineLevelStatus, 
tti.Tag, tti.Serial, ttm.ItemName as ProductName, tty.ITemTypeName as ProductType, td.DepartmentName, fs.FundingSource, 
tts.statusDesc as TagStatus, tsite.SiteName as currentLocation
--, tr.*
from VTech_TransferHistory vth
join tblTechTransfers tr on vth.TransferUID = tr.TransferUID
join tblTechSites trSite on tr.SiteUID = trSite.SiteUID 
join tblTechTransferInventory tri on vth.TransferUID = tri.TransferUID
join tblStatus lts on tri.StatusID = lts.StatusID
join tblTechTransferInventoryTagged tig on tri.TransferInventoryUID = tig.TransferInventoryUID
join tblTechInventory tti on tig.InventoryUID = tti.InventoryUID
join tblTechItems ttm on tti.ItemUID = ttm.ItemUID
join tblTechItemTypes tty on ttm.ItemTypeUID = tty.ItemTypeUID
join tblTechDepartments td on tti.TechDepartmentUID = td.TechDepartmentUID
join tblTechSites tsite on tti.SiteUID = tsite.SiteUID
join tblStatus tts on tti.StatusUID = tts.StatusID
join tblFundingSources fs on tti.FundingSourceUID = fs.FundingSourceUID
order by transfernumber</property>
