                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT A.Serial,C.SiteName,
CASE WHEN A.EntityTypeUID = 4 THEN 'Student: ' + E.FullName + ' - ' + E.StudentID WHEN A.EntityTypeUID = 3 THEN 'Staff: ' + F.FirstName + ' - ' + F.TeacherID  ELSE 'Room: ' + D.RoomNumber END AS [Location],
