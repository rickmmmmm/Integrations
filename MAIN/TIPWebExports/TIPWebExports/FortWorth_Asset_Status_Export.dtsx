          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{BA0177B3-7510-48DB-A450-A8A4751E3562}"
              SQLTask:SqlStatementSource="&#xA;DECLARE @CustomerName&#x9;&#x9;&#x9;VARCHAR(100) = ?&#xA;DECLARE @JobName&#x9;&#x9;&#x9;&#x9;VARCHAR(100) = ?&#xA;DECLARE @IntergrationType&#x9;&#x9;VARCHAR(100) = ?&#xA;DECLARE @FileSize&#x9;&#x9;&#x9;&#x9;INT = ?&#xA;DECLARE @RootPath&#x9;&#x9;&#x9;&#x9;VARCHAR(200) = ?&#xA;DECLARE @ExportPath&#x9;&#x9;&#x9;&#x9;VARCHAR(100) = ?&#xA;DECLARE @ArchivePath&#x9;&#x9;&#x9;VARCHAR(100) = ?&#xA;DECLARE @FileName&#x9;&#x9;&#x9;&#x9;VARCHAR(100) = ?&#xA;DECLARE @TaskFailure&#x9;&#x9;&#x9;BIT = ?&#xA;DECLARE @JobID&#x9;&#x9;&#x9;&#x9;&#x9;INT &#xA;DECLARE @TaskErrorMessage&#x9;&#x9;VARCHAR(MAX) = 'Error In Creating File'&#xA;DECLARE @TaskName&#x9;&#x9;&#x9;&#x9;VARCHAR(100) = 'Export'&#xA;&#xA;&#xA;SELECT @JobID=id FROM FileExist WHERE Job_Name=@JobName AND client=@CustomerName AND category=@IntergrationType&#xA;&#xA;IF (@JobID IS NULL)&#xA;BEGIN&#xA;&#xA;  IF @TaskFailure=0&#xA;  BEGIN&#xA;&#x9;  INSERT INTO TaskOnError&#xA;&#x9;  (Client,JobName,Error_ID,StartTime,EndTime,Message,TaskName,LastModifiedDate,Category)&#xA;&#x9;  VALUES&#xA;&#x9;  (@CustomerName,@JobName,1,GETDATE(),GETDATE(),@TaskErrorMessage,@TaskName,GETDATE(),@IntergrationType)&#xA;  END &#xA;&#xA;&#x9;INSERT INTO FileExist&#xA;&#x9;(Client,Job_Name,FileCheck,FileName,FileSize,File_Rcvd_Date,Check_Time,Category)&#xA;&#x9;VALUES&#xA;&#x9;(@CustomerName,@JobName,CASE WHEN @TaskFailure=1 THEN 1 ELSE 0 END,&#xA;&#x9;ISNULL(@RootPath + @ExportPath + @ArchivePath + @FileName,'No File Was Created'),&#xA;&#x9;ISNULL(@FileSize,0),GETDATE(),GETDATE(),@IntergrationType)&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#xA;  IF @TaskFailure=0&#xA;  BEGIN&#xA;&#x9;  INSERT INTO TaskOnError&#xA;&#x9;  (Client,JobName,Error_ID,StartTime,EndTime,Message,TaskName,LastModifiedDate,Category)&#xA;&#x9;  VALUES&#xA;&#x9;  (@CustomerName,@JobName,1,GETDATE(),GETDATE(),@TaskErrorMessage,@TaskName,GETDATE(),@IntergrationType)&#xA;  END &#xA;&#xA;  UPDATE FileExist&#xA;  SET filesize = @FileSize,&#xA;      filename = ISNULL(@RootPath + @ExportPath + @ArchivePath + @FileName,'No File Was Created'),&#xA;&#x9;  file_rcvd_date = GETDATE(),&#xA;&#x9;  check_time = GETDATE()&#xA;  WHERE id = @JobID&#xA;&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
