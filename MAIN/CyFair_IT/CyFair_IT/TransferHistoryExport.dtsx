                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT distinct TransferNumber, CreatedBy, convert(varchar(10), CreatedDate, 101) as CreatedDate, Name, OriginSite, DestinationSite, [Status], 
        (CASE WHEN (ApprovalState = 1) AND (statusID &lt;&gt; 92) AND (statusID &lt;&gt; 97) AND (statusID &lt;&gt; 25) THEN 'Denied'
			  WHEN TransferTypeUID = 3 THEN 
                (CASE WHEN statusID IN (92, 23, 96) THEN 'Pending'
                    ELSE 'Approved'
                    END)
			  WHEN TransferTypeUID = 1 THEN 
                (CASE WHEN (statusID = 24) OR (statusID = 92) OR (statusID = 23) THEN 'Pending'
                    ELSE 'Approved'
                    END)
              WHEN TransferTypeUID = 4 THEN 
                (CASE WHEN statusID IN (92, 23) THEN 'Pending'
                    ELSE 'Approved'
                    END)
             ELSE 'Approved'
             END) AS ApprovalState, 
