                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select s.SiteID, s.SiteName, u.LoginName, u.RealName, u.Email, substring(u.Phone, 1, 3)+'-'+substring(u.Phone, 4, 3)+'-'+substring(u.Phone, 7, 4) as Phone, --u.Phone,
--u.CreatedDate, 
convert(varchar(25), u.CreatedDate, 100) as CreatedDate,
(case when ur.ViewUID = 3 then 'Site ' + ur.UserRoleName when ur.ViewUID = 2 then 'District ' + ur.UserRoleName else ur.UserRoleName end) as UserRole
--,ur.*
from tblUser u
join tblUnvUserRoles ur on u.UserRoleUID = ur.UserRoleUID
left outer join tblTechSites s on s.SiteUID = u.SiteUID
where applicationUID = 2
and u.CreatedDate &gt; DATEADD(DAY, DATEDIFF(DAY,0, GETDATE()), (case when DATEPART(dw, getdate()) = 2 then -3 else -2 end))
order by u.CreatedDate</property>
