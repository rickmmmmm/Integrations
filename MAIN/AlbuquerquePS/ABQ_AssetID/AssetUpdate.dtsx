            <SQLTask:SqlTaskData
              SQLTask:Connection="{83827DD3-B926-4E10-9048-1B80FAD7FDC3}"
              SQLTask:SqlStatementSource="DECLARE  @rpt Nvarchar(max), @mailProfile NVARCHAR(MAX), @FileExists int, @FileSize NVARCHAR(MAX), @FileSizeOver int &#xA;declare @ClientDB nvarchar(200)&#xA;&#xA;DECLARE @body nvarchar(max), @subject NVARCHAR(max), @sText nvarchar(max), @logdate NVARCHAR(50) &#xA;declare @Email nVarchar(200), @EmailCC nVarchar(200), @EmailAttachment int&#xA;&#xA;&#xA;DECLARE @sSqlBase NVarchar(100), @sSql Nvarchar(max), @ParamDef NVARCHAR(max)&#xA;DECLARE @total int, @Filetotal int, @reject INT &#xA;&#xA;&#xA;SET @mailProfile = ?&#xA;set @Email = ?&#xA;set @EmailCC = ?&#xA;set @ClientDB = ?&#xA;set @rpt = ?&#xA;&#xA;--set @ZipFileSize = ?&#xA;--set @EmailAttachment = ?&#xA;&#xA;--set @ZipFileSizeOver = 0&#xA;--if cast(@ZipFileSize as int) &gt; 1000000 &#xA;--&#x9;set @ZipFileSizeOver = 1&#x9;&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'AssetID Update in TIPWeb-IT for Albuquerque Public Schools' + ' (' + @logdate + ')'&#xA;&#xA;set @sSqlBase = N'SELECT @total = COUNT(*) FROM '+ @ClientDB + '.dbo._ETL_AssetStaging '&#xA;set @ParamDef = N'@total int OUTPUT'&#xA;&#xA;&#xA;set @sText = ''&#xA;-------------------------------------------------&#xA;&#xA;&#x9;set @sSql = @sSqlBase &#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N' where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Asset File: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#xA;set @FileExists = 0&#xA;if isNull(@reject, 0) &gt; 0&#xA;&#x9;set  @FileExists = 1&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;--IF @ZipExists &gt; 0&#xA;--BEGIN&#xA;--&#x9;IF @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;--&#x9;BEGIN&#xA;--&#x9;&#x9;set @sText = @sText + 'A zipped copy of the reject file(s) is attached.'  + '&lt;br/&gt;&lt;br/&gt;' &#xA;--&#x9;END&#xA;--&#x9;ELSE&#xA;--&#x9;BEGIN&#xA;--&#x9;&#x9;if @EmailAttachment &gt; 0&#xA;--&#x9;&#x9;BEGIN&#xA;--&#x9;&#x9;&#x9;set @sText = @sText + 'The zipped copy of the reject file(s) was too large to attach.'  + '&lt;br/&gt;'  +&#xA;-- &#x9;&#x9;&#x9;&#x9;&#x9;'You can find the file in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;--&#x9;&#x9;END&#xA;--&#x9;&#x9;ELSE&#xA;--&#x9;&#x9;BEGIN&#xA;--&#x9;&#x9;&#x9;set @sText = @sText + 'You can find Reject Files (if any) in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;--&#x9;&#x9;END&#xA;&#xA;--&#x9;END&#xA;--END&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;'&lt;p&gt;' + @sText + '&lt;/p&gt;&lt;br/&gt;' +&#xA;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9; '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;&#x9; '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;if @FileExists &gt; 0 &#xA;BEGIN&#xA;&#x9;--print @sText  + ' ' + 'attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @EmailCC,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject,&#xA;&#x9;&#x9;@file_attachments = @rpt;&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;&#xA;&#x9;--print @sText  + ' ' + 'no attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @EmailCC,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject;&#xA;END&#xA;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
