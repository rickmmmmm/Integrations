            <DTS:Executable
              DTS:refId="Package\Site List\Process In Transit Transfers\Get Untagged Count"
              DTS:CreationName="Microsoft.ExecuteSQLTask"
              DTS:Description="Execute SQL Task"
              DTS:DTSID="{B0476176-8245-4019-8CFF-A19A1DC287A6}"
              DTS:ExecutableType="Microsoft.ExecuteSQLTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="Get Untagged Count"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{98E290BE-BE98-4915-BCBC-6632D35A60EC}"
                  SQLTask:SqlStatementSource="declare @TransferUID int&#xA;&#xA;set @TransferUID = ?&#xA;&#xA;&#xA;&#xA;select count(*) as untagged from tblTechTransferInventory &#xA;where TransferUID = @TransferUID and UntaggedInventoryUID &gt; 0"
                  SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
                  <SQLTask:ResultBinding
                    SQLTask:ResultName="untagged"
                    SQLTask:DtsVariableName="User::UntaggedInventoryCount" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@TransferUID"
                    SQLTask:DtsVariableName="User::TransferUID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                </SQLTask:SqlTaskData>
              </DTS:ObjectData>
            </DTS:Executable>
            <DTS:Executable
              DTS:refId="Package\Site List\Process In Transit Transfers\Process In transit Transfers"
              DTS:CreationName="Microsoft.ExecuteSQLTask"
              DTS:Description="Execute SQL Task"
              DTS:DTSID="{7B082A96-B9CB-40E3-9C5B-FBE687087EA4}"
              DTS:ExecutableType="Microsoft.ExecuteSQLTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="Process In transit Transfers"
              DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 RC1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{98E290BE-BE98-4915-BCBC-6632D35A60EC}"
                  SQLTask:SqlStatementSource="&#xA;declare @DestinationSite int, @TransferUID int,  @TagTransferStatusUID int,  @RoomUID int, @ContainerUID int&#xA;&#xA;declare @ReceivingTransferStatusUID int, @CompletedTransferStatusUID int&#xA;&#xA;declare @tagCount int, @OrigSiteName varchar(50)&#xA;&#xA;set @DestinationSite = ?&#xA;set @TransferUID = ?&#xA;set @TagTransferStatusUID = ?&#xA;set @RoomUID = ?&#xA;set @ContainerUID = ?&#xA;&#xA;select @ReceivingTransferStatusUID = (select statusid from tblStatus where statustypeuid = 4 and statusdesc = 'Receiving')&#xA;select @CompletedTransferStatusUID = (select statusid from tblStatus where statustypeuid = 4 and statusdesc = 'Complete')&#xA;--print 'Receiving: ' + cast(@ReceivingTransferStatusUID as varchar(10))&#xA;&#xA;&#xA;&#x9;--Add history record for receiving&#xA;&#x9;INSERT INTO tblTechTransferHistory&#xA;&#x9;(TransferUID, StatusUID, OriginStatusUID, Denied,&#xA;&#x9;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;&#x9;SELECT T.TransferUID, @ReceivingTransferStatusUID, T.StatusUID, 0,&#xA;&#x9;0, GETDATE(), 0, GETDATE()&#xA;&#x9;FROM tblTechTransfers T&#xA;&#x9;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#x9;--Update transfer to Receiving&#xA;&#x9;Update t&#xA;&#x9;set StatusUID = @CompletedTransferStatusUID, LastModifiedByUserID = 0, LastModifiedDate = getdate()&#xA;&#x9;--select *&#xA;&#x9;from tblTechTransfers t&#xA;&#x9;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#xA;&#x9;--Add history for new location&#xA;&#x9;INSERT INTO tblTechInventoryHistory&#xA;&#x9;(InventoryUID, InventoryTypeUID, SiteUID, EntityUID, EntityTypeUID, &#xA;&#x9;StatusUID,&#xA;&#x9;OriginSiteUID, OriginStatusUID, OriginEntityUID, OriginEntityTypeUID, InventoryHistoryNotes,&#xA;&#x9;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate, &#xA;&#x9;ParentInventoryUID, OriginParentInventoryUID, BulkEditUID, InventorySourceUID, ContainerUID, OriginContainerUID)&#xA;&#x9;SELECT I.InventoryUID, I.InventoryTypeUID, T.DestinationSiteUID, @RoomUID, 2, &#xA;&#x9;@TagTransferStatusUID,&#xA;&#x9;I.SiteUID, I.StatusUID, I.EntityUID, I.EntityTypeUID, 'Transfer Received via Automated Process',&#xA;&#x9;0, GETDATE(), 0, GETDATE(), &#xA;&#x9;I.ParentInventoryUID, I.ParentInventoryUID, 0, I.InventorySourceUID, I.ContainerUID, I.ContainerUID&#xA;&#x9;FROM tblTechInventory I&#xA;&#x9;JOIN tblTechTransferInventory TI ON I.InventoryUID = TI.InventoryUID&#xA;&#x9;JOIN tblStatus S ON TI.OriginStatusUID = S.statusID&#xA;&#x9;JOIN tblTechTransfers T ON TI.TransferUID = T.TransferUID &#xA;&#x9;where ti.transferUID = @TransferUID&#xA;&#x9;and ti.InventoryUID &gt; 0&#xA;&#xA;&#x9;--update tag to new location&#xA;&#x9;UPDATE I&#xA;&#x9;SET SiteUID = T.DestinationSiteUID, EntityUID = @RoomUID, EntityTypeUID = 2,&#xA;&#x9;StatusUID = @TagTransferStatusUID, LastModifiedByUserID = 0, LastModifiedDate = GETDATE()&#xA;&#x9;--select *&#xA;&#x9;FROM tblTechInventory I&#xA;&#x9;JOIN tblTechTransferInventory TI ON I.InventoryUID = TI.InventoryUID&#xA;&#x9;JOIN tblStatus S ON TI.OriginStatusUID = S.statusID&#xA;&#x9;JOIN tblTechTransfers T ON TI.TransferUID = T.TransferUID&#xA;&#x9;JOIN tblTechTransfers tr ON TI.TransferUID = tr.TransferUID &#xA;&#x9;WHERE TI.InventoryUID &gt; 0&#xA;&#x9;and ti.transferUID = @TransferUID&#xA;&#xA;&#x9;--Receive Inventory (tags)&#xA;&#x9;--select *&#xA;&#x9;Update ti set Received = 1, ReceivedQuantity = 1, LastModifiedByUserID = 0, LastModifiedDate = getdate()&#xA;&#x9;from tblTechTransferInventory ti&#xA;&#x9;join tblTechInventory tti on ti.InventoryUID = tti.InventoryUID&#xA;&#x9;where ti.TransferUID = @TransferUID&#xA;&#x9;and ti.InventoryUID &gt; 0&#xA;&#xA;&#xA;&#xA;&#x9;--Receive Untagged Inventory into Container&#xA;&#x9;INSERT INTO tblTechUntaggedInventory&#xA;&#x9;(ContainerUID, ItemUID, Quantity, InventorySourceUID, Identifier, CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate) &#xA;&#x9;(select @ContainerUID, ItemUID, tui.Quantity, InventorySourceUID, null, tui.CreatedbyUserID, tui.CreatedDate, 0, getdate()&#xA;&#x9;from tblTechTransferInventory ttr&#xA;&#x9;join tblTechUntaggedInventory tui on ttr.UntaggedInventoryUID = tui.UntaggedInventoryUID&#xA;&#x9;where ttr.UntaggedInventoryUID &gt; 0&#xA;&#x9;and ttr.TransferUID = @TransferUID)&#xA;&#xA;&#x9;&#xA;&#x9;--Receive Untagged Inventory&#xA;&#x9;--select *&#xA;&#x9;Update ti set Received = 1, ReceivedQuantity = tui.Quantity, LastModifiedByUserID = 0, LastModifiedDate = getdate()&#xA;&#x9;from tblTechTransferInventory ti&#xA;&#x9;join tblTechUntaggedInventory tui on tui.UntaggedInventoryUID = ti.UntaggedInventoryUID&#xA;&#x9;where ti.TransferUID = @TransferUID&#xA;&#x9;and ti.UntaggedInventoryUID &lt;&gt; 0&#xA;&#xA;&#xA;&#x9;--Transfer History from Receiving to Complete&#xA;&#x9;INSERT INTO tblTechTransferHistory&#xA;&#x9;(TransferUID, StatusUID, OriginStatusUID, Denied,&#xA;&#x9;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;&#x9;SELECT T.TransferUID, @CompletedTransferStatusUID, T.StatusUID, 0,&#xA;&#x9;0, GETDATE(), 0, GETDATE()&#xA;&#x9;FROM tblTechTransfers T&#xA;&#x9;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#x9;--Set Transfer to Complete&#xA;&#x9;Update t&#xA;&#x9;set StatusUID = @CompletedTransferStatusUID, LastModifiedByUserID = 0, LastModifiedDate = getdate()&#xA;&#x9;--select *&#xA;&#x9;from tblTechTransfers t&#xA;&#x9;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#x9;select @tagCount = count(distinct Tag)&#xA;&#x9;from tblTechInventory tti&#xA;&#x9;join tblTechInventoryHistory ttih on tti.InventoryUID = ttih.InventoryUID and OriginEntityTypeUID = 6 and OriginEntityUID = @TransferUID &#xA;&#x9;join tblTechTransfers ttr on ttr.TransferUID = ttih.OriginEntityUID&#xA;&#x9;join tblTechSites ts on ts.SiteUID = tti.SiteUID&#xA;&#x9;join tblUnvRooms rm on tti.EntityUID = rm.RoomUID&#xA;&#x9;join tblTechItems ttm on tti.ItemUID = ttm.ItemUID&#xA;&#x9;join tblTechItemTypes tty on tty.ItemTypeUID = ttm.ItemTypeUID&#xA;&#x9;where tti.siteuid = @DestinationSite &#xA;&#xA;&#x9;select @OrigSiteName = SiteName &#xA;&#x9;from tblTechSites ts&#xA;&#x9;join tblTechTransfers tr on ts.SiteUID = tr.SiteUID&#xA;&#x9;where tr.TransferUID = @TransferUID&#xA;&#xA;&#xA;&#xA;select @tagCount as cnt, @OrigSiteName as OrigSiteName&#xA;"
                  SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
                  <SQLTask:ResultBinding
                    SQLTask:ResultName="cnt"
                    SQLTask:DtsVariableName="User::TagCount" />
                  <SQLTask:ResultBinding
                    SQLTask:ResultName="OrigSiteName"
                    SQLTask:DtsVariableName="User::OrigSiteName" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@DestinationSite"
                    SQLTask:DtsVariableName="User::SiteUID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@TransferUID"
                    SQLTask:DtsVariableName="User::TransferUID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@TagTransferStatusUID"
                    SQLTask:DtsVariableName="User::TagStatusUID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@RoomUID"
                    SQLTask:DtsVariableName="User::ReceivingRoomUID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@ContainerUID"
                    SQLTask:DtsVariableName="User::ContainerUID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                </SQLTask:SqlTaskData>
              </DTS:ObjectData>
            </DTS:Executable>
            <DTS:Executable
              DTS:refId="Package\Site List\Process In Transit Transfers\Send Email Report"
              DTS:CreationName="Microsoft.ExecuteSQLTask"
              DTS:Description="Execute SQL Task"
              DTS:DTSID="{11367898-9428-4142-BF67-875F46119C37}"
              DTS:ExecutableType="Microsoft.ExecuteSQLTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="Send Email Report"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{27877574-49A2-4A75-A303-813C0CA3D5AB}"
                  SQLTask:SqlStatementSource="DECLARE @mailProfile NVARCHAR(MAX), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX), @rpt Nvarchar(max)&#xA;declare @SiteName varchar(50), @OrigSiteName varchar(50), @TransferType varchar(50), @numTags int, @untaggedcount int&#xA;&#xA;DECLARE @body nvarchar(max), @subject NVARCHAR(max), @sText nvarchar(max), @logdate NVARCHAR(50) &#xA;&#xA;&#xA;SET @mailProfile = ?&#xA;set @email = ?&#xA;set @ccemail = ?&#xA;set @rpt = ?&#xA;set @SiteName = ?&#xA;set @OrigSiteName  = ?&#xA;set @TransferType = ?&#xA;set @numTags = ?&#xA;set @untaggedcount = ?&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= @TransferType  +' Transfer Complete to ' + @SiteName + ' Site (' + @logdate + ')'&#xA;&#xA;&#xA;set @sText = ''&#xA;&#xA;&#xA;set @sText = @sText +'A transfer from ' + @OrigSiteName + ' to the ' + @SiteName + ' Site has been completed.' &#xA;&#xA;if @numTags &gt; 0&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;set @sText = @sText + '&lt;br/&gt;&lt;br/&gt;The list of tags on the transfer is attached.'&#xA;&#x9;&#x9;if @untaggedcount &gt; 0&#xA;&#x9;&#x9;&#x9;set @sText = @sText + '&lt;br/&gt;&lt;br/&gt;The transfer also includes untagged inventory.'&#xA;&#x9;END&#xA;ELSE&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;set @sText = @sText + '&lt;br/&gt;&lt;br/&gt;The transfer includes untagged inventory.'&#xA;&#xA;&#x9;END&#xA;&#x9; &#xA;&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;'&lt;p&gt;' + @sText + '&lt;/p&gt;&lt;br/&gt;' +&#xA;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9; '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;&#x9; '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;&#xA;if @numTags &gt; 0&#xA;&#xA;&#x9;--print @sText  + ' ' + 'attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject,&#xA;&#x9;&#x9;@file_attachments = @rpt;&#xA;&#xA;else&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject;&#xA;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@mailProfile"
                    SQLTask:DtsVariableName="User::EmailProfile"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="129"
                    SQLTask:ParameterSize="500" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@email"
                    SQLTask:DtsVariableName="User::EmailAddress"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="129"
                    SQLTask:ParameterSize="500" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@ccemail"
                    SQLTask:DtsVariableName="$Project::DonateEmailcc"
