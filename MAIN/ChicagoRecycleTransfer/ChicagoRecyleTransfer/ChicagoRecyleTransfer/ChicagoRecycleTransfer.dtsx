                  SQLTask:SqlStatementSource=" /*&#xA; SSIS takes the list of transfers and iterates through them...&#xA; Inserting into history and changing status&#xA; */&#xA;&#xA; declare @TransferUID int, @TransferTypeUID int, @DriverID int&#xA; &#xA; declare @StatusUID int&#xA;&#xA;set @TransferUID = ?&#xA;set @TransferTypeUID = ?&#xA;set @DriverID = ?&#xA;&#xA;&#xA;if @TransferTypeUID = 4&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;select @StatusUID = (select statusid from tblStatus where statustypeuid = 4 and statusdesc = 'Scheduled')&#xA;&#xA;&#x9;&#x9;INSERT INTO tblTechTransferHistory&#xA;&#x9;&#x9;(TransferUID, StatusUID, OriginStatusUID, &#xA;&#x9;&#x9;DriverID, DeliveryDate,&#xA;&#x9;&#x9;Denied,&#xA;&#x9;&#x9;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;&#x9;&#x9;SELECT T.TransferUID, @StatusUID, T.StatusUID,&#xA;&#x9;&#x9;@DriverID, convert(varchar(10), getdate(), 101),&#xA;&#x9;&#x9;0,&#xA;&#x9;&#x9;0, GETDATE(), 0, GETDATE()&#xA;&#x9;&#x9;FROM tblTechTransfers T&#xA;&#x9;&#x9;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#x9;&#x9;Update t &#xA;&#x9;&#x9;set StatusUID = @StatusUID, DriverID = @DriverID, DeliveryDate = convert(varchar(10), getdate(), 101),&#xA;&#x9;&#x9;LastModifiedByUserID = 0, LastModifiedDate = getdate()&#xA;&#x9;&#x9;--select *&#xA;&#x9;&#x9;from tblTechTransfers t&#xA;&#x9;&#x9;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#x9;END&#xA;&#xA;&#xA;select @StatusUID = (select statusid from tblStatus where statustypeuid = 4 and statusdesc = 'In Transit')&#xA;--print cast(@StatusUID as varchar(10))&#xA;&#xA;Update t &#xA;set StatusUID = @StatusUID, LastModifiedByUserID = 0, LastModifiedDate = getdate()&#xA;--select *&#xA;from tblTechTransfers t&#xA;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#xA;INSERT INTO tblTechTransferHistory&#xA;(TransferUID, StatusUID, OriginStatusUID, Denied,&#xA;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;SELECT T.TransferUID, @StatusUID, T.StatusUID, 0,&#xA;0, GETDATE(), 0, GETDATE()&#xA;FROM tblTechTransfers T&#xA;WHERE t.TransferUID = @TransferUID&#xA;&#xA;&#xA;&#xA;&#xA;/*&#xA;Note:  OriginStatusUID is 26 and not the inventory StatusUID (57 - Pending Transfer which is not recorded in the history table)&#xA;The application is putting in 26 for the OriginStatusUID , so this puts in 26 also&#xA;26 is the completed status for the transfer&#xA;*/&#xA;&#xA;INSERT INTO tblTechInventoryHistory&#xA;(InventoryUID, InventoryTypeUID, SiteUID, EntityUID, EntityTypeUID, &#xA;StatusUID,&#xA;OriginSiteUID, OriginStatusUID, OriginEntityUID, OriginEntityTypeUID, &#xA;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate, &#xA;ParentInventoryUID, OriginParentInventoryUID, BulkEditUID, InventorySourceUID, ContainerUID, OriginContainerUID)&#xA;SELECT tti.InventoryUID, tti.InventoryTypeUID, tti.SiteUID, tts.TransferUID, 6, &#xA;@StatusUID, &#xA;tti.SiteUID, 26, tti.EntityUID, tti.EntityTypeUID, &#xA;0, GETDATE(), 0, GETDATE(), &#xA;tti.ParentInventoryUID, tti.ParentInventoryUID, 0, tti.InventorySourceUID, tti.ContainerUID, tti.ContainerUID&#xA;FROM tblTechInventory tti&#xA;JOIN tblTechTransferInventory ttr ON tti.InventoryUID = ttr.InventoryUID&#xA;JOIN tblTechTransfers tts ON tts.TransferUID = ttr.TransferUID &#xA;where tts.transferUID = @TransferUID&#xA;and ttr.InventoryUID &lt;&gt; 0&#xA;&#xA;&#xA;&#xA;--select *&#xA;update tti set EntityUID = ttr.TransferUID, EntityTypeUID = 6, StatusUID = @StatusUID&#xA;from tblTechInventory tti&#xA;join tblTechTransferInventory ttr on tti.InventoryUID = ttr.InventoryUID&#xA;join tblTechTransfers tts on tts.TransferUID = ttr.TransferUID&#xA;where tts.transferUID = @TransferUID&#xA;and ttr.InventoryUID &lt;&gt; 0&#xA; &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
