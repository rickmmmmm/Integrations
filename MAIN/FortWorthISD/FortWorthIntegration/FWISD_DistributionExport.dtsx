          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{99615FE6-9ACE-4F0D-BE21-CD086645F00A}"
              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo._ETL_ExportDistributions', 'U') IS NOT NULL DROP TABLE dbo._ETL_ExportDistributions;&#xA;&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;&#xA;GO&#xA;&#xA;SET QUOTED_IDENTIFIER ON&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[_ETL_ExportDistributions](&#xA;&#x9;[ETLExportDistributionUID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[StudentID] [varchar](50) NOT NULL,&#xA;&#x9;[PaymentStatus] [varchar](50) NOT NULL,&#xA;&#x9;[Disposition] [varchar](1000) NOT NULL,&#xA;&#x9;[ChangeDate] [varchar](50) NOT NULL,&#xA;&#x9;[Description] [varchar](50) NOT NULL,&#xA;&#x9;[SerialNumber] [varchar](50) NOT NULL,&#xA;&#x9;[AssetTag] [varchar](50) NOT NULL,&#xA; CONSTRAINT [PK__ETL_ExportFines] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[ETLExportDistributionUID] ASC&#xA;)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Prepare for Export\INSERT Fines"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{92D6B565-7518-4ED6-836C-38EF186B64EC}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="INSERT Fines"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{99615FE6-9ACE-4F0D-BE21-CD086645F00A}"
              SQLTask:SqlStatementSource="INSERT INTO dbo._ETL_ExportDistributions&#xA;(StudentID, PaymentStatus, Disposition, ChangeDate,&#xA;[Description], SerialNumber, AssetTag)&#xA;SELECT S.StudentID, CASE WHEN TFFY.StudentID IS NULL AND TFS.StudentID IS NULL&#xA;THEN 'N' ELSE 'Y' END,&#xA;CASE WHEN I.StatusUID = 47 THEN 'Lost' ELSE&#xA;CASE WHEN I.StatusUID = 64 THEN 'Stolen' ELSE&#xA;CASE WHEN I.StatusUID = 70 THEN 'Repair' ELSE 'Checked-out' END END END,&#xA;CONVERT(VARCHAR(10), I.LastModifiedDate, 101),&#xA;M.ManufacturerName + ' ' + P.ModelNumber,&#xA;I.Serial, I.Tag&#xA;FROM tblTechInventory I&#xA;JOIN tblStudents S ON I.EntityUID = S.StudentsUID&#xA;AND I.EntityTypeUID = 4&#xA;AND I.StatusUID = 30--IN (30, 47, 64, 70)&#xA;JOIN tblTechItems P ON I.ItemUID = P.ItemUID&#xA;JOIN tblUnvManufacturers M ON P.ManufacturerUID = M.ManufacturerUID&#xA;LEFT JOIN (SELECT S.StudentID&#xA;FROM tblUnvCharges C&#xA;JOIN tblStudents S ON C.EntityUID = S.StudentsUID&#xA;AND C.EntityTypeUID = 4&#xA;JOIN tblUnvChargeTypes CT ON C.ChargeTypeUID = CT.ChargeTypeUID&#xA;WHERE DateSatisfied IS NOT NULL&#xA;AND CT.Name = 'TechFeeFullYear'&#xA;AND C.DateSatisfied &gt;= CASE WHEN MONTH(GETDATE()) &lt; 7&#xA;THEN CAST(CAST(YEAR(GETDATE())-1 AS VARCHAR) + '-7-1' AS DATETIME)&#xA;ELSE CAST(CAST(YEAR(GETDATE()) AS VARCHAR) + '-7-1' AS DATETIME) END&#xA;GROUP BY S.StudentID) AS TFFY ON S.StudentID = TFFY.StudentID&#xA;LEFT JOIN (SELECT S.StudentID&#xA;FROM tblUnvCharges C&#xA;JOIN tblStudents S ON C.EntityUID = S.StudentsUID&#xA;AND C.EntityTypeUID = 4&#xA;JOIN tblUnvChargeTypes CT ON C.ChargeTypeUID = CT.ChargeTypeUID&#xA;WHERE DateSatisfied IS NOT NULL&#xA;AND CT.Name = 'TechFeeSemester'&#xA;AND C.DateSatisfied &gt;= CASE WHEN MONTH(GETDATE()) &lt; 7&#xA;THEN CAST(CAST(YEAR(GETDATE()) AS VARCHAR) + '-1-1' AS DATETIME)&#xA;ELSE CAST(CAST(YEAR(GETDATE()) AS VARCHAR) + '-7-1' AS DATETIME) END&#xA;GROUP BY S.StudentID) AS TFS ON S.StudentID = TFS.StudentID&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
