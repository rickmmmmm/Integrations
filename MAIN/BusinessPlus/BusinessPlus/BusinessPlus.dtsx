S A&#xA;INNER JOIN  BusinessPlus.StagingExceptions AS B&#xA;ON ISNULL(A.PONumber,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='PONumber'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- PO Order QTY&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Order QTY is not Valid' ELSE ExceptionReason + ',Order QTY is not Valid' END &#xA;FROM BusinessPlus.Staging AS A&#xA;INNER JOIN  BusinessPlus.StagingExceptions AS B&#xA;ON ISNULL(A.POOrderQuantity,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='OrderQTY'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- SiteID does not exists&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Site ID does not exists in TIPWeb' ELSE ExceptionReason + ',Site ID does not exists in TIPWeb' END &#xA;FROM BusinessPlus.Staging AS A&#xA;INNER JOIN  BusinessPlus.StagingExceptions AS B&#xA;ON ISNULL(A.SiteID,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='SiteID'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Product Name is not Vaild&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Product Name is not Valid' ELSE ExceptionReason + ',Product Name is not Valid' END &#xA;FROM BusinessPlus.Staging AS A&#xA;INNER JOIN  BusinessPlus.StagingExceptions AS B&#xA;ON ISNULL(A.ProductName,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='ProductName'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Account Code is not Vaild&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Account Code is not Valid' ELSE ExceptionReason + ',Account Code is not Valid' END &#xA;FROM BusinessPlus.Staging AS A&#xA;INNER JOIN  BusinessPlus.StagingExceptions AS B&#xA;ON ISNULL(A.AccountCode,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='AccountCode'&#xA;AND A.CustomerCode=B.CustomerCode" xmlns:SQLTask="www.microsoft.com/s