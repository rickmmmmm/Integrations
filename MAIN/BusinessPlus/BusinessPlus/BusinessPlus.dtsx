CAST(DepartmentID AS varchar(255)) AS DepartmentID,
MAX(POReceiveQuantity) AS TotalQTYRec,
MAX(POOrderQuantity) AS TotalQTYOrderd,
CAST(POLineNumber AS varchar(50)) AS POLineNumber,
'Hayes Integration Visions - ' + CAST(CAST(GETDATE() AS DATE) AS VARCHAR(50)) AS Notes

FROM BusinessPlus.Staging
WHERE Exception = 0
AND CAST(ISNULL(PONumber,'') AS varchar(255)) != ''
GROUP BY CAST(PONumber AS varchar(255)),ProductName,CAST(ProductType AS varchar(255)),CAST(Model AS varchar(255)),
