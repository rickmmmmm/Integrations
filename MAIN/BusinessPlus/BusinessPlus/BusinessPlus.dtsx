MIN(CAST(PODate AS DATE)) AS PurchaseDate,
CAST(Manufacturer AS varchar(255)) AS Manufacturer,
CAST(ProductName AS varchar(100)) AS ProductName,
CAST(ProductType AS varchar(50)) AS ProductType,
