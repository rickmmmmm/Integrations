          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Create Export and Report\Export TIPWeb Asset Changes\Asset Changes"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Asset Changes"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT A.AssetID,C.SiteID,G.Site_ChangeDate,G.Site_ChangeUser,
B.statusDesc AS [Status],H.Status_ChangeDate,H.Status_ChangeUser,
CASE WHEN A.EntityTypeUID = 4 THEN 'Student' WHEN A.EntityTypeUID = 3 THEN 'Staff'  ELSE 'Room'  END AS Location_Type,
I.Location_ChangeDate,I.Location_ChangeUser,CASE WHEN A.EntityTypeUID = 2 THEN D.RoomNumber ELSE NULL END AS Location_RoomNumber,CASE WHEN A.EntityTypeUID = 2 THEN D.RoomDescription ELSE NULL END AS Location_RoomDesc,

CASE WHEN A.EntityTypeUID = 4 THEN E.StudentID ELSE NULL END AS Location_StudentID,CASE WHEN A.EntityTypeUID = 4 THEN E.FirstName ELSE NULL END AS Location_Student_First_Name,
CASE WHEN A.EntityTypeUID = 4 THEN E.MiddleName ELSE NULL END AS Location_Student_Middle_Name,CASE WHEN A.EntityTypeUID = 4 THEN E.LastName ELSE NULL END AS Location_Student_Last_Name,
CASE WHEN A.EntityTypeUID = 4 THEN J.Issue_Date ELSE NULL END AS Student_Issue_Date,CASE WHEN A.EntityTypeUID = 4 THEN J.Due_Date ELSE NULL END AS Student_Due_Date,
CASE WHEN A.EntityTypeUID = 4 THEN K.Accessories ELSE NULL END AS Student_Accessory_Issued,

CASE WHEN A.EntityTypeUID = 3 THEN F.TeacherID ELSE NULL END AS Location_StaffID,CASE WHEN A.EntityTypeUID = 3 THEN F.FirstName ELSE NULL END AS Location_Staff_First_Name,
CASE WHEN A.EntityTypeUID = 3 THEN F.MiddleName ELSE NULL END AS Location_Staff_Middle_Name,CASE WHEN A.EntityTypeUID = 3 THEN F.LastName ELSE NULL END AS Location_Staff_Last_Name,
CASE WHEN A.EntityTypeUID = 3 THEN J.Issue_Date ELSE NULL END AS Staff_Issue_Date,CASE WHEN A.EntityTypeUID = 3 THEN J.Due_Date ELSE NULL END AS Staff_Due_Date,
CASE WHEN A.EntityTypeUID = 3 THEN K.Accessories ELSE NULL END AS Staff_Accessory_Issued,
'' AS StaffType


FROM tblTechInventory AS A WITH (NOLOCK)

INNER JOIN tblStatus AS B WITH (NOLOCK)
ON A.StatusUID = B.statusID

INNER JOIN tblTechSites AS C WITH (NOLOCK)
ON A.SiteUID = C.SiteUID

LEFT JOIN tblUnvRooms AS D WITH (NOLOCK)
ON A.EntityTypeUID = 2 AND A.EntityUID = D.RoomUID

LEFT JOIN tblStudents AS E WITH (NOLOCK)
ON A.EntityTypeUID = 4 AND A.EntityUID = E.StudentsUID

LEFT JOIN tblTeachers AS F WITH (NOLOCK)
ON A.EntityTypeUID = 3 AND A.EntityUID = F.TeachersUID

LEFT JOIN TmpSiteChanges AS G WITH (NOLOCK)
ON A.InventoryUID = G.InventoryUID

LEFT JOIN TmpStatusChanges AS H WITH (NOLOCK)
ON A.InventoryUID = H.InventoryUID

LEFT JOIN TmpEntityChanges AS I WITH (NOLOCK)
ON A.InventoryUID = H.InventoryUID

LEFT JOIN TmpEntityDueDates AS J WITH (NOLOCK)
ON A.InventoryUID = J.EntityUID AND A.EntityTypeUID = J.EntityUID AND A.EntityUID = J.EntityUID

LEFT JOIN TmpEntityAccessories AS K WITH (NOLOCK)
ON A.InventoryUID = J.InventoryUID AND A.EntityUID = K.EntityUID AND A.EntityTypeUID = K.EntityTypeUID

WHERE CAST(A.LastModifiedDate AS DATE) = CAST(GETDATE() AS DATE)</property>
