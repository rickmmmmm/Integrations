                Name="ScriptMain.cs"
                Encoding="UTF8"><![CDATA[#region Help:  Introduction to the script task
/* The Script Task allows you to perform virtually any operation that can be accomplished in
 * a .Net application within the context of an Integration Services control flow. 
 * 
 * Expand the other regions which have "Help" prefixes for examples of specific ways to use
 * Integration Services features within this script task. */
#endregion


#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.IO;
#endregion

namespace ST_7094ebecde224baea82090821d41e0d9
{

	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{

		public void Main()
		{
            string NewFileName = Dts.Variables["User::ExportFileName"].Value.ToString();
            string ExportPath = Dts.Variables["User::ExportFilePath"].Value.ToString();
            string RootPath = Dts.Variables["User::RootPath"].Value.ToString();
            string CustomerCode = Dts.Variables["User::CustomerCode"].Value.ToString();
            string JobName = Dts.Variables["User::JobName"].Value.ToString();
            string Integration = Dts.Variables["User::Integration"].Value.ToString();
            string IntegerationType = Dts.Variables["User::IntegrationType"].Value.ToString();


            // Setting Varables
            JobName = JobName.Replace("CustomerCode", CustomerCode).Replace("Integration", Integration).Replace("IntegerationType", IntegerationType);
            Dts.Variables["User::JobName"].Value = JobName;

            NewFileName = NewFileName.Replace("MM", DateTime.Now.ToString("MM")).Replace("DD", DateTime.Now.ToString("dd")).Replace("YYYY", DateTime.Now.ToString("yyyy"));
            NewFileName = NewFileName.Replace("HH", DateTime.Now.ToString("HH")).Replace("MI", DateTime.Now.ToString("mm"));

            Dts.Variables["User::ExportFileName"].Value = NewFileName.Replace("CustomerCode", CustomerCode);

            // Check to make sure "\" is at the end of Root path
            if (RootPath.Substring(RootPath.Length - 1, 1) != "\\")
            {
                RootPath = RootPath + "\\";
            }

            Dts.Variables["User::RootPath"].Value = RootPath;


            // Check to make sure "\" is at the end of Export path
            if (ExportPath.Substring(ExportPath.Length-1,1) != "\\")
            {
                ExportPath = ExportPath + "\\";
                Dts.Variables["User::ExportFilePath"].Value = ExportPath;
            }

            // Does Export path exists?
            bool DirectoryExists = Directory.Exists(RootPath + ExportPath);

            // Create Directory if it does not exists
            if (!DirectoryExists)
            {
                Directory.CreateDirectory(RootPath + ExportPath);
            }


            if ( Convert.ToBoolean(Dts.Variables["User::Debug"].Value) == true )
            {
                MessageBox.Show("\r\n" +
                    "   Customer Code: (" + CustomerCode + ")\r\n" +
                    "        Job Name: (" + JobName + ")\r\n" +
                    "Export File Name: (" + Dts.Variables["User::ExportFileName"].Value.ToString() + ")\r\n" +
                    "       Root Path: (" + RootPath + ")\r\n" +
                    "     Export Path: (" + ExportPath + ")\r\n" +
                    "Full Export Path: (" + RootPath + ExportPath + ")\r\n");
            }

            Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
