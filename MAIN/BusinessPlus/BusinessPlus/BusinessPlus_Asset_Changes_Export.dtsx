              SQLTask:SqlStatementSource="IF EXISTS (SELECT Name FROM SYS.OBJECTS WHERE Name = 'TmpSiteChanges')&#xA;BEGIN &#xA;&#x9;DROP TABLE TmpSiteChanges&#xA;END&#xA;&#xA;SELECT InventoryUID,Site_ChangeDate,Site_ChangeUserID,Site_ChangeUser&#xA;INTO TmpSiteChanges&#xA;FROM&#xA;&#x9;(&#xA;&#x9;SELECT InventoryUID,A.CreatedByUserID AS Site_ChangeUserID,B.RealName AS Site_ChangeUser,A.CreatedDate AS Site_ChangeDate,ROW_NUMBER() OVER(PARTITION BY InventoryUID ORDER BY InventoryHistoryUID DESC) AS RowID&#xA;&#xA;&#x9;FROM tblTechInventoryHistory AS A WITH (NOLOCK)&#xA;&#xA;&#x9;INNER JOIN tblUser AS B WITH (NOLOCK)&#xA;&#x9;ON A.CreatedByUserID = B.UserID&#xA;&#xA;&#x9;WHERE A.SiteUID != OriginSiteUID&#xA;&#x9;) AS TB&#xA;&#xA;WHERE RowID = 1&#xA;&#xA;&#xA;IF EXISTS (SELECT Name FROM SYS.OBJECTS WHERE Name = 'TmpStatusChanges')&#xA;BEGIN &#xA;&#x9;DROP TABLE TmpStatusChanges&#xA;END&#xA;&#xA;SELECT InventoryUID,Status_ChangeDate,Status_ChangeUserID,Status_ChangeUser&#xA;INTO TmpStatusChanges&#xA;FROM&#xA;&#x9;(&#xA;&#x9;SELECT InventoryUID,A.CreatedByUserID AS Status_ChangeUserID,B.RealName AS Status_ChangeUser,A.CreatedDate AS Status_ChangeDate,ROW_NUMBER() OVER(PARTITION BY InventoryUID ORDER BY InventoryHistoryUID DESC) AS RowID&#xA;&#xA;&#x9;FROM tblTechInventoryHistory AS A WITH (NOLOCK)&#xA;&#xA;&#x9;INNER JOIN tblUser AS B WITH (NOLOCK)&#xA;&#x9;ON A.CreatedByUserID = B.UserID&#xA;&#xA;&#x9;WHERE A.StatusUID != A.OriginStatusUID&#xA;&#x9;) AS TB&#xA;&#xA;WHERE RowID = 1&#xA;&#xA;IF EXISTS (SELECT Name FROM SYS.OBJECTS WHERE Name = 'TmpEntityChanges')&#xA;BEGIN &#xA;&#x9;DROP TABLE TmpEntityChanges&#xA;END&#xA;&#xA;SELECT InventoryUID,Location_ChangeDate,Location_ChangeUserID,Location_ChangeUser&#xA;INTO TmpEntityChanges&#xA;FROM&#xA;&#x9;(&#xA;&#x9;SELECT InventoryUID,A.CreatedByUserID AS Location_ChangeUserID,B.RealName AS Location_ChangeUser,A.CreatedDate AS Location_ChangeDate,ROW_NUMBER() OVER(PARTITION BY InventoryUID ORDER BY InventoryHistoryUID DESC) AS RowID&#xA;&#xA;&#x9;FROM tblTechInventoryHistory AS A WITH (NOLOCK)&#xA;&#xA;&#x9;INNER JOIN tblUser AS B WITH (NOLOCK)&#xA;&#x9;ON A.CreatedByUserID = B.UserID&#xA;&#xA;&#x9;WHERE A.EntityUID != OriginEntityUID&#xA;&#x9;) AS TB&#xA;&#xA;WHERE RowID = 1&#xA;&#xA;IF EXISTS (SELECT Name FROM SYS.OBJECTS WHERE Name = 'TmpEntityDueDates')&#xA;BEGIN &#xA;&#x9;DROP TABLE TmpEntityDueDates&#xA;END&#xA;&#xA;SELECT InventoryUID,EntityUID,EntityTypeUID,Due_Date,Issue_Date&#xA;INTO TmpEntityDueDates&#xA;FROM&#xA;&#x9;(&#xA;&#x9;&#x9;SELECT A.InventoryUID,A.EntityUID,A.EntityTypeUID,B.DueDate AS Due_Date,B.CreatedDate AS Issue_Date,ROW_NUMBER() OVER(PARTITION BY A.InventoryUID ORDER BY B.InventoryHistoryUID DESC) AS RowID&#xA;&#xA;&#x9;&#x9;FROM tblTechInventory AS A&#xA;&#xA;&#x9;&#x9;INNER JOIN tblTechInventoryDueDates AS B&#xA;&#x9;&#x9;ON A.InventoryUID = B.InventoryUID &#xA;&#xA;&#x9;&#x9;WHERE A.EntityTypeUID = 4 OR A.EntityTypeUID = 3&#xA;&#xA;&#x9;) AS TB&#xA;&#xA;WHERE RowID = 1&#xA;&#xA;&#xA;IF EXISTS (SELECT Name FROM SYS.OBJECTS WHERE Name = 'TmpAccessories')&#xA;BEGIN &#xA;&#x9;DROP TABLE TmpAccessories&#xA;END&#xA;&#xA;IF EXISTS (SELECT Name FROM SYS.OBJECTS WHERE Name = 'TmpEntityAccessories')&#xA;BEGIN &#xA;&#x9;DROP TABLE TmpEntityAccessories&#xA;END&#xA;&#xA;&#xA;SELECT A.InventoryUID,A.EntityUID,A.EntityTypeUID,D.AccessoryName,ROW_NUMBER() OVER(PARTITION BY A.InventoryUID,A.EntityUID ORDER BY B.InventoryHistoryUID DESC) AS EnityRowID&#xA;&#xA;INTO TmpAccessories&#xA;&#xA;FROM tblTechInventory AS A&#xA;&#xA;INNER JOIN tblTechInventoryHistory AS B&#xA;ON A.InventoryUID = B.InventoryUID &#xA;&#xA;INNER JOIN tblTechInventoryAccessories AS C&#xA;ON B.InventoryHistoryUID = C.InventoryHistoryUID AND C.Returned = 0&#xA;&#xA;INNER JOIN tblTechAccessories AS D&#xA;ON C.AccessoryUID = D.AccessoryUID&#xA;&#xA;WHERE A.EntityTypeUID = 4 OR A.EntityTypeUID = 3&#xA;&#xA;ALTER TABLE TmpAccessories&#xA;&#x9;ADD RowID INT NOT NULL IDENTITY(1,1)&#xA;&#xA;&#xA;CREATE TABLE TmpEntityAccessories&#xA;(InventoryUID INT NOT NULL,EntityUID INT NOT NULL,EntityTypeUID INT NOT NULL,Accessories VARCHAR(MAX))&#xA;&#xA;DECLARE @Accessories&#x9;VARCHAR(MAX)&#xA;DECLARE @Counter&#x9;&#x9;INT = 1&#xA;DECLARE @CounterMax&#x9;&#x9;INT = 0&#xA;DECLARE @InventoryUID&#x9;INT&#xA;DECLARE @EntityUID&#x9;&#x9;INT&#xA;DECLARE @EntityTypeUID&#x9;INT&#xA;DECLARE @RowID&#x9;&#x9;&#x9;INT&#xA;&#xA;WHILE (SELECT TOP 1 InventoryUID FROM TmpAccessories) IS NOT NULL&#xA;BEGIN&#xA;&#x9;SELECT TOP 1 @InventoryUID=InventoryUID,@EntityUID=EntityUID,@EntityTypeUID=EntityTypeUID,@Counter=1,@Accessories='' FROM TmpAccessories&#xA;&#x9;SELECT @CounterMax=MAX(EnityRowID) FROM TmpAccessories WHERE InventoryUID = @InventoryUID AND @EntityUID = @EntityUID AND EntityTypeUID = @EntityTypeUID&#xA;&#xA;&#x9;WHILE @Counter &lt;= @CounterMax&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;IF @CounterMax &gt; 1 AND @Counter != @CounterMax&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;SELECT TOP 1 @Accessories= @Accessories + AccessoryName + ',',@RowID=RowID&#xA;&#x9;&#x9;&#x9;FROM TmpAccessories WHERE InventoryUID = @InventoryUID AND @EntityUID = @EntityUID AND EntityTypeUID = @EntityTypeUID&#xA;&#xA;&#x9;&#x9;&#x9;DELETE FROM TmpAccessories WHERE RowID=@RowID&#xA;&#x9;&#x9;END&#xA;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;SELECT TOP 1 @Accessories= @Accessories + AccessoryName,@RowID=RowID&#xA;&#x9;&#x9;&#x9;FROM TmpAccessories WHERE InventoryUID = @InventoryUID AND @EntityUID = @EntityUID AND EntityTypeUID = @EntityTypeUID&#xA;&#xA;&#x9;&#x9;&#x9;DELETE FROM TmpAccessories WHERE RowID=@RowID&#xA;&#x9;&#x9;END&#xA;&#xA;&#x9;&#x9;SET @Counter = @Counter + 1&#xA;&#x9;END&#xA;&#xA;&#x9;INSERT INTO TmpEntityAccessories&#xA;&#x9;(InventoryUID,EntityUID,EntityTypeUID,Accessories)&#xA;&#x9;VALUES&#xA;&#x9;(@InventoryUID,@EntityUID,@EntityTypeUID,@Accessories)&#xA;END&#xA;&#xA;DROP TABLE TmpAccessories&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
