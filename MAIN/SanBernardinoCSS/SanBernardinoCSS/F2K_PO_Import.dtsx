          DTS:DTSID="{FCCFB290-3D76-4D70-8786-897C5D13972F}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Mark Exceptions"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 RC1;  2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="1">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{D4BB30C4-CDBF-4E49-B22C-27545FCD8232}"
              SQLTask:SqlStatementSource="SET NOCOUNT ON&#xA;&#xA;-- Purchase Status&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Status is invalid' ELSE ExceptionReason + ',PO Status is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.POStatus,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='POStatus'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Purchase Line Number&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Line Number is invalid' ELSE ExceptionReason + ',PO Number is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.POLineNumber,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='POLineNumber'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;&#xA;-- Purchase Amount&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Line Amount is invalid' ELSE ExceptionReason + ',PO Amount is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.POAmount,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='POAmount'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Purchase Date&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Date is invalid' ELSE ExceptionReason + ',PO Date is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.PODate,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='PODate'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- PO Number&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Number is invalid' ELSE ExceptionReason + ',PO Number is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.PONumber,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='PONumber'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- PO Order QTY&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Order QTY is invalid' ELSE ExceptionReason + ',Order QTY is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.POOrderQuantity,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='OrderQTY'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- SiteID does not exists&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Site ID does not exists in TIPWeb' ELSE ExceptionReason + ',Site ID does not exists in TIPWeb' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.SiteID,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='SiteID'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Product Name is not Vaild&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Product Name is invalid' ELSE ExceptionReason + ',Product Name is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.ProductName,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='ProductName'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Account Code is not Vaild&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Account Code is invalid' ELSE ExceptionReason + ',Account Code is invalid' END &#xA;FROM F2K.StagingPurchaseOrders AS A&#xA;INNER JOIN  F2K.StagingPurchaseOrdersExceptions AS B&#xA;ON ISNULL(A.AccountCode,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='AccountCode'&#xA;AND A.CustomerCode=B.CustomerCode" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
