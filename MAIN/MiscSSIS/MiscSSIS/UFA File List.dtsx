from (
	select clientName, max(filedate) as LatestDate, 'File Date' as Reference
	from FileListReport
	group by clientName having max(fileDate) &lt; dateadd(d, -14, getdate())

	union
	select customerName, lastrundate, 'Run Date'
	from customerlist
	where active = 1
	and customername not in (select clientName from fileListReport)
	
	) a
join CustomerList l on a.ClientName = l.customername
left outer join CustomerNotes n on l.CustomersUID = n.CustomersUID
where SourceOrigin not in ('PSPLUGIN', 'Aeries')
and LatestDate &lt; dateadd(d, -14, getdate())
order by ClientName -- desc</property>
