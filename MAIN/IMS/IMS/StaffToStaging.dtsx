      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2E479B4D-C8AD-4AF0-BA68-1DBE4C4A32F8}"
          SQLTask:SqlStatementSource="&#xA;&#xA;declare @CustomerID int, @SourceOrigin varchar(50), @cnt int&#xA;&#xA;set @CustomerID = ?&#xA;set @SourceOrigin = ?&#xA;&#xA;if @SourceOrigin = 'AERIES'&#xA;&#x9;select @cnt = count(*)&#xA;&#x9;--SELECT SchoolCode, StaffID1, FirstName, LastName&#xA;&#x9;FROM AeriesSSISImport.dbo.AeriesStaff &#xA;&#x9;WHERE CustomerID =@CustomerID&#xA;&#x9;AND ID NOT IN ('0','')  &#xA;&#x9;and ltrim(rtrim(InactiveStatusCode)) = '' &#xA;&#xA;if @SourceOrigin = 'ONEROSTER' and @CustomerID =1&#xA;&#x9;select @cnt = count(*)&#xA;&#x9;from ImportManagementSystem.OneRoster.Staff st&#xA;&#x9;left outer join ImportManagementSystem.OneRoster.StaffJob sj on st.STaffID = sj.StaffID&#xA;&#x9;left outer join ImportManagementSystem.OneRoster.StaffDepartment sd on st.StaffID = sd.StaffID&#xA;&#x9;left outer join ImportManagementSystem.OneRoster.Department d on sd.DepartmentID  = d.DepartmentID&#xA;&#xA;if @SourceOrigin = 'ONEROSTER' and @CustomerID &gt; 1&#xA;&#x9;select @cnt = count(*)&#xA;&#x9; from ImportManagementSystem.OneRoster.Users u&#xA;  &#xA;&#x9; join [ImportManagementSystem].[OneRoster].[Orgs] o on o.sourcedId =u.orgSourcedIds&#xA;    &#xA;&#x9;where u.ClientUID = @CustomerID&#x9;&#xA;&#x9;and u.[role] not in ('', 'student', 'teacher')&#xA;&#xA;&#xA;select @cnt as cnt&#xA;&#xA;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="cnt"
            SQLTask:DtsVariableName="User::StaffFileExists" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@CustomerID"
            SQLTask:DtsVariableName="User::ExternalKey"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="3"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="@SourceOrigin"
            SQLTask:DtsVariableName="User::SourceOrigin"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="129"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
