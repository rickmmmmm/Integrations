              SQLTask:SqlStatementSource="DECLARE @ClientUID int, @rpt Nvarchar(max), @mailProfile NVARCHAR(MAX), @ZipExists int, @ZipFileSize NVARCHAR(MAX), @ZipFileSizeOver int &#xA;&#xA;DECLARE @body nvarchar(max), @subject NVARCHAR(max), @sText nvarchar(max), @logdate NVARCHAR(50) &#xA;DECLARE @ClientCatalog NVARCHAR(max), @CustomerName Nvarchar(max), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX)&#xA;DECLARE @StuDemog int, @TeachDemog int, @Staff int, @MasterCourse int, @CampusCourse int, @ClassList int, @StuSched int, @TeachSched int&#xA;&#xA;&#xA;DECLARE @sSqlBase Nvarchar(max), @sSql Nvarchar(max), @ParamDef NVARCHAR(max)&#xA;DECLARE @total int, @Filetotal INT, @Update INT, @New int, @reject INT &#xA;&#xA;set @ClientUID  = ?&#xA;SET @mailProfile = ?&#xA;set @rpt = ?&#xA;set @ZipExists = ?&#xA;set @ZipFileSize = ?&#xA;&#xA;set @ZipFileSizeOver = 0&#xA;if cast(@ZipFileSize as int) &gt; 1000000 &#xA;&#x9;set @ZipFileSizeOver = 1&#x9;&#xA;&#xA;&#xA;&#xA;&#xA;SELECT @CustomerName = CustomerName, &#xA;@ClientCatalog = InitialCatalog, &#xA;@email  = RejectEmail, &#xA;@ccemail = RejectEmailCC, &#xA;@StuDemog = isNull(StudentDemog, 0),&#xA;@TeachDemog = isNull(TeacherDemog, 0), &#xA;@Staff = isNull(Staff, 0), &#xA;@MasterCourse = isNull(MasterCourse, 0),&#xA;@CampusCourse = isNull(CampusCourse, 0),&#xA;@ClassList = isNull(ClassList, 0),&#xA;@StuSched = isNull(StudentSchedule, 0),&#xA;@TeachSched = isNull(TeacherSchedule, 0)&#xA;FROM ImportManagementSystem.dbo.CustomerList &#xA;where CustomersUID = @ClientUID &#xA;and Active = 1&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'SIS Import Processing Log for ' + @CustomerName + ' (' + @logdate + ')'&#xA;&#xA;set @sSqlBase = N'SELECT @total = COUNT(*) FROM ' + @ClientCatalog + N'.dbo.'&#xA;set @ParamDef = N'@total int OUTPUT'&#xA;&#xA;&#xA;/*&#xA;Run Total, Accept, Reject for each module as it is developed&#xA;select @total = count(*) from _IMS_Students&#xA;select @total = count(*) from _IMS_Students where import = 0&#xA;select @total = count(*) from _IMS_Students where import = 1 and New = 1&#xA;select @total = count(*) from _IMS_Students where import = 1 and New = 0&#xA;&#xA;&#xA;*/&#xA;set @sText = ''&#xA;----------------------STUDENT DEMOGRAPHIC---------------------------&#xA;if @StuDemog &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students where import = 1 and New = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Student Demographic: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;----------------------TEACHER DEMOGRAPHIC---------------------------&#xA;if @TeachDemog &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers where import 