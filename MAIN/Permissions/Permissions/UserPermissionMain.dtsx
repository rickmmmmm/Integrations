              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{4E4701C5-C7B0-4ED2-996E-6F89E5CDF0EE}"
                  SQLTask:SqlStatementSource="&#xA;declare @ClientUID int&#xA;set @ClientUID = ?&#xA;&#xA;&#xA;--select *&#xA;update p set TechDepartmentUID = d.TechDepartmentUID&#xA;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;join tblTechDepartments d on p.Department = d.DepartmentName&#xA;where p.ClientUID = @ClientUID&#xA;and p.Import = 1&#xA;and p.Department not in ('All')&#xA;&#xA;---------------- insert specified departments ----------------------------&#xA;&#xA;&#xA;INSERT INTO [dbo].[tblTechUserDepartments]&#xA;           ([UserID],[TechDepartmentUID],[ViewActive])&#xA;&#xA;select distinct p.userid, p.TechDepartmentUID, 1&#xA;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;left outer join tblTechUserDepartments ud on p.UserID = ud.UserID and p.techdepartmentuid = ud.TechDepartmentUID&#xA;where p.ClientUID = @ClientUID&#xA;and p.Import = 1&#xA;and p.Department not in ('All')&#xA;and ud.UserDepartmentUID is null&#xA;&#xA;&#xA;&#xA;&#xA;---------------- all departments ----------------------------&#xA;&#xA;&#xA;INSERT INTO [dbo].[tblTechUserDepartments]&#xA;           ([UserID],[TechDepartmentUID],[ViewActive])&#xA;&#xA;select distinct p.userid, d.TechDepartmentUID, 1&#xA;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;cross join tblTechDepartments d&#xA;left outer join tblTechUserDepartments ud on p.UserID = ud.UserID and d.techdepartmentuid = ud.TechDepartmentUID &#xA;where p.Import = 1&#xA;and p.ClientUID = @ClientUID&#xA;and p.Department in ('All')&#xA;and d.TechDepartmentUID not in (0)&#xA;and ud.TechDepartmentUID is null&#xA;order by userid&#xA;&#xA;&#xA;------------ Remove if not in table -----------------------&#xA;&#xA;--select *&#xA;delete d&#xA;from tblTechUserDepartments d&#xA;where userid in (select userID from _LDAPFileProcessing.dbo.UserPermissionList where Import = 1 and Department &lt;&gt; 'All' and ClientUID = @ClientUID)&#xA;and userid not in (select userID from _LDAPFileProcessing.dbo.UserPermissionList where Import = 1 and Department &lt;&gt; 'All' and TechDepartmentUID = d.TechDepartmentUID and ClientUID = @ClientUID)&#xA;&#xA;------------ Check for Duplication -----------------------&#xA;&#xA;--select userID, TechDepartmentUID&#xA;--from tblTechUserDepartments&#xA;--group by userID, TechDepartmentUID having count(*) &gt; 1&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="@ClientUID"
