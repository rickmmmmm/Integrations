                  SQLTask:SqlStatementSource="&#xA;declare @ClientUID int&#xA;&#xA;set @ClientUID = 1&#xA;&#xA;&#xA;&#xA;---------------- Parse Depts --------------------------------------&#xA;&#xA;&#xA;&#x9;update p set Department = replace(Department, ';', '.')&#xA;&#x9;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;&#x9;where p.ClientUID = @ClientUID&#xA;&#x9;and p.Department like '%;%'&#xA;&#xA;&#xA;&#x9;insert _LDAPFileProcessing.dbo.UserPermissionList&#xA;&#x9;&#x9;( ClientUID, [Login], Fullname, UserView, UserRole, &#xA;&#x9;&#x9;SiteID, Email, UserType, Department, PermissionTemplate, DateReceived, &#xA;&#x9;&#x9;SiteUID, UserRoleUID, UserID, UserTypeID, PermissionTemplateUID, OriginalSiteID,&#xA;&#x9;&#x9;OriginalDepartment, OriginalPermissionListUID&#xA;&#x9;&#x9;)&#xA;&#xA;&#x9;select ClientUID, [Login], Fullname, UserView, UserRole, &#xA;&#x9;&#x9; SiteID, Email, UserType, Dept, PermissionTemplate, DateReceived, &#xA;&#x9;&#x9;SiteUID, UserRoleUID, UserID, UserTypeID, PermissionTemplateUID, OriginalSiteID,&#x9;&#x9; &#xA;&#x9;&#x9; Department, PermissionsListUID&#xA;&#x9;from (&#xA;&#x9;&#x9;select PermissionsListUID, ClientUID, [Login], SiteID,&#xA;&#x9;&#x9;Fullname, UserView, UserRole, Email, UserType, PermissionTemplate, DateReceived,&#xA;&#x9;&#x9;SiteUID, UserRoleUID, UserID, UserTypeID, PermissionTemplateUID, OriginalSiteID,&#x9;&#xA;&#x9;&#x9;Department, &#xA;&#x9;&#x9;parsename(Department, 1) as dept1,&#xA;&#x9;&#x9;parsename(Department, 2) as dept2,&#xA;&#x9;&#x9;parsename(Department, 3) as dept3,&#xA;&#x9;&#x9;parsename(Department, 4) as dept4,&#xA;&#x9;&#x9;parsename(Department, 5) as dept5&#xA;&#x9;&#x9;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;&#x9;&#x9;where p.ClientUID = @ClientUID&#xA;&#x9;&#x9;and import = 1&#xA;&#x9;&#x9;and p.Department like '%.%'&#xA;&#x9;&#x9;) x&#xA;&#x9;unpivot&#xA;&#x9;(&#xA;&#x9;&#x9;Dept for Precedence in (dept1, dept2, dept3, dept4, dept5)&#xA;&#x9;) y&#xA;&#xA;&#x9;--Do not import the original department record&#xA;&#xA;&#x9;--select * &#xA;&#x9;Update p set import = 0,  RejectMessage = &#xA;&#x9;case when RejectMessage is null then 'Parsed Department' else RejectMessage + '; Parsed Department'  end &#xA;&#x9;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;&#x9;where p.ClientUID = @ClientUID&#xA;&#x9;and import = 1&#xA;&#x9;and Department like '%.%'&#xA;&#x9;and PermissionsListUID   in (select OriginalPermissionListUID from _LDAPFileProcessing.dbo.UserPermissionList where clientuID = p.ClientUID)  &#xA;&#xA;--------------------------- Invalid Dept in Parse --------------------------------------------------&#xA;&#xA;&#x9;--select *&#xA;&#x9;Update p set import = 0,  RejectMessage = &#xA;&#x9;case when RejectMessage is null then 'Invalid Department' else RejectMessage + '; Invalid Department'  end &#xA;&#x9;from _LDAPFileProcessing.dbo.UserPermissionList p&#xA;&#x9;where p.ClientUID = @ClientUID&#xA;&#x9;and import = 1&#xA;&#x9;and OriginalDepartment is not null&#xA;&#x9;and ltrim(rtrim(p.Department)) not in (select DepartmentName from tblTechDepartments)&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
