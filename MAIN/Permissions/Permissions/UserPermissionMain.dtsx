                  SQLTask:Connection="{2752A339-7A74-455F-A87D-27F407914C76}"
                  SQLTask:SqlStatementSource="DECLARE @body nvarchar(max), @subject NVARCHAR(max), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX), @mailProfile NVARCHAR(MAX)&#xA;DECLARE @rpt Nvarchar(max), @CustomerName Nvarchar(max),  @sLabel nvarchar(max), @logdate NVARCHAR(50) &#xA;DECLARE @PermissionsDB NVARCHAR(max), @sSql Nvarchar(max), @total INT, @processed INT, @reject INT &#xA;DECLARE @ParamDef NVARCHAR(max), @ClientUID int&#xA;&#xA;SET @PermissionsDB = ?&#xA;SET @CustomerName = ?&#xA;set @ClientUID = ?&#xA;SET @email = ?&#xA;SET @ccemail = ? &#xA;SET @rpt = ?&#xA;SET @mailProfile = ?&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'Permission List Processing Log for ' + @CustomerName + ' (' + @logdate + ')'&#xA;&#xA;&#xA;/*&#xA;Run Total, Accept, Reject for each module as it is developed&#xA;*/&#xA;---------------------- Stats --------------------------&#xA;--Total&#xA;set @sSql = N'SELECT @total = COUNT(*) FROM ' + @PermissionsDB + '.dbo.UserPermissionList where ClientUID = @ClientUID'&#xA;set @ParamDef = N'@ClientUID int, @total int OUTPUT'&#xA;exec sp_executesql @sSql,  @ParamDef, @ClientUID=@ClientUID, @total=@total OUTPUT &#xA;--Processed &#xA;set @sSql = N'SELECT @processed= COUNT(*) FROM ' + @PermissionsDB + '.dbo.UserPermissionList  where ClientUID = @ClientUID and import = 1'&#xA;set @ParamDef = N'@ClientUID int, @processed int OUTPUT'&#xA;exec sp_executesql @sSql,  @ParamDef, @ClientUID=@ClientUID, @processed=@processed OUTPUT &#xA;--import &#xA;set @sSql = N'SELECT @reject = COUNT(*) FROM ' + @PermissionsDB + '.dbo.UserPermissionList  where ClientUID = @ClientUID and import = 0 and rejectMessage not like ''%parse%'' '&#xA;set @ParamDef = N'@ClientUID int, @reject int OUTPUT'&#xA;exec sp_executesql @sSql,  @ParamDef, @ClientUID=@ClientUID, @reject=@reject OUTPUT &#xA;&#xA;set @sLabel = '&lt;b&gt;&lt;i&gt;Permission List: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; ' Total Records Received: ' + CONVERT(VARCHAR(50), @total) + '&lt;br/&gt;' +&#xA; 'Total Records Processed: ' + CONVERT(VARCHAR(50), @processed) + '&lt;br/&gt;' +&#xA; ' Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;&lt;br/&gt;' +&#xA; ' The list of reject records is attached. &lt;br/&gt;&lt;br/&gt;'&#xA;&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA; '&lt;p&gt;' + @sLabel + '&lt;/p&gt;&lt;br/&gt;' +&#xA;  'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;  '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;  '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;&#xA;print @body&#xA;&#xA;EXEC msdb.dbo.sp_send_dbmail&#xA;    @profile_name = @mailProfile,&#xA;    @recipients =@email,&#xA;    @copy_recipients = @ccemail,&#xA;    @body_format = 'HTML',&#xA;    @body = @body, &#xA;    @subject =@subject, &#xA;    @file_attachments = @rpt ;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
