                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT       tc.ChargeUID, csite.SiteID + ' - ' + csite.chargeSite AS Charge_Site, 
ts.SiteID + ' - ' + ts.SiteName AS Payment_Site, s.CampusID AS Student_Campus_ID, s.StudentID AS Student_ID, s.LastName, 
s.FirstName, s.MiddleName, s.Grade, ch.Name AS Charge_Type, ti.ItemName AS Product, tc.UniversalID, 
tc.ChargeAmount AS Charge_Amount, cp.ChargeAmount AS Amount_Paid, CONVERT(VARCHAR(4), YEAR(cp.CreatedDate)) 
+ '-' + RIGHT('0' + CONVERT(VARCHAR(2), MONTH(cp.CreatedDate)), 2) + '-' + RIGHT('0' + CONVERT(VARCHAR(2), 
DAY(cp.CreatedDate)), 2) + ' ' + CONVERT(varchar(12), CAST(cp.CreatedDate AS time(3))) AS Date_Paid, CONVERT(VARCHAR(4), 
YEAR(tc.DateSatisfied)) + '-' + RIGHT('0' + CONVERT(VARCHAR(2), MONTH(tc.DateSatisfied)), 2) 
+ '-' + RIGHT('0' + CONVERT(VARCHAR(2), DAY(tc.DateSatisfied)), 2) + ' ' + CONVERT(varchar(12), CAST(tc.DateSatisfied AS time(3))) 
AS Satisfied_Date, CONVERT(VARCHAR(4), YEAR(tc.CreatedDate)) + '-' + RIGHT('0' + CONVERT(VARCHAR(2), MONTH(tc.CreatedDate)), 
2) + '-' + RIGHT('0' + CONVERT(VARCHAR(2), DAY(tc.CreatedDate)), 2) + ' ' + CONVERT(varchar(12), CAST(tc.CreatedDate AS time(3))) 
AS Issue_Date, CASE WHEN cp.Void = 1 THEN 'TRUE' WHEN cp.Void = 0 OR
cp.Void IS NULL THEN '' END AS Charge_Payment_Void, CASE WHEN tc.Void = 1 THEN 'TRUE' WHEN tc.Void = 0 OR
tc.Void IS NULL THEN '' END AS Charge_Void, 
(select isnull(RealName, LoginName) from tblUser usr where usr.userID = cp.CreatedByUserID)as PmntEnteredBy

FROM            dbo.tblUnvCharges AS tc LEFT OUTER JOIN
dbo.tblStudents AS s ON tc.EntityUID = s.StudentsUID LEFT OUTER JOIN
dbo.tblUnvChargePayments AS cp ON tc.ChargeUID = cp.ChargeUID LEFT OUTER JOIN
dbo.tblTechSites AS ts ON cp.PaymentSiteUID = ts .SiteUID LEFT OUTER JOIN
dbo.tblUnvChargeTypes AS ch ON tc.ChargeTypeUID = ch.ChargeTypeUID LEFT OUTER JOIN
dbo.tblTechItems AS ti ON tc.ItemUID = ti.ItemUID LEFT OUTER JOIN
(SELECT        uc.ChargeUID, ts .SiteID, uc.ChargeSiteUID, ts .SiteName AS chargeSite
 FROM            dbo.tblUnvCharges AS uc INNER JOIN
                  dbo.tblTechSites AS ts ON ts .SiteUID = uc.ChargeSiteUID
WHERE        (uc.EntityTypeUID = 4)) AS csite ON tc.ChargeUID = csite.ChargeUID
WHERE        (tc.EntityTypeUID = 4) AND (ch.ApplicationUID = 2)

ORDER BY Student_Campus_ID, tc.ChargeUID, Issue_Date, tc.Void, cp.Void</property>
