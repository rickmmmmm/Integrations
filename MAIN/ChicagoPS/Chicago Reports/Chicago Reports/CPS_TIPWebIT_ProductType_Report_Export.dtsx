    <DTS:Executable
      DTS:refId="Package\Stage History Data"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{D9F3FA37-9385-49BF-A4BB-BDE6D0ABC583}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Stage History Data"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2F3CE4FD-BC9E-40F9-BDBA-176261FC4338}"
          SQLTask:SqlStatementSource="IF EXISTS ( SELECT Name FROM sys.objects WHERE object_id = OBJECT_ID(N'dbo.TmpHistory'))&#xA;BEGIN &#xA; DROP TABLE TmpHistory&#xA;END&#xA;&#xA;SELECT InventoryUID, CreatedDate AS LastScanDate,LoginName AS LastScanBy&#xA;&#xA;INTO TmpHistory&#xA;&#xA;FROM&#xA;(&#xA;SELECT A.InventoryUID,CAST(H.CreatedDate AS DATE) AS CreatedDate,U.RealName AS LoginName,ROW_NUMBER() OVER(PARTITION BY A.InventoryUID ORDER BY H.CreatedDate DESC) AS RowNumber&#xA;&#xA;FROM tblTechInventory AS A WITH (NOLOCK)&#xA;&#xA;INNER JOIN tblTechItems AS B WITH (NOLOCK)&#xA;ON A.ItemUID = B.ItemUID&#xA;&#xA;INNER JOIN tblTechItemTypes AS C WITH (NOLOCK)&#xA;ON B.ItemTypeUID = C.ItemTypeUID AND C.ItemTypeName = 'CELL PHONE - HOTSPOT'&#xA;&#xA;INNER JOIN tblTechInventoryHistory AS H WITH (NOLOCK)&#xA;ON A.InventoryUID = H.InventoryUID&#xA;&#xA;INNER JOIN tblUser AS U WITH (NOLOCK)&#xA;ON H.CreatedByUserID = U.UserID&#xA;) AS History&#xA;WHERE RowNumber = 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
  </DTS:Executables>
