            <component
              refId="Package\Export Assets and Status\Get Assets"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Get Assets"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
D.SiteName,A.Tag,A.Serial,H.LastScanDate AS LastScanDate, H.LastScanBy AS LastScanBy,ISNULL(ISNULL(E.StudentID,F.TeacherID),G.RoomNumber) AS LocationID, 
CASE WHEN E.StudentID IS NOT NULL THEN 'Student' WHEN F.TeacherID IS NOT NULL THEN 'Teacher' ELSE 'Room' END  AS LocationType,
CASE WHEN E.StudentID IS NOT NULL THEN E.FullName WHEN F.TeacherID IS NOT NULL THEN F.FullName ELSE 'Room ' + G.RoomNumber END  AS LocationName,
B.ItemNumber AS ProductNumber, B.ItemName AS ProductName,C.ItemTypeName AS ProductType,I.RegionName AS Region, J.statusDesc AS Status,
K.InventorySourceName AS Source,P.OrderNumber,B.ItemSuggestedPrice AS SuggestedPrice,E.Grade,ISNULL(E.StudentEmail,F.Email) AS Email,
E.ParentEmail

FROM tblTechInventory AS A WITH (NOLOCK)

INNER JOIN tblTechItems AS B WITH (NOLOCK)
ON A.ItemUID = B.ItemUID

INNER JOIN tblTechItemTypes AS C WITH (NOLOCK)
ON B.ItemTypeUID = C.ItemTypeUID AND C.ItemTypeName = 'CELL PHONE - HOTSPOT'

INNER JOIN tblTechSites AS D WITH (NOLOCK)
ON A.SiteUID = D.SiteUID

LEFT JOIN tblStudents AS E WITH (NOLOCK)
ON A.EntityTypeUID = 4 AND A.EntityUID = E.StudentsUID

LEFT JOIN tblTeachers AS F WITH (NOLOCK)
ON A.EntityTypeUID = 3 AND A.EntityUID = F.TeachersUID

LEFT JOIN tblUnvRooms AS G WITH (NOLOCK)
ON A.EntityTypeUID = 2 AND A.EntityUID = G.RoomUID

INNER JOIN TmpHistory AS H WITH (NOLOCK)
ON A.InventoryUID = H.InventoryUID

INNER JOIN tblRegion AS I WITH (NOLOCK)
ON D.RegionID = I.RegionID

INNER JOIN tblStatus AS J WITH (NOLOCK)
ON A.StatusUID = J.statusID

INNER JOIN tblTechInventorySource AS K WITH (NOLOCK)
ON A.InventorySourceUID = K.InventorySourceUID

LEFT JOIN tblTechPurchaseInventory AS L WITH (NOLOCK)
ON A.InventoryUID = L.InventoryUID

LEFT JOIN tblTechPurchaseItemShipments AS M WITH (NOLOCK)
ON L.PurchaseItemShipmentUID = M.PurchaseItemShipmentUID

LEFT JOIN tblTechPurchaseItemDetails AS N WITH (NOLOCK)
ON M.PurchaseItemDetailUID = N.PurchaseItemDetailUID

LEFT JOIN tblTechPurchases AS P WITH (NOLOCK)
ON N.PurchaseUID = P.PurchaseUID</property>
