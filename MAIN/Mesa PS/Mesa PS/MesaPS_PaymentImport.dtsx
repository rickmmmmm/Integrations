          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{319F2CA5-DC94-4E7D-8C1E-25A639AB96AA}"
              SQLTask:SqlStatementSource="INSERT INTO dbo.tblPaymentsCollected&#xA;(ID, ISBN, Accession, Code, Price, Copies, TotalOwed,&#xA;TotalPaid, UserID, ModifiedDate, Notes, Archived)&#xA;SELECT P.ID, P.ISBN, P.Accession, P.Code, P.Price, P.Copies, P.TotalOwed,&#xA;--P.TotalPaid, 0, P.ModifiedDate, 'InTouch PaymentID ' + Notes, 0&#xA;P.TotalPaid, 0, P.ModifiedDate, 'Payment collected in InTouch', 0&#xA;FROM dbo._ETL_Payments P&#xA;LEFT JOIN tblPaymentsCollected PC ON P.ID = PC.ID&#xA;AND P.Accession = PC.Accession&#xA;WHERE [Type] = 'Payment'&#xA;AND PC.PaymentID IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
