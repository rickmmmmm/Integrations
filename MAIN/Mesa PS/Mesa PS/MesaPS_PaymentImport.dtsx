          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{319F2CA5-DC94-4E7D-8C1E-25A639AB96AA}"
              SQLTask:SqlStatementSource="UPDATE tblPaymentsCollected&#xA;SET TotalPaid = P.TotalPaid, Code = P.Code,&#xA;--Notes = CASE WHEN PC.Notes IS NULL THEN 'InTouch PaymentID ' + P.PaymentID&#xA;--ELSE PC.Notes + '; InTouch PaymentID ' + P.PaymentID END&#xA;Notes = CASE WHEN PC.Notes IS NULL THEN 'Payment collected in InTouch'&#xA;ELSE PC.Notes + '; Payment collected in InTouch' END&#xA;FROM tblPaymentsCollected PC&#xA;JOIN _ETL_Payments P ON PC.PaymentID = P.PaymentID&#xA;WHERE P.[Type] = 'Payment'&#xA;AND (PC.TotalPaid &lt;&gt; P.TotalPaid&#xA;OR PC.Code &lt;&gt; P.Code)&#xA;&#xA;GO&#xA;&#xA;UPDATE tblPaymentsCollected&#xA;SET TotalPaid = P.TotalPaid, Code = P.Code,&#xA;--Notes = CASE WHEN PC.Notes IS NULL THEN 'InTouch PaymentID ' + P.PaymentID&#xA;--ELSE PC.Notes + '; InTouch PaymentID ' + P.PaymentID END&#xA;Notes = CASE WHEN PC.Notes IS NULL THEN 'Adjustment made in InTouch'&#xA;ELSE PC.Notes + '; Adjustment made in InTouch' END&#xA;FROM tblPaymentsCollected PC&#xA;JOIN _ETL_Payments P ON PC.PaymentID = P.PaymentID&#xA;WHERE P.[Type] = 'Adjustment'&#xA;AND (PC.TotalPaid &lt;&gt; P.TotalPaid&#xA;OR PC.Code &lt;&gt; P.Code)&#xA;&#xA;GO&#xA;&#xA;UPDATE tblStudentsDistribution&#xA;SET Code = P.Code&#xA;FROM tblStudentsDistribution SD&#xA;JOIN _ETL_Payments P ON SD.Accession = P.Accession&#xA;AND SD.StudentsUID = P.ID&#xA;WHERE SD.Code &lt;&gt; P.Code&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
