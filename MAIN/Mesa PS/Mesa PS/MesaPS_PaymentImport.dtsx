              SQLTask:SqlStatementSource="&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET FineID = ''&#xA;WHERE FineID IS NULL&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Amount = ''&#xA;WHERE Amount IS NULL&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET [Type] = ''&#xA;WHERE [Type] IS NULL&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET [Date] = ''&#xA;WHERE [Date] IS NULL&#xA;GO&#xA;&#xA;--select *,&#xA;UPDATE SP&#xA;SET Import = 0, RejectMessage = &#xA;CASE WHEN RejectMessage IS NULL THEN 'ChargeUID not found in TIPWeb-IM'&#xA;ELSE RejectMessage + '; ' +  'ChargeUID not found in TIPWeb-IM' END&#xA;FROM _ETL_StagingPayments SP&#xA;LEFT JOIN tblUnvCharges UC ON SP.FineID = UC.ChargeUID&#xA;WHERE UC.ChargeUID IS NULL&#xA;&#xA;GO&#xA;--------------------------------------------------&#xA;&#xA;--select *,&#xA;UPDATE SP&#xA;SET Import = 0, RejectMessage = &#xA;CASE WHEN RejectMessage IS NULL THEN 'Payment already imported'&#xA;ELSE RejectMessage + '; ' +  'Payment already imported' END&#xA;FROM dbo._ETL_StagingPayments SP&#xA;JOIN tblUnvCharges UC ON SP.FineID = UC.ChargeUID&#xA;where DateSatisfied is not null&#xA;GO&#xA;--------------------------------------------------&#xA;&#xA;--select *,&#xA;UPDATE SP&#xA;SET Import = 0, RejectMessage = &#xA;CASE WHEN RejectMessage IS NULL THEN 'Invalid Amount'&#xA;ELSE RejectMessage + '; ' +  'Invalid Amount' END&#xA;from dbo._ETL_StagingPayments SP&#xA;WHERE ISNUMERIC(Amount) = 0&#xA;&#xA;GO&#xA;--------------------------------------------------&#xA;--select *, &#xA;UPDATE SP &#xA;SET Import = 0, RejectMessage = &#xA;CASE WHEN RejectMessage IS NULL THEN 'Type must be Payment or Adjustment'&#xA;ELSE RejectMessage + '; ' +  'Type must be Payment or Adjustment' END&#xA;from dbo._ETL_StagingPayments SP&#xA;WHERE [Type] NOT IN ('Payment', 'Adjustment')&#xA;&#xA;GO&#xA;--------------------------------------------------&#xA;&#xA;--select *,&#xA;UPDATE SP&#xA;SET Import = 0, RejectMessage = &#xA;CASE WHEN RejectMessage IS NULL THEN 'Invalid Date'&#xA;ELSE RejectMessage + '; ' +  'Invalid Date' END&#xA;from dbo._ETL_StagingPayments SP&#xA;WHERE ISDATE([Date]) = 0&#xA;&#xA;GO&#xA;--------------------------------------------------&#xA;UPDATE  dbo._ETL_StagingPayments&#xA;SET Import = 1&#xA;WHERE Import IS NULL&#xA;&#xA;GO &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
