          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{319F2CA5-DC94-4E7D-8C1E-25A639AB96AA}"
              SQLTask:SqlStatementSource="UPDATE _ETL_StagingPayments&#xA;SET Amount = CONVERT(VARCHAR, SJ.Amount)&#xA;FROM _ETL_StagingPayments SP&#xA;JOIN (SELECT MAX(ETLStagingPaymentUID) AS ID,&#xA;SUM(CONVERT(MONEY, Amount)) AS Amount&#xA;FROM _ETL_StagingPayments&#xA;WHERE Accepted = 1&#xA;GROUP BY FineID) SJ ON SP.ETLStagingPaymentUID = SJ.ID&#xA;&#xA;GO&#xA;&#xA;INSERT INTO dbo._ETL_Payments&#xA;(ID, ISBN, Accession, Code, Price, Copies, TotalOwed,&#xA;TotalPaid, [Type], ModifiedDate)--, Notes)&#xA;SELECT NULL, NULL, FineID, NULL, NULL, NULL, NULL,&#xA;ABS(CONVERT(money, Amount)/100), 'Payment',&#xA;CONVERT(datetime, [Date])--, PaymentID&#xA;FROM _ETL_StagingPayments&#xA;WHERE ETLStagingPaymentUID IN&#xA;(SELECT MAX(ETLStagingPaymentUID) AS ID&#xA;FROM _ETL_StagingPayments&#xA;WHERE Accepted = 1&#xA;GROUP BY FineID)&#xA;AND [Type] = 'Payment'&#xA;&#xA;GO&#xA;&#xA;INSERT INTO dbo._ETL_Payments&#xA;(ID, ISBN, Accession, Code, Price, Copies, TotalOwed,&#xA;TotalPaid, [Type], ModifiedDate)--, Notes)&#xA;SELECT NULL, NULL, FineID, NULL, NULL, NULL, NULL,&#xA;ABS(CONVERT(money, Amount)/100) * -1, 'Adjustment',&#xA;CONVERT(datetime, [Date])--, PaymentID&#xA;FROM _ETL_StagingPayments&#xA;WHERE ETLStagingPaymentUID IN&#xA;(SELECT MAX(ETLStagingPaymentUID) AS ID&#xA;FROM _ETL_StagingPayments&#xA;WHERE Accepted = 1&#xA;GROUP BY FineID)&#xA;AND [Type] = 'Adjustment'&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
