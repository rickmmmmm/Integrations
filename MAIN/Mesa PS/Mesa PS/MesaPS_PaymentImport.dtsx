            <SQLTask:SqlTaskData
              SQLTask:Connection="{319F2CA5-DC94-4E7D-8C1E-25A639AB96AA}"
              SQLTask:SqlStatementSource="UPDATE dbo._ETL_Payments&#xA;SET PaymentID = PC.PaymentID, ID = PC.ID, ISBN = PC.ISBN,&#xA;Price = PC.Price, Copies = PC.Copies, TotalOwed = PC.TotalOwed,&#xA;TotalPaid = P.TotalPaid + PC.TotalPaid&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblStudentsDistribution SD ON P.Accession = SD.Accession&#xA;JOIN tblPaymentsCollected PC ON SD.Accession = PC.Accession&#xA;AND SD.StudentsUID = PC.ID&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET ID = SD.StudentsUID, ISBN = SD.ISBN, Price = BI.Price,&#xA;Copies = 1, TotalOwed = BI.Price&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblStudentsDistribution SD ON P.Accession = SD.Accession&#xA;JOIN tblBookInventory BI ON SD.ISBN = BI.ISBN&#xA;WHERE P.PaymentID IS NULL&#xA;AND SD.Code IN ('LOST', 'PRTL')&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET TotalPaid = 0&#xA;WHERE TotalPaid &lt; 0&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET TotalPaid = TotalOwed&#xA;WHERE TotalPaid &gt; TotalOwed&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET Code = 'PAY1'&#xA;WHERE [Type] = 'Payment'&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET Code = 'PRTL'&#xA;WHERE TotalPaid &lt; TotalOwed&#xA;AND TotalPaid &gt; 0&#xA;AND [Type] = 'Adjustment'&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET Code = 'LOST'&#xA;WHERE TotalPaid = 0&#xA;AND [Type] = 'Adjustment'&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
