            <SQLTask:SqlTaskData
              SQLTask:Connection="{319F2CA5-DC94-4E7D-8C1E-25A639AB96AA}"
              SQLTask:SqlStatementSource="UPDATE dbo._ETL_StagingPayments&#xA;SET FineID = ''&#xA;WHERE FineID IS NULL&#xA;&#xA;GO&#xA;&#xA;--UPDATE dbo._ETL_StagingPayments&#xA;--SET PaymentID = ''&#xA;--WHERE PaymentID IS NULL&#xA;&#xA;--GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Amount = ''&#xA;WHERE Amount IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET [Type] = ''&#xA;WHERE [Type] IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET [Date] = ''&#xA;WHERE [Date] IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Fine ID not found in TIPWeb-IM'&#xA;ELSE Note + '; ' +  'Fine ID not found in TIPWeb-IM' END&#xA;FROM _ETL_StagingPayments SP&#xA;LEFT JOIN tblStudentsDistribution SD ON SP.FineID = SD.Accession&#xA;WHERE SD.DistributionID IS NULL&#xA;&#xA;GO&#xA;&#xA;--UPDATE dbo._ETL_StagingPayments&#xA;--SET Accepted = 0,&#xA;--Note = CASE WHEN Note IS NULL THEN 'Missing Payment ID'&#xA;--ELSE Note + '; ' +  'Missing Payment ID' END&#xA;--WHERE PaymentID = ''&#xA;&#xA;--GO&#xA;&#xA;--UPDATE dbo._ETL_StagingPayments&#xA;--SET Accepted = 0,&#xA;--Note = CASE WHEN Note IS NULL THEN 'Payment already imported'&#xA;--ELSE Note + '; ' +  'Payment already imported' END&#xA;--FROM dbo._ETL_StagingPayments SP&#xA;--JOIN tblPaymentsCollected PC ON PC.Notes LIKE '%' + SP.PaymentID + '%'&#xA;&#xA;--GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid Amount'&#xA;ELSE Note + '; ' +  'Invalid Amount' END&#xA;WHERE ISNUMERIC(Amount) = 0&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Type must be Payment or Adjustment'&#xA;ELSE Note + '; ' +  'Type must be Payment or Adjustment' END&#xA;WHERE [Type] NOT IN ('Payment', 'Adjustment')&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid Date'&#xA;ELSE Note + '; ' +  'Invalid Date' END&#xA;WHERE ISDATE([Date]) = 0&#xA;&#xA;GO&#xA;&#xA;UPDATE  dbo._ETL_StagingPayments&#xA;SET Accepted = 1&#xA;WHERE Accepted IS NULL&#xA;&#xA;GO &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
