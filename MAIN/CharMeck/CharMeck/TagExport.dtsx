        public void Main()
        {
            String filedir;
            filedir = Dts.Variables["User::DeleteFileName"].Value.ToString();
            // string[] files = Directory.GetFiles(filedir);
            //MessageBox.Show(filedir);
            //MessageBox.Show(File.GetCreationTime(filedir).ToString());

            if (File.GetCreationTime(filedir)
              < DateTime.Now.AddDays(-14))
            {
                File.Delete(filedir);
               
            }
            Dts.TaskResult = (int)ScriptResults.Success;
        }

