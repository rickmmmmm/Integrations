        <SQLTask:SqlTaskData
          SQLTask:Connection="{733D2823-8F17-4E3F-BA6D-3496A7FAF0A6}"
          SQLTask:SqlStatementSource="&#xA;--select * &#xA;update etl set InventoryNotes = replace(replace(replace(replace(tti.InventoryNotes, char(10), ' '), char(13), ''), char(9), ' '), '&quot;', '')&#xA;from _ETL_TagExport etl&#xA;join tblTechInventory tti on etl.InventoryUID = tti.InventoryUID&#xA;&#xA;&#xA;--select * &#xA;update etl set PoliceReportNumber = (case when tid.PoliceReportNumber is null then '' else tid.PoliceReportNumber end), &#xA;&#x9;LastScanDate = (case when tid.PoliceReportNumber is not null &#xA;&#x9;&#x9;and tid.LastModifiedDate &gt; cast(etl.LastScanDate as datetime) &#xA;&#x9;&#x9;then convert(varchar(10), tid.LastModifiedDate, 101) else etl.LastScanDate end), &#xA;&#x9;LastScanBy = (case when tid.PoliceReportNumber is not null &#xA;&#x9;&#x9;and tid.LastModifiedDate &gt; cast(etl.LastScanDate as datetime) &#xA;&#x9;&#x9;then u.RealName else etl.LastScanBy end)&#xA;&#xA;from _ETL_TagExport etl&#xA;join tblTechInventoryDetails tid on etl.InventoryUID = tid.InventoryUID&#xA;join tblUser u on tid.LastModifiedByUserID = u.UserID&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
