      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{733D2823-8F17-4E3F-BA6D-3496A7FAF0A6}"
          SQLTask:SqlStatementSource="update _ETL_TagExport &#xA;set CustomField1_Label = ltrim(rtrim(CustomField1_Label)), &#xA;&#x9;CustomField2_Label = ltrim(rtrim(CustomField2_Label)), &#xA;&#x9;CustomField3_Label = ltrim(rtrim(CustomField3_Label)), &#xA;&#x9;CustomField4_Label = ltrim(rtrim(CustomField4_Label))&#xA;GO&#xA;&#xA;&#xA;&#xA;--select * &#xA;update etl set CustomField1_Type = mta.InventoryMetaType&#xA;from _ETL_TagExport etl&#xA;join tblTechInventoryMeta mta on etl.ItemTypeUID = mta.ItemTypeUID and etl.CustomField1_Label = mta.InventoryMetaLabel&#xA;GO &#xA;--select * &#xA;update etl set CustomField2_Type = mta.InventoryMetaType&#xA;from _ETL_TagExport etl&#xA;join tblTechInventoryMeta mta on etl.ItemTypeUID = mta.ItemTypeUID and etl.CustomField2_Label = mta.InventoryMetaLabel&#xA;GO&#xA;--select * &#xA;update etl set CustomField3_Type = mta.InventoryMetaType&#xA;from _ETL_TagExport etl&#xA;join tblTechInventoryMeta mta on etl.ItemTypeUID = mta.ItemTypeUID and etl.CustomField3_Label = mta.InventoryMetaLabel&#xA;GO&#xA;--select * &#xA;update etl set CustomField4_Type = mta.InventoryMetaType&#xA;from _ETL_TagExport etl&#xA;join tblTechInventoryMeta mta on etl.ItemTypeUID = mta.ItemTypeUID and etl.CustomField4_Label = mta.InventoryMetaLabel&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
