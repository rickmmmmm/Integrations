          SQLTask:SqlStatementSource="insert _ETL_TagExport &#xA;&#x9;(InventoryUID, tag, Serial, productNumber, productName, model, ManufacturerName, ProductType, &#xA;&#x9;SiteID, SiteName, EntityTypeDescription, LocationID, LocationDescription, RoomDescription, [Status],&#xA;&#x9;CustomField1_Type, CustomField1_Label, CustomField1_Value, &#xA;&#x9;CustomField2_Type, CustomField2_Label, CustomField2_Value, &#xA;&#x9;CustomField3_Type, CustomField3_Label, CustomField3_Value, &#xA;&#x9;CustomField4_Type, CustomField4_Label, CustomField4_Value, &#xA;&#x9;FundingSource, PurchaseOrder, LastScanDate, LastScanBy, ItemTypeUID&#xA;)&#xA;&#xA;&#xA;select InventoryUID, tag, Serial, productNumber, productName, model, ManufacturerName, ItemTypeName as ProductType, &#xA;SiteID, SiteName, EntityTypeDescription, LocationID, LocationDescription, RoomDescription, [Status],&#xA;'' as CustomField1_Type,&#xA;case when charindex(':', isNull(CustomTagField1, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField1, 1, charindex(':', CustomTagField1)-1) &#xA;&#x9;else '' end as CustomField1_Label,&#xA;case when charindex(':', isNull(CustomTagField1, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField1, charindex(':', CustomTagField1)+1, 100) &#xA;&#x9;else '' end as CustomField1_Value,&#xA;'' as CustomField2_Type,&#xA;case when charindex(':', isNull(CustomTagField2, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField2, 1, charindex(':', CustomTagField2)-1) &#xA;&#x9;else '' end as CustomField2_Label,&#xA;case when charindex(':', isNull(CustomTagField2, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField2, charindex(':', CustomTagField2)+1, 100) &#xA;&#x9;else '' end as CustomField2_Value,&#xA;'' as CustomField3_Type,&#xA;case when charindex(':', isNull(CustomTagField3, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField3, 1, charindex(':', CustomTagField3)-1) &#xA;&#x9;else '' end as CustomField3_Label,&#xA;case when charindex(':', isNull(CustomTagField3, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField3, charindex(':', CustomTagField3)+1, 100) &#xA;&#x9;else '' end as CustomField3_Value,&#xA;'' as CustomField4_Type,&#xA;case when charindex(':', isNull(CustomTagField4, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField4, 1, charindex(':', CustomTagField4)-1) &#xA;&#x9;else '' end as CustomerField4_Label,&#xA;case when charindex(':', isNull(CustomTagField4, '')) &gt; 1 &#xA;&#x9;then substring(CustomTagField4, charindex(':', CustomTagField4)+1, 100) &#xA;&#x9;else '' end as CustomerField4_Value,&#xA;FundingSource, isNull(OrderNumber, '') as PurchaseOrder, convert(varchar(10), LastScanDate, 101) as LastScanDate, LastScanBy, &#xA;ItemTypeUID&#xA;&#xA;from [FTechInventoryMeta]() &#xA;where inventoryUID &gt; 0&#xA;---and siteID in ('374', '594', '397', '9707')&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
