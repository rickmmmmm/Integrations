    <DTS:Executable
      DTS:refId="Package\Get Tag Change Date"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{2832B44B-D85A-4CC5-8F29-64C96FD74EFF}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Get Tag Change Date"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{733D2823-8F17-4E3F-BA6D-3496A7FAF0A6}"
          SQLTask:SqlStatementSource="--select etl.InventoryUID, histDate.maxDate, etl.LastScanDate, --*, &#xA;update etl set LastScanDate = &#xA;&#x9;(case when histDate.maxDate &gt; cast(etl.LastScanDate as datetime) then convert(varchar(10), histDate.maxDate, 101) else etl.LastScanDate end), &#xA;&#x9;LastScanBy = (case when histDate.maxDate &gt; cast(etl.LastScanDate as datetime) then u.RealName else etl.LastScanBy end)&#xA;from _ETL_TagExport etl&#xA;join (&#xA;&#x9;select InventoryUID, CreatedByUserID, th.CreateDate as maxDate&#xA;&#x9;from tblTechTagHistory th&#xA;&#x9;where TagHistoryUID = &#xA;&#x9;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;select max(TagHistoryUID) &#xA;&#x9;&#x9;&#x9;from tblTechTagHistory th1 where InventoryUID = th.InventoryUID&#xA;&#x9;&#x9;&#x9;and (OriginTag is not null or OriginSerial is not null)&#xA;&#x9;&#x9;&#x9;)&#xA;&#x9;) histDate on histDate.InventoryUID = etl.InventoryUID&#xA;&#xA;join tblUser u on histDate.CreatedByUserID = u.UserID" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Get Tag Notes and Police Report"
