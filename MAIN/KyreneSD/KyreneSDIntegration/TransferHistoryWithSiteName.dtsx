                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT trn.TransferNumber, tti.Tag, tti.Serial, 
		ttri.CreatedDate as TransferCreatedDate,
      StartSite.SiteID  as [FromSiteID], 
      StartSite.SiteName as [FromSiteName],
      StartRoom.RoomNumber AS [From Room],

      DestSite.SiteID AS [Destination Site ID], 
      DestSite.SiteName as DesinationSiteName,
      DestRoom.RoomNumber AS [Destination Room]
, isNull((select InventoryExtValue 
	from tblTechInventoryExt ext
	join tblTechInventoryMeta meta on ext.InventoryMetaUID = meta.InventoryMetaUID
	where InventoryUID = tti.InventoryUID
	and InventoryMetaLabel = 'Computer Name' ), '') as ComputerName
      
FROM  tblTechTransferInventory ttri
INNER JOIN tblTechInventory tti ON ttri.InventoryUID = tti.InventoryUID 
INNER JOIN tblTechTransfers trn ON ttri.TransferUID = trn.TransferUID 

INNER JOIN tblTechInventoryHistory  StartHistory ON StartHistory.InventoryUID = tti.InventoryUID 
	and StartHistory.EntityUID = trn.TransferUID and StartHistory.EntityTypeUID = 6
INNER JOIN tblTechSites StartSite ON StartHistory.OriginSiteUID = StartSite.SiteUID 
INNER JOIN tblUnvRooms StartRoom ON StartHistory.OriginEntityUID = StartRoom.RoomUID 
	AND StartHistory.OriginEntityTypeUID = 2

INNER JOIN tblTechInventoryHistory  DestHistory ON DestHistory.InventoryUID = tti.InventoryUID 
	and DestHistory.OriginEntityUID = trn.TransferUID and DestHistory.OriginEntityTypeUID = 6
INNER JOIN tblTechSites DestSite ON DestHistory.SiteUID = DestSite.SiteUID 
INNER JOIN tblUnvRooms DestRoom ON DestHistory.EntityUID = DestRoom.RoomUID 
	AND DestHistory.EntityTypeUID = 2

WHERE (ttri.Received = '1')
order by 1</property>
