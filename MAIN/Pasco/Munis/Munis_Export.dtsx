                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @FromDate DATE = ?

SELECT DISTINCT A.AssetID,C.SiteID,C.SiteName AS [Site]

FROM tblTechInventory AS A WITH (NOLOCK)

INNER JOIN tblTechDepartments AS B WITH (NOLOCK)
ON A.TechDepartmentUID=B.TechDepartmentUID

INNER JOIN tblTechSites AS C WITH (NOLOCK)
ON A.SiteUID=C.SiteUID

INNER JOIN tblTechInventoryHistory AS D
ON A.InventoryUID=D.InventoryUID AND D.SiteUID != D.OriginSiteUID 
 
WHERE A.InventoryUID &gt; 1
AND A.ArchiveUID = 0
AND CAST(D.CreatedDate AS date) BETWEEN @FromDate  AND CAST(GETDATE() AS date)</property>
