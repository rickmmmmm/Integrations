                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select tti.tag, OriginTag, tti.Serial, SiteID, 
replace(replace(replace(replace(SiteName, char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as SiteName,
replace(replace(replace(replace(ItemNumber,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as ItemNumber,
replace(replace(replace(replace(ItemName,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as ItemName, 
replace(replace(replace(replace(ttm.ModelNumber,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as ModelNumber,
replace(replace(replace(replace(ItemTypeName,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as ItemTypeName,
replace(replace(replace(replace(fs.FundingSource,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as FundingSource,
isNull(po.OrderNumber, '') as PurchaseOrder,
isNull(po.PurchaseDate, isNull(tti.PurchaseDate, '')) as PurchaseDate,
case 
	when tti.EntityTypeUID = 2 then 'Room'
	when tti.EntityTypeUID = 3 then 'Teacher'
	when tti.EntityTypeUID = 4 then 'Student'
end as AssignedToEntity,
replace(replace(replace(replace(
	(case 
		when tti.EntityTypeUID = 2 then rm.RoomNumber
		when tti.EntityTypeUID = 3 then tch.FullName
		when tti.EntityTypeUID = 4 then stu.FullName
	end )
	,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ')  AssignedToEntityName,
s.statusDesc as TagStatus,
th.CreatedByUserID, u.RealName as CreatedBy, 
convert(varchar(10), th.createdate, 101) as CreatedDate,
replace(replace(replace(replace( isNull(ProductChangeNote, '') ,  char(44), ' '), char(10), ' '), char(13), ''), char(9), ' ') as ProductChangeNote

from tblTechTagHistory th
join tblTechInventory tti on th.InventoryUID = tti.InventoryUID
join tblTechSites ts on tti.SiteUID = ts.SiteUID
join tblTechItems ttm on tti.ItemUID = ttm.ItemUID
join tblTechItemTypes tty on ttm.ItemTypeUID = tty.ItemTypeUID
join tblFundingSources fs on tti.FundingSourceUID = fs.FundingSourceUID
join tblStatus s on tti.StatusUID = s.statusID
join tblUser u on th.CreatedByUserID = u.UserID
left outer join (
	select OrderNumber, tpi.InventoryUID, tpur.PurchaseDate
	from tblTechPurchases tpur
	join tblTechPurchaseItemDetails tpid on tpur.PurchaseUID = tpid.PurchaseUID
	join tblTechPurchaseItemShipments tship on tpid.PurchaseItemDetailUID = tship.PurchaseItemDetailUID
	join tblTechPurchaseInventory tpi on tship.PurchaseItemShipmentUID = tpi.PurchaseItemShipmentUID
	) po on tti.InventoryUID = po.InventoryUID
left outer join (
	select TeachersUID, FullName
	from tblTeachers
	) tch on tti.EntityTypeUID = 3 and tti.EntityUID = tch.TeachersUID
left outer join (
	select StudentsUID, FullName
	from tblStudents
	) stu on tti.EntityTypeUID = 4 and tti.EntityUID = stu.StudentsUID
left outer join (
	Select RoomUID, RoomNumber
	from tblUnvRooms
	) rm on tti.EntityTypeUID = 2 and tti.EntityUID = RoomUID
where th.origintag is not null
and th.CreateDate &gt; ?</property>
