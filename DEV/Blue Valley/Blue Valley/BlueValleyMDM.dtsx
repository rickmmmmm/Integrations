              SQLTask:Connection="{3EE44891-4CDA-4EFA-A941-87C7746BEA64}"
              SQLTask:SqlStatementSource="UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Serial missing'&#xA;ELSE Note + '; ' +  'Serial missing' END&#xA;WHERE LTRIM(RTRIM(ISNULL(Serial, ''))) = ''&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Serial longer than 50 characters'&#xA;ELSE Note + '; ' +  'Serial longer than 50 characters' END&#xA;WHERE LEN(Serial) &gt; 50&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Serial not found in TIPWeb-IT'&#xA;ELSE Note + '; ' +  'Serial not found in TIPWeb-IT' END&#xA;FROM _ETL_StagingMDM&#xA;LEFT JOIN tblTechInventory&#xA;ON UPPER(LTRIM(RTRIM(ISNULL(_ETL_StagingMDM.Serial, '')))) = UPPER(tblTechInventory.Serial)&#xA;WHERE LTRIM(RTRIM(ISNULL(_ETL_StagingMDM.Serial, ''))) &lt;&gt; ''&#xA;AND tblTechInventory.InventoryUID IS NULL&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Duplicate Serial in Import'&#xA;ELSE Note + '; ' +  'Duplicate Serial in Import' END&#xA;WHERE Serial IN&#xA;(SELECT Serial FROM _ETL_StagingMDM&#xA;GROUP BY Serial HAVING COUNT(*) &gt; 1)&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Duplicate Serial in TIPWeb-IT'&#xA;ELSE Note + '; ' +  'Duplicate Serial in TIPWeb-IT' END&#xA;WHERE Serial IN&#xA;(SELECT Serial FROM tblTechInventory&#xA;GROUP BY Serial HAVING COUNT(*) &gt; 1)&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Device Name longer than 50 characters'&#xA;ELSE Note + '; ' +  'Device Name longer than 50 characters' END&#xA;WHERE LEN(ComputerName) &gt; 50&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Device Type longer than 50 characters'&#xA;ELSE Note + '; ' +  'Device Type longer than 50 characters' END&#xA;WHERE LEN(DeviceType) &gt; 50&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingMDM&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid Product Type: ' + tblTechItemTypes.ItemTypeName&#xA;ELSE Note + '; ' +  'Invalid Product Type: ' + tblTechItemTypes.ItemTypeName END&#xA;FROM _ETL_StagingMDM&#xA;JOIN tblTechInventory ON _ETL_StagingMDM.Serial = tblTechInventory.Serial&#xA;JOIN tblTechItems ON tblTechInventory.ItemUID = tblTechItems.ItemUID&#xA;JOIN tblTechItemTypes ON tblTechItems.ItemTypeUID = tblTechItemTypes.ItemTypeUID&#xA;WHERE tblTechItemTypes.ItemTypeName NOT IN&#xA; (select ItemTypeName&#xA; from tblTechItemTypes&#xA; where ItemTypeuid in &#xA;  (select Itemtypeuid from tblTechInventoryMeta &#xA;  where ((InventoryMetaLabel = 'Device Name' and InventoryMetaOrder = 1)&#xA;  or (InventoryMetaLabel = 'Program Type' and InventoryMetaOrder = 2)&#xA;  or (InventoryMetaLabel = 'Program Device Type' and InventoryMetaOrder = 3))&#xA;  )&#xA; )&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE  dbo._ETL_StagingMDM&#xA;SET Accepted = 1&#xA;WHERE Accepted IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
