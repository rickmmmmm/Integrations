              SQLTask:SqlStatementSource="DECLARE&#xA; @bodyHTML NVARCHAR(MAX),&#xA; @title NVARCHAR(150),&#xA; @email NVARCHAR(1000),&#xA; @ccemail NVARCHAR(1000),&#xA; @mailProfile NVARCHAR(100),&#xA; @clientname NVARCHAR(100),&#xA; @totalcount NVARCHAR(20),&#xA; @acceptcount NVARCHAR(20),&#xA; @rejectcount NVARCHAR(20),&#xA; @jobname NVARCHAR(100),&#xA; @attachment NVARCHAR(1000),&#xA; @attachments NVARCHAR(MAX)&#xA;&#xA;SET @email = ?&#xA;SET @ccemail = ?&#xA;SET @clientname = ?&#xA;SET @jobname = ?&#xA;SET @attachment = ?&#xA;SET @mailProfile = ?&#xA;&#xA;&#xA;SET @acceptcount =  CAST((SELECT ISNULL(acceptcount, '0') FROM&#xA;(SELECT  COUNT(*) AS acceptcount&#xA;FROM _ETL_StagingMDM&#xA;WHERE Accepted = 1) a) AS VARCHAR(20))&#xA;&#xA;SET @rejectcount =  CAST((SELECT ISNULL(rejectcount, '0') FROM&#xA;(SELECT  COUNT(*) AS rejectcount&#xA;FROM _ETL_StagingMDM&#xA;WHERE Accepted = 0) a) AS VARCHAR(20))&#xA;&#xA;SET @totalcount = CONVERT(VARCHAR(20), CONVERT(INT, @acceptcount) + CONVERT(INT, @rejectcount))&#xA;&#xA;SET @attachments = @attachment&#xA;&#xA;SET @title = @jobname + ' Integration Status - '+  CONVERT(VARCHAR(10), GETDATE(), 101)&#xA;&#xA;SET @bodyHTML = &#xA;N'&lt;html&gt;&lt;body&gt;The Error file has been generated&lt;br&gt;&lt;br&gt;'+&#xA;N'Total Records Processed: '+ @totalcount + '&lt;br&gt;&lt;br&gt;'+&#xA;N'Number of Records Accepted: '+ @acceptcount + '&lt;br&gt;&lt;br&gt;'+&#xA;N'Number of Records Rejected: '+ @rejectcount + '&lt;br&gt;&lt;br&gt;'+&#xA;N'If you have any questions, please contact us at 1-800-495-5993&lt;br&gt;&lt;br&gt;'+&#xA;N'Thank you,&lt;br&gt;&lt;br&gt;'+&#xA;N'Hayes Software Systems&lt;br&gt;'+&#xA;N'www.hayessoft.com'+&#xA;N'&lt;/body&gt;&lt;/html&gt;'&#xA;&#xA;EXEC msdb.dbo.sp_send_dbmail&#xA; @profile_name = @mailProfile,&#xA; @recipients = @email,&#xA; @copy_recipients = @ccemail,&#xA; @body_format = 'HTML',&#xA; @body = @bodyHTML, &#xA; @subject = @title,&#xA; @file_attachments = @attachments;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
              <SQLTask:ParameterBinding
                SQLTask:ParameterName="@email"
                SQLTask:DtsVariableName="$Project::Email"
                SQLTask:ParameterDirection="Input"
                SQLTask:DataType="130"
                SQLTask:ParameterSize="1000" />
              <SQLTask:ParameterBinding
                SQLTask:ParameterName="@ccemail"
