                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT ISNULL(A.AssetID,A.Serial) AS 'AbsoluteIdentifier',C.SiteID AS 'SchoolID',
A.Tag AS 'AssetTag',A.Serial AS SerialNumber,
E.StatusDesc AS 'AssetTagStatus',
'Room' AS 'Location'

FROM tblTechInventory AS A

INNER JOIN tblUnvRooms AS B
ON A.EntityUID=B.RoomUID

INNER JOIN tblTechSites AS C
ON B.SiteUID=C.SiteUID

INNER JOIN tblStatus AS E
ON A.StatusUID=E.statusID

INNER JOIN tblTechItems AS F
ON A.ItemUID=F.ItemUID

INNER JOIN tblTechItemTypes AS G
ON F.ItemTypeUID=G.ItemTypeUID
AND G.ItemTypeName IN ('DESKTOP', 'LAPTOP (CHROMEBOOK)','LAPTOP (MACBOOK)','LAPTOP (PC)','TABLET/IPAD')

WHERE A.EntityTypeUID=2 AND A.ArchiveUID=0
AND A.AssetID IS NOT NULL

UNION 

SELECT ISNULL(A.AssetID,A.Serial) AS 'AbsoluteIdentifier',CAST(C.CampusID AS varchar(50)) AS 'SchoolID',
A.Tag AS 'AssetTag',A.Serial AS SerialNumber,
E.StatusDesc AS 'AssetTagStatus',
'Staff' AS 'Location'

FROM tblTechInventory AS A

INNER JOIN tblTeachers AS B
ON A.EntityUID=B.TeachersUID

INNER JOIN tblCampuses AS C
ON B.CampusID=C.CampusID

INNER JOIN tblStatus AS E
ON A.StatusUID=E.statusID

INNER JOIN tblTechItems AS F
ON A.ItemUID=F.ItemUID

INNER JOIN tblTechItemTypes AS G
ON F.ItemTypeUID=G.ItemTypeUID
AND G.ItemTypeName IN ('DESKTOP', 'LAPTOP (CHROMEBOOK)','LAPTOP (MACBOOK)','LAPTOP (PC)','TABLET/IPAD')

WHERE A.EntityTypeUID=3 AND A.ArchiveUID=0
AND A.AssetID IS NOT NULL

UNION 

SELECT ISNULL(A.AssetID,A.Serial) AS 'AbsoluteIdentifier',CAST(C.CampusID AS varchar(50)) AS 'SchoolID',
A.Tag AS 'AssetTag',A.Serial AS SerialNumber,
E.StatusDesc AS 'AssetTagStatus',
'Student' AS 'Location'

FROM tblTechInventory AS A

INNER JOIN tblStudents AS B
ON A.EntityUID=B.StudentsUID

INNER JOIN tblCampuses AS C
ON B.CampusID=C.CampusID

INNER JOIN tblStatus AS E
ON A.StatusUID=E.statusID

INNER JOIN tblTechItems AS F
ON A.ItemUID=F.ItemUID

INNER JOIN tblTechItemTypes AS G
ON F.ItemTypeUID=G.ItemTypeUID
AND G.ItemTypeName IN ('DESKTOP', 'LAPTOP (CHROMEBOOK)','LAPTOP (MACBOOK)','LAPTOP (PC)','TABLET/IPAD')

WHERE A.EntityTypeUID=4 AND A.ArchiveUID=0
AND A.AssetID IS NOT NULL</property>
