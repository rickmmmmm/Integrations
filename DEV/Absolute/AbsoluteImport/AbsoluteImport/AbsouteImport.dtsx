		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{

            // Paths 
            bool Debug = Convert.ToBoolean(Dts.Variables["Debug"].Value);
            string SFTPPath_bat = Dts.Variables["SFTPPath_bat"].Value.ToString();
            string SFTPPath_exe = Dts.Variables["SFTPPath_exe"].Value.ToString();
            string SFTPPath_sc = Dts.Variables["SFTPPath_sc"].Value.ToString();
            string SFTPPath_cert = Dts.Variables["SFTPPath_cert"].Value.ToString();
            //Sending Files
            //string SFTPPath_file = Dts.Variables["RootPath"].Value.ToString() + Dts.Variables["ExportFilePath"].Value.ToString();
            //Recieving Files
            string SFTPPath_file = Dts.Variables["ExportFilePath"].Value.ToString();

            string SFTPPath_Downaload = Dts.Variables["SFTPDownloadPath"].Value.ToString();

            if (Debug == true)
            {
                MessageBox.Show("SFTP BatchFile: " + SFTPPath_bat.ToString());
            }

            // Check to make sure "\" is at the end of path
            // Bat File Path
            if (SFTPPath_bat.Substring(SFTPPath_bat.Length - 1, 1) != "\\")
            {
                SFTPPath_bat = SFTPPath_bat + "\\";
            }
            // Exe File Path
            if (SFTPPath_exe.Substring(SFTPPath_exe.Length - 1, 1) != "\\")
            {
                SFTPPath_exe = SFTPPath_exe + "\\";
            }
            // Script File Path
            if (SFTPPath_sc.Substring(SFTPPath_sc.Length - 1, 1) != "\\")
            {
                SFTPPath_exe = SFTPPath_sc + "\\";
            }
            // Cert file Path
            if (SFTPPath_cert.Substring(SFTPPath_cert.Length - 1, 1) != "\\")
            {
                SFTPPath_cert = SFTPPath_cert + "\\";
            }

            // Does Script path exists?
            bool DirectoryExists = Directory.Exists(SFTPPath_sc);

            // Create Directory if it does not exists
            if (!DirectoryExists)
            {
                Directory.CreateDirectory(SFTPPath_sc);
            }

            // Does Bat path exists?
            DirectoryExists = Directory.Exists(SFTPPath_bat);

            // Create Bat Directory if it does not exists
            if (!DirectoryExists)
            {
                Directory.CreateDirectory(SFTPPath_bat);
            }


            // Name of Files 
            if (Debug == true)
            {
                MessageBox.Show("Setting SFTP File Names");
            }
            string SFTP_bat =  Dts.Variables["SFTP_bat"].Value.ToString().Replace(".bat", "_" + Dts.Variables["CustomerCode"].Value.ToString()+".bat");
            string SFTP_exe = Dts.Variables["SFTP_exe"].Value.ToString();
            string SFTP_sc = Dts.Variables["SFTP_sc"].Value.ToString().Replace(".sc", "_" + Dts.Variables["CustomerCode"].Value.ToString() + ".sc");
            string SFTP_cert = Dts.Variables["SFTP_cert"].Value.ToString();
            string SFTP_file = Dts.Variables["ExportFileName"].Value.ToString();

            // Connection Information
            if (Debug == true)
            {
                MessageBox.Show("Setting SFTP Connection Information");
            }
            string SFTPServer = Dts.Variables["SFTP_Server"].Value.ToString();
            string SFTPUserName = Dts.Variables["SFTPUserName"].Value.ToString();
            string SFTPUserPassword = Dts.Variables["SFTPUserPassword"].Value.ToString();

            // Create dynamic content of script file with local variable values
            if (Debug == true)
            {
                MessageBox.Show("Setting script commands");
            }
            string commands = "cd " + SFTPPath_file + System.Environment.NewLine;
            commands = commands + "lcd " + SFTPPath_Downaload + System.Environment.NewLine;
            commands = commands + "mget *" + System.Environment.NewLine;
            commands = commands + "mv *.csv *.csv Archive" + System.Environment.NewLine;
            commands = commands + "quit" + System.Environment.NewLine;

            // Create script file

            // Check if script file exists. If yes, delete it. 
            string ScriptFileToBeCreated = SFTPPath_sc + SFTP_sc;

            if (File.Exists(ScriptFileToBeCreated))
            {
                File.Delete(ScriptFileToBeCreated);
            }

            System.IO.File.WriteAllText(ScriptFileToBeCreated, commands);

            // Create Bat file -command line to run psftp in this format: psftp.exe sftp.server.domain -l username -p password -b script_file_path
            string BatchCommands = "echo off" + System.Environment.NewLine;
            BatchCommands = BatchCommands + SFTPPath_exe.Substring(0,2) + System.Environment.NewLine;
            BatchCommands = BatchCommands + "cd " + SFTPPath_exe  + System.Environment.NewLine;

            string PuttyCommandLine = "CALL " + SFTP_exe + " " + SFTPServer + " -l " + SFTPUserName + " ";

            if (SFTP_cert == "")
            {
                PuttyCommandLine = PuttyCommandLine + " -pw " + SFTPUserPassword;
            }
            else
            {
                PuttyCommandLine = PuttyCommandLine + " -i " + SFTPPath_cert + SFTP_cert;
            }

            PuttyCommandLine = PuttyCommandLine + " -b " + SFTPPath_sc + SFTP_sc + " -batch" + System.Environment.NewLine;

            BatchCommands = BatchCommands + PuttyCommandLine;

            // Check if batch file exists. If yes, delete it. 
            string BatFileToBeCreated = SFTPPath_bat + SFTP_bat;

            if (File.Exists(BatFileToBeCreated))
            {
                File.Delete(BatFileToBeCreated);
            }

            System.IO.File.WriteAllText(BatFileToBeCreated, BatchCommands);

            // Run  batch file
            //int ExitCode;
            ProcessStartInfo ProcessInfo;
            Process process;

            ProcessInfo = new ProcessStartInfo(BatFileToBeCreated);
            ProcessInfo.CreateNoWindow = true;
            ProcessInfo.UseShellExecute = false;
            ProcessInfo.WorkingDirectory = SFTPPath_bat;
            // *** Redirect the output ***
            ProcessInfo.RedirectStandardError = true;
            ProcessInfo.RedirectStandardOutput = true;

            //process = Process.Start(ProcessInfo);
            //process.WaitForExit();

            //***Read the streams ***
            //string output = process.StandardOutput.ReadToEnd();
            //string error = process.StandardError.ReadToEnd();

            //ExitCode = process.ExitCode;

            //if (ExitCode != 0)
            //{
            //    string ErrorMessage = "<b>Error Code of " + ExitCode.ToString() + " - " + (String.IsNullOrEmpty(error) ? "(none)" : error) + "</b><br><br>";
            //    ErrorMessage = ErrorMessage + "            SFTP Server: <b>" + SFTPServer + "</b><br>";
            //    ErrorMessage = ErrorMessage + "          SFTP UserName: <b>" + SFTPUserName + "</b><br>";

            //    if (SFTP_cert == "")
            //    {
            //        ErrorMessage = ErrorMessage + "  SFTP UserPassword: <b>" + SFTPUserPassword + "</b><br>";
            //    }
            //    else
            //    {
            //        ErrorMessage = ErrorMessage + "SFTP Cert Path Name: <b>" + SFTPPath_cert + SFTP_cert + "</b><br>";
            //    }

            //    Dts.Variables["SFTPErrorMessage"].Value = ErrorMessage;
            //}

            //process.Close();

            //Cleanup delete created files
            //File.Delete(ScriptFileToBeCreated);
            //File.Delete(BatFileToBeCreated);


            Dts.TaskResult = (int)ScriptResults.Success;
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
