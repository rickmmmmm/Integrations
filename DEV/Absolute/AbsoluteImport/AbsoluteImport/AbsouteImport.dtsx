        #endregion


		public void Main()
		{
            //TargetFileName,User::TargetFilePath
            bool Debug = Convert.ToBoolean(Dts.Variables["User::Debug"].Value);
            string CustomerCode = Dts.Variables["User::CustomerCode"].Value.ToString();
            string RootPath = Dts.Variables["User::RootPath"].Value.ToString();
            string ExportPath =Dts.Variables["User::TargetFilePath"].Value.ToString();
            string ArchivePath = Dts.Variables["User::ArchiveFilePath"].Value.ToString();
            string ExportFileName = Dts.Variables["User::TargetFileName"].Value.ToString();
            Int32 FileRetentionDays = Convert.ToInt32(Dts.Variables["User::ArchiveKeepDays"].Value) * -1;
            string FileToBeMoved = "";

            //Check to make sure "\" is at the end of Archive path
            if (ArchivePath.Substring(ArchivePath.Length - 1, 1) != "\\")
            {
                ArchivePath = ArchivePath + "\\";
                Dts.Variables["User::ArchiveFilePath"].Value = ArchivePath;
            }

            if (Convert.ToBoolean(Dts.Variables["User::Debug"].Value) == true)
            {
                MessageBox.Show("ArchiveFilePath: " + ArchivePath);
                MessageBox.Show("RootPath: " + RootPath);
                MessageBox.Show("ExportPath: " + ExportPath);
                MessageBox.Show("ExportFileName: " + ExportFileName);
            }

            // Does Archive path exists?
            bool DirectoryExists = Directory.Exists(RootPath +  ArchivePath);

            // Create Directory if it does not exists
            if (!DirectoryExists)
            {
                Directory.CreateDirectory(RootPath + ArchivePath);
            }

            if (Debug == true)
            {
                MessageBox.Show("File Path and Name: " + ExportPath + ExportFileName);
            }

            string ToGetFileSize = ExportPath + ExportFileName;
            Dts.Variables["User::ExportFileSize"].Value = new FileInfo(ToGetFileSize).Length.ToString();
                        
            if (Debug == true)
            {
                MessageBox.Show("FileSize Varable: " + Dts.Variables["User::ExportFileSize"].Value.ToString() );
            }

            // Move File(s) to Arhive Folder
            string[] Archivefiles = Directory.GetFiles(ExportPath);
            foreach (string file in Archivefiles)
            {
                if (Debug == true)
                {
                    MessageBox.Show("*** Sending to Archive Folder ***\r\n\r\n" +
                        "File From: (" + file + ")\r\n\r\n" +
                        "File To: (" + file.Replace(ExportPath, RootPath + ArchivePath) + ")\r\n\r\n" +
                      "File Size: (" + file.Length.ToString() + ")\r\n");
                }

                FileToBeMoved = file.Replace(ExportPath, RootPath + ArchivePath);

                if (File.Exists(FileToBeMoved))
                {
                    File.Delete(FileToBeMoved);
                }

                File.Move(file, file.Replace(ExportPath, RootPath + ArchivePath));
            }

           
           // Archive folder cleanup of old filesf
            string[] Cleanupfiles = Directory.GetFiles(RootPath + ArchivePath);
            foreach (string file in Cleanupfiles)
            {
                FileInfo fi = new FileInfo(file);
                if (File.GetCreationTime(fi.ToString()) < DateTime.Now.AddDays(FileRetentionDays))
                {
                    if (Debug == true)
                    {
                        MessageBox.Show("*** Cleaning up Archive Folder ***\r\n" +
                            "Deleting File Name: (" + file + ")\r\n" +
                            "      To Directory: (" + RootPath + CustomerCode + "\\" + ExportPath + ArchivePath + ")\r\n");
                    }

                    fi.Delete();
                }
                    
            }

            Dts.TaskResult = (int)ScriptResults.Success;
		}

