      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Asset Export\Room Assets"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Room Assets"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT H.DepartmentName,A.Tag,REPLACE(A.Serial,'"','') AS Serial,C.ItemNumber AS ProductNumber,F.ManufacturerName,REPLACE(C.ItemName,'"','') AS ProductName,D.ItemTypeName AS ProductType,
CASE WHEN A.ParentInventoryUID IS NULL THEN 'Standalone' WHEN A.ParentInventoryUID = 0 THEN 'Parent' ELSE 'Component' END AS AssetType,
E.SiteID,E.SiteName,B.RoomNumber AS LocationID, B.RoomDescription AS Location,G.statusDesc AS Status,
CAST(N.LastScanDate AS DATE) AS LastScanDate,N.LastScanBy AS LastScanBy,
REPLACE(C.ModelNumber,'"','') AS ModelNumber,M.VendorName,isNull(FS.FundingSource, '') as FundingSource

FROM tblTechInventory AS A WITH (NOLOCK)

INNER JOIN tblUnvRooms AS B WITH (NOLOCK)
ON A.EntityUID = B.RoomUID

INNER JOIN tblTechItems AS C WITH (NOLOCK)
ON A.ItemUID= C.ItemUID

INNER JOIN tblTechItemTypes AS D WITH (NOLOCK)
ON C.ItemTypeUID = D.ItemTypeUID

INNER JOIN tblTechSites AS E WITH (NOLOCK)
ON A.SiteUID = E.SiteUID

INNER JOIN tblUnvManufacturers AS F WITH (NOLOCK)
ON C.ManufacturerUID = F.ManufacturerUID

INNER JOIN tblStatus AS G WITH (NOLOCK)
ON A.StatusUID = G.statusID 

INNER JOIN tblTechDepartments AS H WITH (NOLOCK)
ON A.TechDepartmentUID = H.TechDepartmentUID

LEFT JOIN tblTechPurchaseInventory AS I WITH (NOLOCK)
ON A.InventoryUID = I.InventoryUID

LEFT JOIN tblTechPurchaseItemShipments AS J WITH (NOLOCK)
ON I.PurchaseItemShipmentUID = J.PurchaseItemShipmentUID

LEFT JOIN tblTechPurchaseItemDetails AS K WITH (NOLOCK)
ON J.PurchaseItemDetailUID = K.PurchaseItemDetailUID

LEFT JOIN tblTechPurchases AS L WITH (NOLOCK)
ON K.PurchaseUID = L.PurchaseUID

LEFT JOIN tblVendor AS M WITH (NOLOCK)
ON L.VendorUID = M.VendorID

INNER JOIN TmpStanHistory AS N WITH (NOLOCK)
ON A.InventoryUID = N.InventoryUID

left JOIN tblFundingSources AS FS WITH (NOLOCK)
ON FS.FundingSourceUID = A.FundingSourceUID

WHERE A.EntityTypeUID = 2
AND ISNUMERIC(E.SiteID) = 1
AND CAST(E.SiteID AS INT) BETWEEN 1 AND 550</property>
