          SQLTask:SqlStatementSource="&#xA;---- Last ScanBy Information&#xA;IF EXISTS ( SELECT Name FROM sys.objects WHERE object_id = OBJECT_ID(N'dbo.TmpStanHistory'))&#xA;BEGIN &#xA; DROP TABLE TmpStanHistory&#xA;END&#xA;&#xA;SELECT InventoryUID, CreatedDate AS LastScanDate,LoginName AS LastScanBy&#xA;&#xA;INTO TmpStanHistory&#xA;&#xA;FROM&#xA;(&#xA;SELECT A.InventoryUID,CAST(B.CreatedDate AS DATE) AS CreatedDate,U.RealName AS LoginName,ROW_NUMBER() OVER(PARTITION BY A.InventoryUID ORDER BY B.InventoryHistoryUID DESC) AS RowNumber&#xA;&#xA;FROM tblTechInventory AS A WITH (NOLOCK)&#xA;&#xA;INNER JOIN tblTechInventoryHistory AS B WITH (NOLOCK)&#xA;ON A.InventoryUID = B.InventoryUID&#xA;&#xA;INNER JOIN tblUser AS U WITH (NOLOCK)&#xA;ON B.CreatedByUserID = U.UserID&#xA;&#xA;&#xA;) AS History&#xA;WHERE RowNumber = 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
