<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{41E81A0E-AE62-4F3D-94A5-386D9B158A25}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="&#xA;--/*&#xA;-- Update Purchase Price&#xA;&#xA;--select distinct detail.PurchaseUID, PurchasePrice, DISDPricePerUnit&#xA;update tblTechPurchaseItemDetails set PurchasePrice = DISDPricePerUnit &#xA;from _POUPdates poudate&#xA;join tblTechPurchaseItemDetails detail on poudate.PurchaseItemDetailUID = detail.PurchaseItemDetailUID&#xA;where updated = 1&#xA;and PurchasePrice &lt;&gt; cast(poudate.DISDPricePerUnit as decimal(18,2)) &#xA;GO&#xA;&#xA;update tblTechInventory&#xA;set PurchasePrice = poud.DISDPricePerUnit&#xA;from tblTechInventory tti&#xA;join _POUpdates poud on tti.InventoryUID = poud.InventoryUID&#xA;where Updated = 1&#xA;and tti.PurchasePrice &lt;&gt; cast(poud.DISDPricePerUnit as decimal(18,2)) &#xA;GO&#xA;&#xA;--*/&#xA;&#xA;--/*&#xA;-- Update Purchase Date&#xA;&#xA;--select  detail.PurchaseUID, CreationDate, PurchaseDate &#xA;--count(distinct ttp.PurchaseUID )&#xA;update tblTechPurchases set PurchaseDate = CreationDate &#xA;from _POUPdates poudate&#xA;join tblTechPurchaseItemDetails detail on poudate.PurchaseItemDetailUID = detail.PurchaseItemDetailUID&#xA;join tblTechPurchases ttp on detail.PurchaseUID = ttp.PurchaseUID&#xA;where updated = 1&#xA;and CreationDate &lt;&gt; PurchaseDate &#xA;GO&#xA;&#xA;update tblTechInventory&#xA;set PurchaseDate = poud.CreationDate&#xA;from tblTechInventory tti&#xA;join _POUpdates poud on tti.InventoryUID = poud.InventoryUID&#xA;where Updated = 1&#xA;and tti.PurchaseDate &lt;&gt; poud.CreationDate&#xA;GO&#xA;&#xA;&#xA;--group by detail.PurchaseUID having count(distinct ttp.PurchaseUID) &gt; 1&#xA;--*/&#xA;&#xA;-- Update Account Code&#xA;--select distinct detail.PurchaseItemDetailUID --, Acct, AccountCode&#xA;--,* &#xA;update tblTechPurchaseItemDetails set AccountCode = Acct &#xA;from _POUPdates poudate&#xA;join tblTechPurchaseItemDetails detail on poudate.PurchaseItemDetailUID = detail.PurchaseItemDetailUID&#xA;where updated = 1&#xA;and ltrim(rtrim(Acct)) &lt;&gt; ltrim(rtrim(AccountCode))&#xA;GO&#xA;&#xA;--FundingSource&#xA;--select distinct detail.PurchaseItemDetailUID, FundingSourceID, FundingSourceUID&#xA;--,* &#xA;update tblTechPurchaseItemDetails set FundingSourceUID = FundingSourceID &#xA;from _POUPdates poudate&#xA;join tblTechPurchaseItemDetails detail on poudate.PurchaseItemDetailUID = detail.PurchaseItemDetailUID&#xA;where updated = 1&#xA;and ltrim(rtrim(FundingSourceID)) &lt;&gt; ltrim(rtrim(FundingSourceUID))&#xA;GO&#xA;&#xA;update tblTechInventory&#xA;set FundingSourceUID = poud.FundingSourceID&#xA;from tblTechInventory tti&#xA;join _POUpdates poud on tti.InventoryUID = poud.InventoryUID&#xA;where Updated = 1&#xA;and tti.FundingSourceUID &lt;&gt; poud.FundingSourceID&#xA;GO&#xA;&#xA;---Vendor&#xA;update tblTechPurchases&#xA;set VendorUID = poud.VendorUID&#xA;from tblTechPurchases ttp&#xA;join _POUpdates poud on ttp.PurchaseUID = poud.NewPurchaseUID&#xA;where Updated = 1&#xA;and ttp.VendorUID &lt;&gt; poud.VendorUID&#xA;GO&#xA;&#xA;/*&#xA; select os.PurchaseItemDetailUID, os.FundingSourceID , os.* -- os.PurchaseItemDetailUID, os.DISDPricePerUnit&#xA; from _POUpdates os&#xA; join (&#xA;	 select distinct PurchaseItemDetailUID, FundingSourceID&#xA;	 from _POUpdates poupdate &#xA;	 where updated = 1&#xA;	 and PurchaseItemDetailUID is not null&#xA;	) t on os.PurchaseItemDetailUID = t.PurchaseItemDetailUID &#xA;		&#xA;where updated = 1&#xA;and os.FundingSourceID &lt;&gt; t.FundingSourceID&#xA; --group by os.PurchaseItemDetailUID, os.CreationDate having count(*) &gt; 1&#xA; order by os.PurchaseItemDetailUID, os.FundingSourceID&#xA;&#xA;select distinct detail.* &#xA;--detail.PurchaseItemDetailUID, OldPurchaseUID, NewPurchaseUID, ttp.PurchaseUID&#xA;--,* &#xA;--update tblTechPurchaseItemDetails set FundingSourceUID = FundingSourceID &#xA;from _POUPdates poudate&#xA;join tblTechPurchaseItemDetails detail on poudate.PurchaseItemDetailUID = detail.PurchaseItemDetailUID&#xA;where updated = 1&#xA;and FundingSourceID &lt;&gt; FundingSourceUID&#xA;*/&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
