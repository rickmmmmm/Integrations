<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="--select ottpur.ordernumber, tag, serial, ItemName, ItemDescription, ItemTypeName, ottpur.PurchaseDate, otti.PurchaseDate,&#xA;--ottid.purchasePrice, otti.purchasePrice, ottid.QuantityOrdered, QuantityReceived,&#xA;--ottid.AccountCode,FundingSource, ottpur.Notes&#xA;&#xA;select PurchaseItemDetailUID, AccountCode, FundingSourceUID,  QuantityOrdered, purchasePrice &#xA;from tblTechPurchaseItemDetails ttid&#xA;--join tblTechPurchases ttpur on ttpur.Purchaseuid = ttid.Purchaseuid&#xA;where PurchaseItemDetailUID in (&#xA;&#xA;select PurchaseItemDetailUID&#xA;from (&#xA;&#xA;select PurchaseItemDetailUID, acct, fund, qtyOrdered, DISDPricePerUnit, CreationDate&#xA;from _POUpdates&#xA;where updated = 1&#xA;and PurchaseItemDetailUID is not null&#xA;group by PurchaseItemDetailUID, acct, fund, qtyOrdered, DISDPricePerUnit, CreationDate&#xA;) t&#xA;group by  PurchaseItemDetailUID having  count(*) &gt; 1&#xA;)&#xA;order by PurchaseItemDetailUID&#xA;&#xA;/*&#xA;&#xA;--OLD COUNTS&#xA;select distinct PurchaseItemDetailUID, acct, fund, qtyOrdered, DISDPricePerUnit&#xA;from _POUpdates&#xA;where updated = 1&#xA;and PurchaseItemDetailUID in (19295,19296)&#xA;--in (19295,19296,40561,40966,41082,41085)&#xA;order by PurchaseItemDetailUID&#xA;&#xA;--select * from _POUpdates where updated = 1and PurchaseItemDetailUID is null&#xA;&#xA;--select distinct acct from _POUpdates where updated = 1 and PurchaseItemDetailUID = 40966&#xA;--19295 is quantity ordered is 5/10 - s/b 10	&#xA;--19296 is quantity ordered is 5/10 - s/b 10	&#xA;--40561 is quantity ordered is 6/14- s/b 11; multiple accts	&#xA;--40966 is quantity ordered is 2/1 - s/b 3; multiple accts	&#xA;--41082 quantity ordered is 1/5; sb/4; multiple accts	&#xA;--41085 quantity ordered is 6/2; sb/1/6; mult fundacct, multiple accts	&#xA;&#xA;--END OLD COUNTS&#xA;*/&#xA;&#xA;--Checking the whole table&#xA;/*&#xA;select otti.* &#xA;from tblTechPurchaseItemDetails ottid &#xA;join tblTechPurchases tpur on ottid.PurchaseUID = tpur.PurchaseUID&#xA;join tblTechPurchaseItemShipments otship on ottid.PurchaseItemDetailUID = otship.PurchaseItemDetailUID&#xA;join tblTechPurchaseInventory ottpi on otship.PurchaseItemShipmentUID = ottpi.PurchaseItemShipmentUID&#xA;join tblTechInventory otti on  ottpi.InventoryUID = otti.InventoryUID&#xA;where ottid.PurchaseUID = 1988&#xA;*/&#xA;&#xA;--Check for orphan shipment records&#xA;select *&#xA;--delete  &#xA;FROM [dbo].[tblTechPurchaseItemShipments]  &#xA;where PurchaseItemDetailUID in (select PurchaseItemDetailUID from _POUpdates where updated = 1)&#xA;and PurchaseItemShipmentUID not in (select PurchaseItemShipmentUID from tblTechPurchaseInventory)&#xA;------------------------------------------------------------------------------------------------------------&#xA;&#xA;&#xA;--Correct quantities where needed - where number of tags is greater than quantity ordered.&#xA;--NOTE:  There are orphan PurchaseItemDetail records that need to be cleaned up manually&#xA;--			If there are records in the extraDetails column, they will need to be considered individually before updating the quantities&#xA;&#xA;select ottid.PurchaseItemDetailUID, ottid.PurchaseUID, OrderNumber, ottid.quantityOrdered, quantityReceived, invCount, QuantityShipped&#xA;, ottid.ItemUID, ottid.FundingSourceUID&#xA;, (select count(PurchaseItemDetailUID) from tblTechPurchaseItemDetails ittid&#xA;	where ittid.PurchaseUID = ottid.PurchaseUID &#xA;	and ittid.ItemUID = ottid.ItemUID&#xA;	and ittid.FundingSourceUID = ottid.FundingSourceUID&#xA;	and ittid.PurchaseItemDetailUID &lt;&gt; ottid.PurchaseItemDetailUID) as extraDetails&#xA;update tblTechPurchaseItemDetails set quantityOrdered = t.InvCount, quantityReceived = t.InvCount&#xA;from tblTechPurchaseItemDetails ottid &#xA;join tblTechPurchases tpur on ottid.PurchaseUID = tpur.PurchaseUID&#xA;join (select PurchaseItemDetailUID, QuantityShipped, count(otti.InventoryUID) as invCount&#xA;		from tblTechPurchaseItemShipments otship &#xA;		join tblTechPurchaseInventory ottpi on otship.PurchaseItemShipmentUID = ottpi.PurchaseItemShipmentUID&#xA;		join tblTechInventory otti on  ottpi.InventoryUID = otti.InventoryUID&#xA;		group by PurchaseItemDetailUID, QuantityShipped&#xA;		) t&#xA;&#xA;		on ottid.PurchaseItemDetailUID = t.PurchaseItemDetailUID&#xA;where ottid.PurchaseItemDetailUID  in (select PurchaseItemDetailUID from _POUpdates where updated = 1)&#xA;and ottid.quantityOrdered &lt; t.invCount&#xA;--and tpur.purchaseuid = 1988&#xA;&#xA;&#xA;/*&#xA;--Lookup/Cleanup query for extra detail records&#xA;&#xA;select * &#xA;from tblTechPurchaseItemDetails&#xA;where PurchaseUID = 13519&#xA;and ItemUID = 654&#xA;and FundingSourceUID = 18&#xA;&#xA;--delete tblTechPurchaseItemDetails where PurchaseItemDetailUID = 41568&#xA;&#xA;update tblTechPurchaseItemDetails&#xA;set quantityOrdered = 2, quantityReceived= 2&#xA;where PurchaseItemDetailUID in (22147)&#xA;&#xA;update tblTechPurchaseItemDetails&#xA;set quantityOrdered = 1, quantityReceived= 1&#xA;where PurchaseItemDetailUID in (40530)&#xA;&#xA;&#xA;update [tblTechPurchaseItemShipments] set QuantityShipped = 45&#xA;where PurchaseItemShipmentUID in (24920,24927,24939,24955,24964,24989)&#xA;&#xA;update [tblTechPurchaseItemShipments] set QuantityShipped = 15&#xA;where PurchaseItemShipmentUID = 24998&#xA;*/&#xA;&#xA;-----------------------------------------------------------------------------------------------------&#xA;--Close statuses on details where PO is closed and all items received&#xA;--32 is open and 33 is closed&#xA;&#xA;select ottid.PurchaseItemDetailUID, ttpur.purchaseuid, ttpur.StatusUID, ottid.StatusUID, invCount&#xA;--update tblTechPurchaseItemDetails set StatusUID = ttpur.StatusUID&#xA;from tblTechPurchaseItemDetails ottid &#xA;join tblTechPurchases ttpur on ttpur.PurchaseUID = ottid.PurchaseUID&#xA;&#xA;join (select PurchaseItemDetailUID, count(*) as invCount&#xA;		from tblTechPurchaseItemShipments otship &#xA;		join tblTechPurchaseInventory ottpi on otship.PurchaseItemShipmentUID = ottpi.PurchaseItemShipmentUID&#xA;		join tblTechInventory otti on  ottpi.InventoryUID = otti.InventoryUID&#xA;		group by PurchaseItemDetailUID&#xA;		) t&#xA;&#xA;		on ottid.PurchaseItemDetailUID = t.PurchaseItemDetailUID&#xA;&#xA;where ottid.PurchaseItemDetailUID  in (select PurchaseItemDetailUID from _POUpdates where updated = 1)&#xA;and ottid.quantityReceived = t.invCount&#xA;and ttpur.StatusUID = 33 &#xA;and ttpur.StatusUID &lt;&gt; ottid.StatusUID&#xA;Order by OrderNumber&#xA;&#xA;&#xA;&#xA;--Multiple Item detail rows mean mismatch on quantities; this query resets ordered/received to &#xA;--what was shipped and tagged&#xA;select ttpur.purchaseuid, ottid.PurchaseItemDetailUID, OrderNumber, quantityOrdered, quantityReceived, invCount, tshipOrdered&#xA;--update tblTechPurchaseItemDetails set quantityOrdered = invCount, quantityReceived = invCount&#xA;from tblTechPurchaseItemDetails ottid &#xA;join tblTechPurchases ttpur on ttpur.PurchaseUID = ottid.PurchaseUID&#xA;&#xA;join (select PurchaseItemDetailUID, count(*) as invCount&#xA;		from tblTechPurchaseItemShipments otship &#xA;		join tblTechPurchaseInventory ottpi on otship.PurchaseItemShipmentUID = ottpi.PurchaseItemShipmentUID&#xA;		join tblTechInventory otti on  ottpi.InventoryUID = otti.InventoryUID&#xA;		group by PurchaseItemDetailUID&#xA;		) t&#xA;&#xA;		on ottid.PurchaseItemDetailUID = t.PurchaseItemDetailUID&#xA;&#xA;join (select PurchaseItemDetailUID, sum(QuantityShipped) as tshipOrdered&#xA;		from tblTechPurchaseItemShipments otship&#xA;		group by PurchaseItemDetailUID &#xA;		) tship&#xA;		on ottid.PurchaseItemDetailUID = tship.PurchaseItemDetailUID&#xA;where ottid.PurchaseItemDetailUID  in (select PurchaseItemDetailUID from _POUpdates where updated = 1)&#xA;and quantityOrdered = quantityReceived&#xA;and ottid.quantityReceived &lt;&gt; t.invCount&#xA;and ttpur.StatusUID = 33 &#xA;and ottid.StatusUID = 33 &#xA;Order by OrderNumber, PurchaseItemDetailUID&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
