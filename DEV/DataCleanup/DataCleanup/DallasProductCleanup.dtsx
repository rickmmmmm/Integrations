<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{41E81A0E-AE62-4F3D-94A5-386D9B158A25}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="&#xA;/*&#xA;UPDATE _POUpdates SET PurchaseItemShipmentUID = null&#xA;*/&#xA;--Update shipping sites&#xA;update tblTechPurchaseItemShipments set ShippedToSiteUID = t.SiteUID&#xA;from tblTechPurchaseItemShipments oship&#xA;join &#xA;(&#xA;select distinct ship.PurchaseItemShipmentUID, SiteUID&#xA;FROM _POUpdates AS poud&#xA;JOIN tblTechPurchaseItemShipments ship ON poud.PurchaseItemDetailUID = ship.PurchaseItemDetailUID&#xA;AND poud.SiteUID &lt;&gt; ship.ShippedToSiteUID&#xA;WHERE Updated = 1 &#xA;) t&#xA;on oship.PurchaseItemShipmentUID = t.PurchaseItemShipmentUID&#xA;GO&#xA;&#xA;UPDATE _POUpdates&#xA;SET PurchaseItemShipmentUID = ship.PurchaseItemShipmentUID&#xA;--select * &#xA;FROM _POUpdates AS Purchases&#xA;JOIN tblTechPurchaseItemShipments ship ON Purchases.PurchaseItemDetailUID = ship.PurchaseItemDetailUID&#xA;AND Purchases.SiteUID = ship.ShippedToSiteUID&#xA;WHERE Updated = 1 &#xA;and (Purchases.PurchaseItemShipmentUID IS NULL&#xA;	OR Purchases.PurchaseItemShipmentUID &lt;&gt; ship.PurchaseItemShipmentUID)&#xA;GO&#xA;&#xA;&#xA;--/*&#xA;&#xA;INSERT INTO tblTechPurchaseItemShipments (PurchaseItemDetailUID, ShippedToSiteUID, TicketNumber, QuantityShipped,&#xA;TicketedByUserID, TicketedDate, StatusUID, CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;SELECT PurchaseItemDetailUID, SiteUID, NULL, COUNT(*),&#xA;NULL, NULL, 33, 0, GETDATE(), 0, GETDATE()&#xA;FROM _POUpdates &#xA;WHERE Updated = 1&#xA;and InventoryUID is not null&#xA;and SiteUID is not null&#xA;and NewPurchaseUID is not null&#xA;and PurchaseItemDetailUID is not null&#xA;and PurchaseItemShipmentUID IS NULL&#xA;GROUP BY PurchaseItemDetailUID, SiteUID&#xA;&#xA;--*/&#xA;&#xA;UPDATE _POUpdates&#xA;SET PurchaseItemShipmentUID = ship.PurchaseItemShipmentUID&#xA;--select * &#xA;FROM _POUpdates AS Purchases&#xA;JOIN tblTechPurchaseItemShipments ship ON Purchases.PurchaseItemDetailUID = ship.PurchaseItemDetailUID&#xA;AND Purchases.SiteUID = ship.ShippedToSiteUID&#xA;WHERE Updated = 1 &#xA;and (Purchases.PurchaseItemShipmentUID IS NULL&#xA;	OR Purchases.PurchaseItemShipmentUID &lt;&gt; ship.PurchaseItemShipmentUID)&#xA;&#xA;&#xA;--select * &#xA;--FROM _POUpdates AS Purchases&#xA;--WHERE Updated = 1 &#xA;--and PurchaseItemShipmentUID IS NULL&#xA;&#xA;--select * &#xA;update tblTechPurchaseItemShipments set QuantityShipped = detail.QuantityShipped&#xA;from tblTechPurchaseItemShipments ship&#xA;join (&#xA;	select PurchaseItemDetailUID, SiteUID, COUNT(*) as QuantityShipped&#xA;	FROM _POUpdates &#xA;	WHERE Updated = 1&#xA;	and PurchaseItemDetailUID is not null&#xA;	GROUP BY PurchaseItemDetailUID, SiteUID&#xA;	) detail&#xA;on ship.PurchaseItemDetailUID = detail.PurchaseItemDetailUID&#xA;	and ship.ShippedToSiteUID = detail.SiteUID&#xA;	and ship.QuantityShipped &lt;&gt; detail.QuantityShipped&#xA;&#xA;/*&#xA;--DELETE old&#xA;select old.*&#xA;FROM tblTechPurchaseItemShipments AS old&#xA;JOIN tblTechPurchaseItemDetails ON old.PurchaseItemDetailUID = tblTechPurchaseItemDetails.PurchaseItemDetailUID&#xA;JOIN tblTechPurchases ON tblTechPurchaseItemDetails.PurchaseUID = tblTechPurchases.PurchaseUID&#xA;LEFT JOIN (SELECT PurchaseItemShipmentUID FROM _ETL_Inventory&#xA;			GROUP BY PurchaseItemShipmentUID) AS Inv &#xA;		ON old.PurchaseItemShipmentUID = Inv.PurchaseItemShipmentUID&#xA;LEFT JOIN (SELECT PurchaseItemShipmentUID FROM _ETL_Purchases&#xA;			GROUP BY PurchaseItemShipmentUID) AS Purch &#xA;		ON old.PurchaseItemShipmentUID = Purch.PurchaseItemShipmentUID&#xA;LEFT JOIN (SELECT PurchaseItemShipmentUID FROM tblTechPurchaseInventory&#xA;			GROUP BY PurchaseItemShipmentUID) AS Tags &#xA;		ON old.PurchaseItemShipmentUID = Tags.PurchaseItemShipmentUID&#xA;WHERE 1=1 &#xA;AND Inv.PurchaseItemShipmentUID IS NULL&#xA;AND Purch.PurchaseItemShipmentUID IS NULL&#xA;AND Tags.PurchaseItemShipmentUID IS NULL&#xA;*/&#xA;&#xA;/*&#xA;&#xA;select *&#xA;FROM _POUpdates AS Purchases&#xA;WHERE Updated = 1&#xA;--and PurchaseItemShipmentUID is null&#xA;and PurchaseItemShipmentUID is not null&#xA;and PurchaseItemShipmentUID not in (select PurchaseItemShipmentUID from tblTechPurchaseItemShipments)&#xA;&#xA;*/&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
