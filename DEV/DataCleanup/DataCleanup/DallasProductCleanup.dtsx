<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{41E81A0E-AE62-4F3D-94A5-386D9B158A25}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="&#xA;--select * &#xA;update _POUpdates set PurchaseInventoryUID = tpi.PurchaseInventoryUID&#xA;from _POUpdates poupdate&#xA;join tblTechPurchaseInventory tpi on poupdate.PurchaseItemShipmentUID = tpi.PurchaseItemShipmentUID &#xA;	and poupdate.InventoryUID = tpi.InventoryUID &#xA;where updated = 1&#xA;and poupdate.PurchaseItemShipmentUID is not null&#xA;and poupdate.InventoryUID is not null&#xA;GO&#xA;&#xA;update tblTechPurchaseInventory set PurchaseItemShipmentUID = poupdate.PurchaseItemShipmentUID &#xA;--select *&#xA;from _POUpdates poupdate&#xA;join tblTechPurchaseInventory tpi on poupdate.InventoryUID = tpi.InventoryUID &#xA;where updated = 1&#xA;and poupdate.PurchaseItemShipmentUID is not null&#xA;and poupdate.InventoryUID is not null&#xA;and poupdate.PurchaseItemShipmentUID &lt;&gt; tpi.PurchaseItemShipmentUID &#xA;GO&#xA;&#xA;INSERT INTO tblTechPurchaseInventory (InventoryUID, PurchaseItemShipmentUID,&#xA;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;SELECT poupdate.InventoryUID, poupdate.PurchaseItemShipmentUID,&#xA;0, GETDATE(), 0, GETDATE()&#xA;FROM _POUpdates AS poupdate&#xA;WHERE poupdate.InventoryUID IS NOT NULL&#xA;AND poupdate.SiteUID IS NOT NULL&#xA;AND poupdate.NewPurchaseUID IS NOT NULL&#xA;AND poupdate.PurchaseItemDetailUID IS NOT NULL&#xA;AND poupdate.PurchaseItemShipmentUID IS NOT NULL&#xA;AND poupdate.PurchaseInventoryUID IS NULL&#xA;and poupdate.InventoryUID not in (select InventoryUID from tblTechPurchaseInventory)&#xA;GO&#xA;&#xA;&#xA;update _POUpdates&#xA;set PurchaseInventoryUID = ttpi.PurchaseInventoryUID&#xA;from _POUpdates poud&#xA;join tblTechPurchaseInventory ttpi on ttpi.InventoryUID = poud.InventoryUID&#xA;	and ttpi.PurchaseItemShipmentUID = poud.PurchaseItemShipmentUID&#xA;where Updated = 1&#xA;and poud.PurchaseInventoryUID is null&#xA;GO&#xA;&#xA;/*&#xA;SELECT *&#xA;FROM _POUpdates AS poupdate&#xA;join tblTechPurchaseInventory ttip on poupdate.PurchaseInventoryUID = ttip.PurchaseInventoryUID &#xA;WHERE poupdate.InventoryUID IS NOT NULL&#xA;AND poupdate.SiteUID IS NOT NULL&#xA;AND poupdate.NewPurchaseUID IS NOT NULL&#xA;AND poupdate.PurchaseItemDetailUID IS NOT NULL&#xA;AND poupdate.PurchaseItemShipmentUID IS NOT NULL&#xA;AND poupdate.PurchaseInventoryUID IS NULL&#xA;and (&#xA;	poupdate.PurchaseItemShipmentUID &lt;&gt; ttip.PurchaseItemShipmentUID&#xA;	or&#xA;	poupdate.InventoryUID &lt;&gt; ttip.InventoryUID&#xA;	)&#xA;	&#xA;&#xA;*/&#xA;&#xA;DELETE old&#xA;--select *&#xA;FROM tblTechPurchaseInventory AS old&#xA;JOIN tblTechPurchaseItemShipments ON old.PurchaseItemShipmentUID = tblTechPurchaseItemShipments.PurchaseItemShipmentUID&#xA;JOIN tblTechPurchaseItemDetails ON tblTechPurchaseItemShipments.PurchaseItemDetailUID = tblTechPurchaseItemDetails.PurchaseItemDetailUID&#xA;JOIN tblTechPurchases ON tblTechPurchaseItemDetails.PurchaseUID = tblTechPurchases.PurchaseUID&#xA;LEFT JOIN _POUpdates AS Tags ON old.PurchaseInventoryUID = Tags.PurchaseInventoryUID&#xA;LEFT JOIN tblTechInventory ON old.InventoryUID = tblTechInventory.InventoryUID&#xA;AND old.PurchaseItemShipmentUID = Tags.PurchaseItemShipmentUID&#xA;WHERE tags.Updated = 1&#xA;and Tags.PurchaseInventoryUID IS NULL&#xA;AND tblTechInventory.InventoryUID IS NULL&#xA;GO&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
