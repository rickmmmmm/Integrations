<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{41E81A0E-AE62-4F3D-94A5-386D9B158A25}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="/*&#xA;--Update PurchaseItemDetailUID&#xA;&#xA;Get the PurchaseItemDetailUID bases on the OLD PurchaseUID&#xA;&#xA;update _POUpdates set  PurchaseItemDetailUID = null&#xA;&#xA;*/&#xA;--First, RESET the OldPurchaseUID based on the info from the Purchase Detail&#xA;--Here, we are assuming that the Old PO given from the spreadsheet my be incorrect&#xA;&#xA;--select *&#xA;update _POUpdates set  OldPurchaseUID = tpid.PurchaseUID&#xA;from _POUpdates poupdate --88741&#xA; join tblTechInventory tti on poupdate.InventoryUID = tti.InventoryUID  --88192&#xA; join tblTechPurchaseInventory ttpi on ttpi.InventoryUID = tti.InventoryUID --88094&#xA; join tblTechPurchaseItemShipments tpis on tpis.PurchaseItemShipmentUID = ttpi.PurchaseItemShipmentUID --88094&#xA; join tblTechPurchaseItemDetails tpid on tpid.PurchaseItemDetailUID = tpis.PurchaseItemDetailUID &#xA;where updated = 1&#xA;and OldPurchaseUID is not null&#xA;GO&#xA;&#xA;--Next, give the records the detail UID based on the OLD Purchase UID where it's the same as the new&#xA;&#xA;--select distinct --detail.* -- poudate.*&#xA;--tpid.PurchaseItemDetailUID, poupdate.PurchaseItemDetailUID, OldPurchaseUID, NewPurchaseUID, tpid.PurchaseUID&#xA;update _POUpdates set  PurchaseItemDetailUID = tpid.PurchaseItemDetailUID&#xA;from _POUpdates poupdate --88741&#xA; join tblTechInventory tti on poupdate.InventoryUID = tti.InventoryUID &#xA; join tblTechPurchaseInventory ttpi on ttpi.InventoryUID = tti.InventoryUID&#xA; join tblTechPurchaseItemShipments tpis on tpis.PurchaseItemShipmentUID = ttpi.PurchaseItemShipmentUID&#xA; join tblTechPurchaseItemDetails tpid on tpid.PurchaseItemDetailUID = tpis.PurchaseItemDetailUID&#xA;	and OldPurchaseUID = tpid.PurchaseUID&#xA; join tblFundingSources tfund  on tfund.FundingSourceUID = tpid.FundingSourceUID&#xA; where updated = 1&#xA;and OldPurchaseUID is not null&#xA;and NewPurchaseUID = OldPurchaseUID&#xA;and ltrim(rtrim(AccountCode)) = ltrim(rtrim(acct))&#xA;and ltrim(rtrim(tfund.FundingSource)) = ltrim(rtrim(fund))&#xA;GO&#xA;&#xA;--Update any records where detail matches new po&#xA;update _POUpdates set PurchaseItemDetailUID = tpid.PurchaseItemDetailUID&#xA;from _POUpdates poupdate --3339&#xA; join tblTechInventory tti on poupdate.InventoryUID = tti.InventoryUID  --3339&#xA; join tblTechPurchaseInventory ttpi on ttpi.InventoryUID = tti.InventoryUID --3215&#xA; join tblTechPurchaseItemShipments tpis on tpis.PurchaseItemShipmentUID = ttpi.PurchaseItemShipmentUID --3215&#xA; join tblTechPurchaseItemDetails tpid on tpid.PurchaseItemDetailUID = tpis.PurchaseItemDetailUID&#xA;	and NewPurchaseUID = tpid.PurchaseUID&#xA; join tblFundingSources tfund  on tfund.FundingSourceUID = tpid.FundingSourceUID&#xA;where updated = 1&#xA; and poupdate.PurchaseItemDetailUID is null&#xA; and ltrim(rtrim(AccountCode)) = ltrim(rtrim(acct))&#xA;and ltrim(rtrim(tfund.FundingSource)) = ltrim(rtrim(fund))&#xA;GO&#xA;&#xA;--These are problems with different Account Codes so delete and redo&#xA;update _POUpdates set  PurchaseItemDetailUID = null&#xA;where PurchaseItemDetailUID in (17809,19251,26589,26708,35809,35844)&#xA;&#xA;--These are problems with different Funding Source so delete and redo&#xA;update _POUpdates set  PurchaseItemDetailUID = null&#xA;where PurchaseItemDetailUID in (35844)&#xA;GO&#xA;&#xA; &#xA; --Add Detail records&#xA; INSERT INTO [tblTechPurchaseItemDetails]&#xA;           ([PurchaseUID], [ItemUID], [FundingSourceUID], [StatusUID], [SiteAddedSiteUID], [QuantityOrdered], [QuantityReceived],&#xA;           [PurchasePrice],[AccountCode],[TechDepartmentUID],[CreatedByUserID],[CreatedDate],&#xA;           [LastModifiedByUserID],[LastModifiedDate],[LineNumber])&#xA;           &#xA;&#xA;select NewPurchaseUID, ItemUID, FundingSourceID, NewStatusUID, 1, count(NewPurchaseUID), count(NewPurchaseUID), &#xA;	   DISDPricePerUnit,  Acct, TechDepartmentUID, 0, getdate(), 0, getdate(), 0     &#xA;from _POUPdates poudate &#xA;where PurchaseItemDetailUID is null &#xA;and updated = 1&#xA;group by NewPurchaseUID, ItemUID, FundingSourceID, NewStatusUID, DISDPricePerUnit,  Acct, TechDepartmentUID&#xA;order by NewPurchaseUID&#xA;GO &#xA;&#xA;--Insert the Detail UID from the recent insert&#xA;&#xA;--select * &#xA;update _POUPdates set PurchaseItemDetailUID = ttid.PurchaseItemDetailUID&#xA;from _POUPdates poupdate &#xA;join [tblTechPurchaseItemDetails] ttid&#xA;	on ttid.[PurchaseUID] = poupdate.NewPurchaseUID&#xA;	and ttid.[ItemUID] = poupdate.[ItemUID]&#xA;	and ttid.[FundingSourceUID] = poupdate.[FundingSourceID]&#xA;	and ttid.[StatusUID] = poupdate.NewStatusUID&#xA;	and cast(DISDPricePerUnit as decimal(18,2)) = PurchasePrice&#xA;	and ttid.[AccountCode] = poupdate.Acct&#xA;where poupdate.PurchaseItemDetailUID is null &#xA;and updated = 1&#xA;&#xA;/*&#xA;select * &#xA;from _POUPdates poupdate &#xA;where poupdate.PurchaseItemDetailUID is null &#xA;and updated = 1&#xA;*/&#xA;&#xA;&#xA;	&#xA;&#xA;&#xA;&#xA; &#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
