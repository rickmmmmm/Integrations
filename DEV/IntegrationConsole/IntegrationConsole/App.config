<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="WebServiceSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="WebServiceSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost/TIPWebIT/WebService.asmx"
                binding="basicHttpBinding" bindingConfiguration="WebServiceSoap"
                contract="TIPWebIT.WebServiceSoap" name="WebServiceSoap" />
            <endpoint address="http://localhost/TIPWebIT/WebService.asmx"
                binding="customBinding" bindingConfiguration="WebServiceSoap12"
                contract="TIPWebIT.WebServiceSoap" name="WebServiceSoap12" />
        </client>
    </system.serviceModel>
</configuration>