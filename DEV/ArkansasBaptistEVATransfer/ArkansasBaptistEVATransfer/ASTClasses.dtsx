                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT c.SchoolCode AS LocationGIvenID, cl.ClassID, 
		replace(replace(cl.Name, '"', ''), ',', '') as ClassShort, 
		replace(replace(c.Title, '"', ''), ',', '') as ClassLong, 
		(CASE WHEN c.SchoolCode = 'EL' THEN 0 
		WHEN c.SchoolCode = 'MSHS' AND cl.Pattern = '9' THEN '5' ELSE cl.Pattern END) AS PeriodCode, 
		(CASE when cl.Term1 = 1 and cl.Term2 = 1 and cl.Term3 = 1 and cl.Term4 = 1 THEN 'YR' 
			  WHEN (cl.Term1 = 1 or cl.Term2 = 1) and cl.Term3 = 0 and cl.Term4 = 0 THEN '1' 
			  WHEN (cl.Term3 = 1 or cl.Term4 = 1) and cl.Term1 = 0 and cl.Term2 = 0 THEN '2' END) 
		AS SemesterCode, 
		c.CourseID, 
		cl.Section AS SectionID, r.Room AS RoomShort
FROM    ASTLINKEDSERVER.ABS_AR.dbo.Courses AS c INNER JOIN
        ASTLINKEDSERVER.ABS_AR.dbo.vw_Class AS cl ON c.CourseID = cl.CourseID INNER JOIN
        ASTLINKEDSERVER.ABS_AR.dbo.Rooms AS r ON cl.RoomID = r.RoomID INNER JOIN
        ASTLINKEDSERVER.ABS_AR.dbo.SchoolYear AS sy ON cl.YearID = sy.YearID
WHERE        (sy.LastDay &gt; DATEADD(dd, 30, GETDATE()))</property>
