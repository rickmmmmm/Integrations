                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        SchoolCode AS LocationGivenID, 
StudentID, 
replace(replace(FirstName, '"', ''), ',', '') as FirstName, 
replace(replace(LastName, '"', ''), ',', '') as LastName, 
replace(replace(MiddleName, '"', ''), ',', '') as MiddleName, 
Gender, convert(varchar(10), Birthdate, 101) as Birthdate, GradeLevel, '' AS General1, '' AS General2
FROM            ASTLINKEDSERVER.ABS_AR.dbo.Students AS Students_1
WHERE        (SchoolCode IN ('EL', 'MSHS')) AND (WithdrawDate IS NULL) AND (GraduationDate IS NULL OR
                         GraduationDate IS NOT NULL AND GraduationDate &gt; GETDATE()) AND (Status IN ('Enrolled', 'Pre-Enrolled', '')) AND (StudentID IN
