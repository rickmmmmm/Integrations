                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        '' AS contactrowid, p.StudentID AS GivenID, s.SchoolCode AS LocationGivenID, 1 AS coretypeid, 
replace(replace((p.Salutation + p.FirstName + ' ' + p.LastName + p.Suffix), '"', ''), ',', '') AS ContactFullName, 
                         p.RelationshipToStu AS ContactRelation, 11 AS ContactOrder, p.Address, p.Address2, p.City, p.State, p.ZIP, p.CellPhone, p.WorkPhone, p.HomePhone, '' AS OtherPhone, p.Email, '' AS OtherContactBearings, 
                         '' AS Other
FROM            ASTLINKEDSERVER.ABS_AR.dbo.vw_ParentofstudentDetails AS p INNER JOIN
                         ASTLINKEDSERVER.ABS_AR.dbo.Students AS s ON p.StudentID = s.StudentID
WHERE        (s.SchoolCode IN ('EL', 'MSHS')) AND (s.WithdrawDate IS NULL) AND (s.GraduationDate IS NULL OR
                         s.GraduationDate IS NOT NULL AND s.GraduationDate &gt; GETDATE()) AND (s.Status IN ('Enrolled', 'Pre-Enrolled', '')) AND (s.StudentID IN
                             (SELECT        r.StudentID
                               FROM            ASTLINKEDSERVER.ABS_AR.dbo.vw_Roster AS r INNER JOIN
                                                         ASTLINKEDSERVER.ABS_AR.dbo.SchoolYear AS sy ON r.YearID = sy.YearID
                               WHERE        (sy.LastDay &gt; DATEADD(dd, 30, GETDATE()))))</property>
