              SQLTask:SqlStatementSource="INSERT INTO tblUnvChargePayments&#xA;(ApplicationUID, ChargeUID, PaymentSiteUID,&#xA;ChargeAmount, Void, [Description], Notes,&#xA;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;SELECT 2, P.ChargeUID, P.SiteUID,&#xA;P.PaymentAmount, 0, NULL, 'OPS Transaction ID: ' + P.TransactionID,&#xA;0, GETDATE(), 0, GETDATE()&#xA;FROM dbo._ETL_Payments P&#xA;WHERE P.ChargePaymentUID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET ChargePaymentUID = CP.ChargePaymentUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblUnvChargePayments CP ON CP.Notes = 'OPS Transaction ID: ' + P.TransactionID&#xA;WHERE P.ChargePaymentUID IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
