              SQLTask:SqlStatementSource="UPDATE dbo._ETL_StagingPayments&#xA;SET SiteID = ''&#xA;WHERE SiteID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET SiteID = RIGHT(SiteID, 3)&#xA;WHERE LEN(SiteID) = 4&#xA;AND LEFT(SiteID, 1) = '0'&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET TransactionID = ''&#xA;WHERE TransactionID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET ChargeAmount = ''&#xA;WHERE ChargeAmount IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET ChargeStatus = ''&#xA;WHERE ChargeStatus IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET ChargeType = ''&#xA;WHERE ChargeType IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET PaymentDate = ''&#xA;WHERE PaymentDate IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET PaymentDate = LEFT(PaymentDate, 23)&#xA;WHERE LEN(PaymentDate) &gt; 23&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET PaymentAmount = ''&#xA;WHERE PaymentAmount IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET StudentID = ''&#xA;WHERE StudentID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'SiteID not found in TIPWeb-IT'&#xA;ELSE Note + '; ' +  'SiteID not found in TIPWeb-IT' END&#xA;FROM _ETL_StagingPayments SP&#xA;LEFT JOIN tblTechSites S ON SP.SiteID = S.SiteID&#xA;WHERE S.SiteID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Missing Transaction ID'&#xA;ELSE Note + '; ' +  'Missing Transaction ID' END&#xA;WHERE TransactionID = ''&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Transaction already imported'&#xA;ELSE Note + '; ' +  'Transaction already imported' END&#xA;FROM dbo._ETL_StagingPayments SP&#xA;JOIN tblUnvChargePayments CP ON CP.Notes LIKE '%OPS Transaction ID: ' + SP.TransactionID + '%'&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid Charge Amount'&#xA;ELSE Note + '; ' +  'Invalid Charge Amount' END&#xA;WHERE ISNUMERIC(ChargeAmount) = 0&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Charge Type must be TechFeeFullYear or TechFeeSemester'&#xA;ELSE Note + '; ' +  'Charge Type must be TechFeeFullYear or TechFeeSemester' END&#xA;WHERE ChargeType NOT IN ('TechFeeFullYear', 'TechFeeSemester')&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid Payment Date'&#xA;ELSE Note + '; ' +  'Invalid Payment Date' END&#xA;WHERE ISDATE(PaymentDate) = 0&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'Invalid Payment Amount'&#xA;ELSE Note + '; ' +  'Invalid Payment Amount' END&#xA;WHERE ISNUMERIC(PaymentAmount) = 0&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingPayments&#xA;SET Accepted = 0,&#xA;Note = CASE WHEN Note IS NULL THEN 'StudentID not found in TIPWeb-IT'&#xA;ELSE Note + '; ' +  'StudentID not found in TIPWeb-IT' END&#xA;FROM _ETL_StagingPayments SP&#xA;LEFT JOIN tblStudents S ON SP.StudentID = S.StudentID&#xA;WHERE S.StudentID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE  dbo._ETL_StagingPayments&#xA;SET Accepted = 1&#xA;WHERE Accepted IS NULL&#xA;&#xA;GO &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
