          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="UPDATE dbo._ETL_Payments&#xA;SET SiteUID = S.SiteUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblTechSites S ON P.SiteID = S.SiteID&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET ChargeUID = C.ChargeUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblUnvCharges C ON C.Notes LIKE '%OPS Transaction ID: ' + P.TransactionID + '%'&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET ChargePaymentUID = CP.ChargePaymentUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblUnvChargePayments CP ON CP.Notes LIKE '%OPS Transaction ID: ' + P.TransactionID + '%'&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET ChargeTypeUID = CT.ChargeTypeUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblUnvChargeTypes CT ON P.ChargeType = CT.Name&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET StudentsUID = S.StudentsUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblStudents S ON P.StudentID = S.StudentID&#xA;AND P.SiteID = S.CampusID&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET StudentsUID = S.StudentsUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN (SELECT MAX(StudentsUID) AS StudentsUID, StudentID&#xA;FROM tblStudents&#xA;GROUP BY StudentID) AS S ON P.StudentID = S.StudentID&#xA;WHERE P.StudentsUID IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
