              SQLTask:SqlStatementSource="UPDATE tblUnvCharges&#xA;SET DateSatisfied = P.PaymentDate&#xA;FROM tblUnvCharges C&#xA;JOIN _ETL_Payments P ON C.ChargeUID = P.ChargeUID&#xA;WHERE P.ChargeStatus = 'Satisfied'&#xA;AND C.DateSatisfied IS NULL&#xA;&#xA;GO&#xA;&#xA;INSERT INTO tblUnvCharges&#xA;(ChargeTypeUID, EntityUID, EntityTypeUID, ChargeSiteUID,&#xA;ChargeAmount, Void, [Description], Notes, UniversalID,&#xA;ItemUID, ItemTypeUID, DateSatisfied, ApplicationUID,&#xA;CreatedByUserID, CreatedDate, LastModifiedByUserID, LastModifiedDate)&#xA;SELECT P.ChargeTypeUID, P.StudentsUID, 4, P.SiteUID,&#xA;P.ChargeAmount, 0, NULL, 'OPS Transaction ID: ' + P.TransactionID, '',&#xA;0, 0, CASE WHEN P.ChargeStatus = 'Satisfied' THEN P.PaymentDate ELSE NULL END, 2,&#xA;0, GETDATE(), 0, GETDATE()&#xA;FROM dbo._ETL_Payments P&#xA;WHERE P.ChargeUID IS NULL&#xA;&#xA;GO&#xA;&#xA;INSERT INTO tblTechStudentCharges&#xA;(StudentID, ChargeUID)&#xA;SELECT S.StudentID, UC.ChargeUID&#xA;FROM tblUnvCharges UC&#xA;JOIN tblStudents S ON UC.EntityTypeUID = 4&#xA;AND UC.EntityUID = S.StudentsUID&#xA;LEFT JOIN tblTechStudentCharges TSC&#xA;ON S.StudentID = TSC.StudentID&#xA;AND UC.ChargeUID = TSC.ChargeUID&#xA;WHERE UC.Notes LIKE '%OPS Transaction ID:%'&#xA;AND UC.ApplicationUID = 2&#xA;AND TSC.StudentChargeUID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_Payments&#xA;SET ChargeUID = C.ChargeUID&#xA;FROM dbo._ETL_Payments P&#xA;JOIN tblUnvCharges C ON C.Notes = 'OPS Transaction ID: ' + P.TransactionID&#xA;WHERE P.ChargeUID IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
