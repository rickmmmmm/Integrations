<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition">
  <Description />
  <DataSet Name="DupAccessories3">
    <Query>
      <DataSourceReference>HayesConversion</DataSourceReference>
      <CommandText>DECLARE @rowCount INT
SET @rowCount = (



SELECT COUNT (*)
--etl.ProductName, etl.AccessoryName3, etl.AccessoryPrice3, etl.AccessoryQuantity3, etl.AccessoryConsumable3
FROM  Clients_ETL AS etl 
INNER JOIN
         (SELECT  ProductName, AccessoryName3, COUNT(*) AS dupcount
           FROM  (SELECT DISTINCT ProductName, AccessoryName3, AccessoryPrice3, AccessoryQuantity3, AccessoryConsumable3
			 FROM   Clients_ETL
			 WHERE  (AccessoryName3 IS NOT NULL)) AS a
           GROUP BY ProductName, AccessoryName3
           HAVING  (COUNT(*) &gt; 1)) AS b ON etl.ProductName = b.ProductName AND etl.AccessoryName3 = b.AccessoryName3
           )
           
           ---SELECT @rowCount
           
 IF @rowCount &gt;0
 BEGIN 
  SELECT etl.ProductName, etl.AccessoryName3, etl.AccessoryPrice3, etl.AccessoryQuantity3, etl.AccessoryConsumable3
  FROM  Clients_ETL AS etl 
  INNER JOIN
			 (SELECT  ProductName, AccessoryName3, COUNT(*) AS dupcount
           FROM  (SELECT DISTINCT ProductName, AccessoryName3, AccessoryPrice3, AccessoryQuantity3, AccessoryConsumable3
			 FROM   Clients_ETL
			 WHERE  (AccessoryName3 IS NOT NULL)) AS a
           GROUP BY ProductName, AccessoryName3
           HAVING  (COUNT(*) &gt; 1)) AS b ON etl.ProductName = b.ProductName AND etl.AccessoryName3 = b.AccessoryName3
 END 
 ELSE 
 BEGIN 
  SELECT 'No duplciate accessories' AS productname, '' AS accesooryname3, null AS accessoryprice3, NULL AS accessoryquantity3, '' AS AccessoryConsumable3
 END</CommandText>
      <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
    </Query>
    <Fields>
      <Field Name="ProductName">
        <DataField>ProductName</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryName3">
        <DataField>AccessoryName3</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryPrice3">
        <DataField>AccessoryPrice3</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryQuantity3">
        <DataField>AccessoryQuantity3</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryConsumable3">
        <DataField>AccessoryConsumable3</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>