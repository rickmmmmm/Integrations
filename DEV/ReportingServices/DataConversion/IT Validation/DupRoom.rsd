<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition">
  <Description />
  <DataSet Name="DupRoom">
    <Query>
      <DataSourceReference>HayesConversion</DataSourceReference>
      <CommandText>SELECT DISTINCT*
FROM 
(
	SELECT DISTINCT etl.SiteID, etl.RoomNumber, RoomDesc ,  RoomType
	FROM dbo.Clients_ETL etl
	JOIN 
	(

	  SELECT SiteID, RoomNumber, COUNT(RoomNumber) AS dupRoom
	  FROM 
	  (
	   SELECT DISTINCT SiteID, RoomNumber, RoomDesc
		FROM Clients_ETL

	  )a GROUP BY SiteID, a.RoomNumber HAVING COUNT(a.RoomNumber)&gt;1
	)b  ON etl.SiteID =b.SiteID AND etl.RoomNumber = b.RoomNumber

	UNION ALL 
	 ---get Dup Room Numbers with different Room Type
	SELECT DISTINCT etl.SiteID, etl.RoomNumber, etl.RoomDesc ,  RoomType
	FROM dbo.Clients_ETL etl
	JOIN 
	(

	  SELECT SiteID, RoomNumber, RoomDesc, COUNT(RoomNumber) AS dupRoom
	  FROM 
	  (
	   SELECT DISTINCT SiteID, RoomNumber, RoomDesc, roomType
		FROM Clients_ETL

	  )a GROUP BY SiteID, a.RoomNumber , a.RoomDesc HAVING COUNT(a.RoomNumber)&gt;1
	)b  ON etl.SiteID =b.SiteID AND etl.RoomNumber = b.RoomNumber AND etl.RoomDesc = b.RoomDesc
)c

order by SiteID, RoomNumber</CommandText>
      <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
    </Query>
    <Fields>
      <Field Name="SiteID">
        <DataField>SiteID</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="RoomNumber">
        <DataField>RoomNumber</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="RoomDesc">
        <DataField>RoomDesc</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="RoomType">
        <DataField>RoomType</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>