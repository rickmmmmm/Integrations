<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition">
  <Description />
  <DataSet Name="DupAccessories1">
    <Query>
      <DataSourceReference>HayesConversion</DataSourceReference>
      <CommandText>DECLARE @rowCount INT
SET @rowCount = (



SELECT COUNT (*)
--etl.ProductName, etl.AccessoryName1, etl.AccessoryPrice1, etl.AccessoryQuantity1, etl.AccessoryConsumable1
FROM  Clients_ETL AS etl 
INNER JOIN
         (SELECT  ProductName, AccessoryName1, COUNT(*) AS dupcount
           FROM  (SELECT DISTINCT ProductName, AccessoryName1, AccessoryPrice1, AccessoryQuantity1, AccessoryConsumable1
			 FROM   Clients_ETL
			 WHERE  (AccessoryName1 IS NOT NULL)) AS a
           GROUP BY ProductName, AccessoryName1
           HAVING  (COUNT(*) &gt; 1)) AS b ON etl.ProductName = b.ProductName AND etl.AccessoryName1 = b.AccessoryName1
           )
           
           ---SELECT @rowCount
           
 IF @rowCount &gt;0
 BEGIN 
  SELECT  distinct etl.ProductName, etl.AccessoryName1, etl.AccessoryPrice1, etl.AccessoryQuantity1, etl.AccessoryConsumable1
  FROM  Clients_ETL AS etl 
  INNER JOIN
			 (SELECT  ProductName, AccessoryName1, COUNT(*) AS dupcount
           FROM  (SELECT DISTINCT ProductName, AccessoryName1, AccessoryPrice1, AccessoryQuantity1, AccessoryConsumable1
			 FROM   Clients_ETL
			 WHERE  (AccessoryName1 IS NOT NULL)) AS a
           GROUP BY ProductName, AccessoryName1
           HAVING  (COUNT(*) &gt; 1)) AS b ON etl.ProductName = b.ProductName AND etl.AccessoryName1 = b.AccessoryName1
 END 
 ELSE 
 BEGIN 
  SELECT 'No duplciate accessories' AS productname, '' AS accesooryname1, null AS accessoryprice1, NULL AS accessoryquantity1, '' AS AccessoryConsumable1
 END</CommandText>
      <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
    </Query>
    <Fields>
      <Field Name="ProductName">
        <DataField>ProductName</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryName1">
        <DataField>AccessoryName1</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryPrice1">
        <DataField>AccessoryPrice1</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryQuantity1">
        <DataField>AccessoryQuantity1</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="AccessoryConsumable1">
        <DataField>AccessoryConsumable1</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>