<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition">
  <Description />
  <DataSet Name="DupSerials">
    <Query>
      <DataSourceReference>HayesConversion</DataSourceReference>
      <CommandText>SELECT        Serial, COUNT(Serial) AS dupSerial
FROM            Clients_ETL
WHERE        (LEN(Serial) &gt; 0)
GROUP BY Serial
HAVING        (COUNT(Serial) &gt; 1)</CommandText>
      <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
    </Query>
    <Fields>
      <Field Name="Serial">
        <DataField>Serial</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="dupSerial">
        <DataField>dupSerial</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>