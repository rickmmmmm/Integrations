  <DataSets>
    <DataSet Name="DupTeacherList">
      <Query>
        <DataSourceName>Main</DataSourceName>
        <CommandText>IF OBJECT_ID('tempdb..#dup_teachers')IS NOT NULL
BEGIN
DROP TABLE #dup_teachers
END

SELECT TeacherID,FullName,
-- CampusID,

 MAX(dupCheck) AS #OFCampus_Listed
INTO #dup_teachers
FROM(

		SELECT ROW_NUMBER() OVER (PARTITION BY TeacherID ORDER BY CampusID) AS dupCheck,  TeacherID, 
		REPLACE(FirstName + ' ' + ISNULL(MiddleName,'') + ' '+LastName,'  ',' ') AS FullName, CampusID
		FROM dbo.tblTeachers
		--ORDER BY TeacherID
)a
GROUP BY TeacherID, FullName
HAVING MAX(dupCheck)&gt;1


SELECT TeacherID,
FullName,
#OFCampus_Listed
FROM #dup_teachers</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TeacherID">
          <DataField>TeacherID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_OFCampus_Listed">
          <DataField>#OFCampus_Listed</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseNames">
      <Query>
        <DataSourceName>sysdata</DataSourceName>
        <CommandText>SELECT        name
FROM            sys.databases
WHERE        (name LIKE '%TipWeb%')
ORDER BY name</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Sort2_Parameter_List">
      <Query>
        <DataSourceName>Main</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Sort1">
            <Value>=Parameters!Sort1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Sort1 VARCHAR(50)
--SET                @Sort1 = 'ID_OFCampus_Listed' 

IF @Sort1 = 'TeacherID' BEGIN
                             SELECT        'FullName' AS fields
UNION ALL
SELECT        'ID_OFCampus_Listed' AS fileds END IF @Sort1 = 'FullName' BEGIN
                             SELECT        'TeacherID' AS fields
UNION ALL
SELECT        'ID_OFCampus_Listed' AS fields END IF @Sort1 = 'ID_OFCampus_Listed' BEGIN
                             SELECT        'TeacherID' AS fields
UNION ALL
SELECT        'FullName' AS fields END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="fields">
          <DataField>fields</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
