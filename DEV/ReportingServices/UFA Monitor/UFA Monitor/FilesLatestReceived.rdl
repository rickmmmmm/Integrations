  <DataSets>
    <DataSet Name="CustomerList">
      <Query>
        <DataSourceName>DB</DataSourceName>
        <CommandText>select customersUID, CustomerName
from ImportManagementSystem.dbo.CustomerList
order by CustomerName
</CommandText>
      </Query>
      <Fields>
        <Field Name="customersUID">
          <DataField>customersUID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
