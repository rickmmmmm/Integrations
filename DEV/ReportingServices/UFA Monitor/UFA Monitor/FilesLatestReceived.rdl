          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ListFiles">
      <Query>
        <DataSourceName>DB</DataSourceName>
        <CommandText>="select FileReceived, a.Area, Description, RunDate, FileName "+
 "from ImportManagementSystem.dbo.CustomerFileStatus cfs "+
 "join ImportManagementSystem.dbo.IMS_Area a  on cfs.Area = a.area "+
 "where CustomersUID =  " + Str(Parameters!CustomerUIDFiles.Value) +
 "order by RunDate desc "</CommandText>
      </Query>
      <Fields>
        <Field Name="FileReceived">
          <DataField>FileReceived</DataField>
