  <DataSets>
    <DataSet Name="SpecialProcessList">
      <Query>
        <DataSourceName>DB</DataSourceName>
        <CommandText>select cl.CustomersUID, CustomerName,
ia.[Description] as Area,  
 PreProcess, spProcessDescription, spName, cp.Active
 from ImportManagementSystem.dbo.CustomerList cl
 cross join ImportManagementSystem.dbo.IMS_Area ia 
join ImportManagementSystem.dbo.CustomerExtraProcesses cp on cl.CustomersUID = cp.CustomersUID and ia.Area = cp.area
 where cl.active = 1 
 order by customername, area</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CustomersUID">
          <DataField>CustomersUID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Area">
