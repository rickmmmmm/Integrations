          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{1206D90D-C338-4A46-956C-56FA62AFD2D6}"
              SQLTask:SqlStatementSource="INSERT INTO dbo._ETL_Export&#xA;(AssetID, Tag, Serial, SiteID, [Status], LocationType, Location)&#xA;SELECT TI.AssetID, TI.Tag, TI.Serial, S.SiteID, SS.StatusDesc,&#xA;CASE WHEN TI.EntityTypeUID = 4 THEN 'ST' ELSE&#xA;CASE WHEN TI.EntityTypeUID = 3 THEN 'SF' ELSE 'RM' END END,&#xA;CASE WHEN TI.EntityTypeUID = 4 THEN ST.StudentID ELSE&#xA;CASE WHEN TI.EntityTypeUID = 3 THEN SF.TeacherID ELSE R.RoomNumber END END&#xA;FROM tblTechInventory TI&#xA;JOIN tblTechSites S ON TI.SiteUID = S.SiteUID&#xA;JOIN tblStatus SS ON TI.StatusUID = SS.statusID&#xA;LEFT JOIN tblStudents ST ON TI.EntityUID = ST.StudentsUID&#xA;AND TI.EntityTypeUID = 4&#xA;LEFT JOIN tblTeachers SF ON TI.EntityUID = SF.TeachersUID&#xA;AND TI.EntityTypeUID = 3&#xA;LEFT JOIN tblUnvRooms R ON TI.EntityUID = R.RoomUID&#xA;AND TI.EntityTypeUID = 2&#xA;LEFT JOIN dbo._ETL_Export E ON TI.Tag = E.Tag&#xA;WHERE TI.InventoryUID &gt; 0&#xA;AND TI.EntityTypeUID IN (2, 3, 4)&#xA;AND E.ETLExportUID IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
