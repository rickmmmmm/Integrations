              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo._ETL_Export', 'U') IS NOT NULL DROP TABLE dbo._ETL_Export;&#xA;&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;&#xA;GO&#xA;&#xA;SET QUOTED_IDENTIFIER ON&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[_ETL_Export](&#xA;&#x9;[ETLExportUID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[AssetID] [varchar](100) NOT NULL,&#xA;&#x9;[Tag] [varchar](50) NOT NULL,&#xA;&#x9;[Serial] [varchar](50) NOT NULL,&#xA;&#x9;[SiteID] [varchar](100) NOT NULL,&#xA;&#x9;[Status] [varchar](50) NOT NULL,&#xA;&#x9;[LocationType] [varchar](4) NOT NULL,&#xA;&#x9;[Location] [varchar](50) NOT NULL,&#xA; CONSTRAINT [PK__ETL_Export] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[ETLExportUID] ASC&#xA;)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
