emTypes)&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET fa_model_number = ''&#xA;WHERE fa_model_number = 'NA'&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET Import = 0,&#xA;RejectMessage = CASE WHEN RejectMessage IS NULL THEN 'Invalid Purchase Date'&#xA;ELSE RejectMessage + '; ' +  'Invalid Purchase Date' END&#xA;WHERE ISDATE(fa_acquire_date) = 0&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;SET Import = 0,&#xA;RejectMessage = CASE WHEN RejectMessage IS NULL THEN 'Invalid Purchase Price'&#xA;ELSE RejectMessage + '; ' +  'Invalid Purchase Price' END&#xA;WHERE ISNUMERIC(fa_purchase_cost) = 0 AND ltrim(rtrim(isNull(fa_purchase_cost, ''))) &lt;&gt; ''&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;set fa_purchase_cost = 0&#xA;where ltrim(rtrim(isNull(fa_purchase_cost, ''))) &lt;&gt; ''&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;set a_purch_order_no = '00Integration'&#xA;where ltrim(rtrim(isNull(a_purch_order_no, ''))) in ('Null', '')&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;set a_vendor = ''&#xA;where ltrim(rtrim(isNull(a_vendor, ''))) = 'Null'&#xA;&#xA;UPDATE dbo._ETL_StagingInventory&#xA;set a_vendor_name = ''&#xA;where ltrim(rtrim(isNull(a_vendor_name, ''))) = 'Null'&#xA;&#xA;UPDATE  dbo._ETL_StagingInventory&#xA;SET Import = 1&#xA;WHERE Import IS NULL&#xA;&#xA;UPDATE  dbo._ETL_StagingInventory&#xA;SET Import = 1&#xA;WHERE Import IS NULL&#xA;&#xA;GO&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
