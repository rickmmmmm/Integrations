              SQLTask:SqlStatementSource="&#xA;update _ETL_Inventory&#xA;set Department = &#xA;&#x9;case when substring(accountCode, 28, 3) = '836' then 'Athletics'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) in ('007', '870', '871') then 'CATE'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) = '990' then 'Child Nutrition Services'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) = '905' then 'Custodial'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) in ('804', '831', '864') then 'Fine Arts'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) = '882' then 'Health Services'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) = '837' then 'Pre-K'&#xA;&#x9;&#x9;when substring(accountCode, 28, 3) = '888' then 'Special Education'&#xA;&#x9;&#x9;else 'ALL'&#xA;&#x9;end &#xA;&#xA;GO&#xA;&#xA;update _ETL_Inventory&#xA;set FundingSource = &#xA;&#x9;case when ltrim(rtrim(accountCode)) = '' then 'Unassigned'&#xA;&#x9;else&#xA;&#x9;&#x9;case when ISNUMERIC(substring(accountCode, 1, 3)) = 1&#xA;&#x9;&#x9;&#x9;then&#xA;&#x9;&#x9;&#x9;case when cast(substring(accountCode, 1, 3) as int) &gt;=100 and cast(substring(accountCode, 1, 3) as int) &lt;= 199 then 'General Fund'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=200 and cast(substring(accountCode, 1, 3) as int) &lt;= 289 then 'Federal Program'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=290 and cast(substring(accountCode, 1, 3) as int) &lt;= 379 then 'Federal SSA'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=380 and cast(substring(accountCode, 1, 3) as int) &lt;= 429 then 'State Funded'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=430 and cast(substring(accountCode, 1, 3) as int) &lt;= 459 then 'State/Local SSA'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=460 and cast(substring(accountCode, 1, 3) as int) &lt;= 499 then 'Local Program'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=500 and cast(substring(accountCode, 1, 3) as int) &lt;= 599 then 'Debt Service Fund'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=600 and cast(substring(accountCode, 1, 3) as int) &lt;= 679 then 'Bond Fund'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=680 and cast(substring(accountCode, 1, 3) as int) &lt;= 699 then 'Capital Improvements'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=700 and cast(substring(accountCode, 1, 3) as int) &lt;= 749 then 'Enterprise Fund'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=750 and cast(substring(accountCode, 1, 3) as int) &lt;= 799 then 'Internal Service Fund'&#xA;&#x9;&#x9;&#x9;&#x9;when cast(substring(accountCode, 1, 3) as int) &gt;=860 and cast(substring(accountCode, 1, 3) as int) &lt;= 899 then 'Agency Fund'&#xA;&#x9;&#x9;&#x9;else 'Unassigned'&#xA;&#x9;&#x9;&#x9;end&#xA;&#xA;&#x9;&#x9;else 'Unassigned'&#xA;&#x9;&#x9;end&#xA;&#x9;end&#xA;&#xA;GO&#xA;&#xA;       " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
