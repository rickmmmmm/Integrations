              SQLTask:SqlStatementSource="&#xA;--select * &#xA;update e set import = 0, RejectNotes = 'Duplicate ISBN'&#xA;from [dbo].[_ETL_TextbookStaging] e&#xA;where ISBN in (&#xA;&#x9;select ISBN from (&#xA;&#x9;&#x9;select ISBN, Title, Publisher&#xA;&#x9;&#x9;from  [dbo].[_ETL_TextbookStaging] e&#xA;&#x9;&#x9;where import = 1 &#xA;&#x9;&#x9;) a&#xA;&#x9;group by ISBN having count(*) &gt; 1&#xA;&#x9;)&#xA;and ISBN not in (select ISBN from tblBookInventory)&#xA;&#xA;&#xA;--select *&#xA;update e set BookInventoryUID = tbi.BookInventoryUID, New = 0&#xA;from  [dbo].[_ETL_TextbookStaging] e&#xA;join tblBookInventory tbi on e.ISBN = tbi.ISBN&#xA;where import = 1&#xA;&#xA;insert into tblBookInventory (ISBN, slc, Title, Price, Publisher, UserID, ModifiedDate, notes, grade, Expire)&#xA;select distinct e.ISBN, e.slc, e.Title, e.Price, e.PublisherID, -2, getdate(), 'Textbook Integration' as Notes, isNull(e.Grade, '') as Grade, isNull(Expires, '') as Expires&#xA;from  [dbo].[_ETL_TextbookStaging] e &#xA;where import = 1 and BookInventoryUID is null&#xA;&#xA;&#xA;--select *&#xA;update e set BookInventoryUID = tbi.BookInventoryUID&#xA;from  [dbo].[_ETL_TextbookStaging] e&#xA;join tblBookInventory tbi on e.ISBN = tbi.ISBN&#xA;where import = 1 and New = 1 and e.BookInventoryUID is null&#xA;&#xA;&#xA;--select *,&#xA;update tbi set Title = e.Title, Publisher = e.PublisherID, SLC = e.SLC, Price = &#xA; (Case when cast(isNull(e.Price, '0') as Money) &gt; cast(isNull(tbi.Price, '0') as Money) then e.Price else tbi.Price end)&#xA;from  [dbo].[_ETL_TextbookStaging] e&#xA;join tblBookInventory tbi on e.ISBN = tbi.ISBN&#xA;where import = 1&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
