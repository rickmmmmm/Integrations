          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{29D19D31-7728-4AEF-B997-501E7F43A173}"
              SQLTask:SqlStatementSource="DECLARE @ClientUID int, @rpt Nvarchar(max), @mailProfile NVARCHAR(MAX), @ZipExists int, @ZipFileSize NVARCHAR(MAX), @ZipFileSizeOver int &#xA;&#xA;DECLARE @body nvarchar(max), @subject NVARCHAR(max), @sText nvarchar(max), @logdate NVARCHAR(50) &#xA;DECLARE @ClientCatalog NVARCHAR(max), @CustomerName Nvarchar(max), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX)&#xA;&#xA;declare @EmailAttachment int, @recipients varchar(100), @copy_recipients varchar(100)&#xA;&#xA;&#xA;DECLARE @sSqlBase Nvarchar(max), @sSql Nvarchar(max), @ParamDef NVARCHAR(max)&#xA;DECLARE @total int, @Filetotal INT, @New int, @reject INT, @Ordered int&#xA;&#xA;&#xA;set @ClientCatalog = ?&#xA;SET @mailProfile = ?&#xA;--set @rpt = ?&#xA;set @ZipExists = 0&#xA;set @ZipFileSize = 0&#xA;set @EmailAttachment = 0&#xA;set @email = ?&#xA;set @ccemail =  ?&#xA;&#xA;&#xA;set @ZipFileSizeOver = 0&#xA;if cast(@ZipFileSize as int) &gt; 1000000 &#xA;&#x9;set @ZipFileSizeOver = 1&#x9;&#xA;&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'Textbook Integration Processing Log for Anne Arundel (' + @logdate + ')'&#xA;&#xA;set @sSqlBase = N'SELECT @total = COUNT(*) FROM ' + @ClientCatalog + N'.dbo.'&#xA;set @ParamDef = N'@total int OUTPUT'&#xA;&#xA;&#xA;set @sText = ''&#xA;-------------------------------------------------&#xA;&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_ETL_TextbookStaging '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_ETL_TextbookStaging where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_ETL_TextbookStaging where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Textbook: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#xA;&#xA;--IF @ZipExists &gt; 0&#xA;--BEGIN&#xA;--&#x9;IF @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;--&#x9;BEGIN&#xA;--&#x9;&#x9;set @sText = @sText + 'A zipped copy of the reject file(s) is attached.'  + '&lt;br/&gt;&lt;br/&gt;' &#xA;--&#x9;END&#xA;--&#x9;ELSE&#xA;--&#x9;BEGIN&#xA;--&#x9;&#x9;if @EmailAttachment &gt; 0&#xA;--&#x9;&#x9;BEGIN&#xA;--&#x9;&#x9;&#x9;set @sText = @sText + 'The zipped copy of the reject file(s) was too large to attach.'  + '&lt;br/&gt;'  +&#xA;-- &#x9;&#x9;&#x9;&#x9;&#x9;'You can find the file in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;--&#x9;&#x9;END&#xA;--&#x9;&#x9;ELSE&#xA;--&#x9;&#x9;BEGIN&#xA;--&#x9;&#x9;&#x9;set @sText = @sText + 'You can find Reject Files (if any) in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;--&#x9;&#x9;END&#xA;&#xA;--&#x9;END&#xA;--END&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;'&lt;p&gt;' + @sText + '&lt;/p&gt;&lt;br/&gt;' +&#xA;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9; '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;&#x9; '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;if @ZipExists &gt; 0 and @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;BEGIN&#xA;&#x9;--print @sText  + ' ' + 'attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject,&#xA;&#x9;&#x9;@file_attachments = @rpt;&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;&#xA;&#x9;--print @sText  + ' ' + 'no attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject;&#xA;END&#xA;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
