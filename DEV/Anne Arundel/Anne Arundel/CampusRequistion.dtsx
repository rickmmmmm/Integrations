          DTS:refId="Package\Cleanup and go home\Send Processing Email"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{5805EE25-5D7E-4585-A4A3-5DC080391F0F}"
