          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{8D5F7C46-9087-4498-8AE8-62562C93F3E3}"
              SQLTask:SqlStatementSource="&#xA;&#xA;DECLARE @rpt Nvarchar(max), @mailProfile NVARCHAR(MAX), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX)&#xA;DECLARE @body nvarchar(max), @subject NVARCHAR(max), @sText nvarchar(max), @logdate NVARCHAR(50) &#xA;&#xA;DECLARE @sSqlBase Nvarchar(max), @sSql Nvarchar(max)&#xA;DECLARE @total int, @reject INT&#xA;&#xA;SET @mailProfile = ?  &#xA;set @email = ? &#xA;set @ccemail = ? &#xA;set @rpt = ?  &#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'Campus Purchasing Integration Processing Log for Anne Arundel (' + @logdate + ')'&#xA;&#xA;&#xA;&#xA;set @sText = ''&#xA;&#xA;SELECT @total = COUNT(*) FROM _ETL_CampusStaging &#xA;SELECT @reject = COUNT(*) FROM _ETL_CampusStaging where import = 0 &#xA;&#xA;&#xA;set @sText = @sText +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @total) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' &#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;'&lt;p&gt;' + @sText + '&lt;/p&gt;' +&#xA;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9; '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;&#x9; '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;if @reject &gt; 0&#xA;BEGIN&#xA;&#x9;--print @sText  + ' ' + 'attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject,&#xA;&#x9;&#x9;@file_attachments = @rpt;&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;&#xA;&#x9;--print @sText  + ' ' + 'no attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject;&#xA;END&#xA;&#xA;    &#xA;    &#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
