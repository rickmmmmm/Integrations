    public override void CreateNewOutputRows()
    {
        reader = new StreamReader(Variables.InputFile);

        reader.ReadLine();

        while (!reader.EndOfStream)
        {
            string line = reader.ReadLine();
            string sSeparator = ",";
            string sDelimiter = $@"{sSeparator}"; //$@"""{sSeparator}""";

            string[] columns = Regex.Split(line, sDelimiter); //line.Split(new string[] { sDelimiter, StringSplitOptions.None });

            int i = 0;

            Output0Buffer.AddRow();

            Output0Buffer.Tag = System.Convert.ToString(columns[i++]).Replace("\"", "");
            Output0Buffer.Serial = System.Convert.ToString(columns[i++]).Replace("\"", "");
            Output0Buffer.LastSeenDate = System.Convert.ToString(columns[i++]).Replace("\"", "");
