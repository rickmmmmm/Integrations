          SQLTask:Connection="{F76E38E7-1F78-49A1-BFB0-890E4DDC0275}"
          SQLTask:SqlStatementSource="DECLARE @SerialsStaging TABLE&#xA;(&#xA;&#x9;[Tag] [varchar](255) NULL,&#xA;&#x9;[Serial] [varchar](255) NULL,&#xA;&#x9;[LastSeenDate] [varchar](255) NULL,&#xA;&#x9;[LastUser] [varchar](255) NULL,&#xA;&#x9;RowID int&#xA;)&#xA;&#xA;&#xA;DECLARE @EntityUID&#x9;INT, @OriginStatusID&#x9;INT, @InUseStatusID&#x9;INT,@MDMUserID INT, @InventoryUID INT, @SiteUID INT, @EntityTypeUID INT&#xA;DECLARE @MDMDate DATETIME = CAST( '3/24/21' AS datetime)&#xA;&#xA;&#xA;SELECT @MDMUserID = [UserID] FROM [TipWebHostedGarlandISD].[dbo].[tblUser]&#xA;WHERE LoginName = 'MDM_Verification'&#xA;&#xA;INSERT INTO @SerialsStaging([Tag],[Serial],[LastSeenDate],[LastUser],RowID)&#xA;SELECT [Tag],[Serial],[LastSeenDate],[LastUser],ROW_NUMBER() OVER(ORDER BY [Tag] ASC) AS Row#&#xA;FROM [StagingFound]&#xA;WHERE [Exception] = 0&#xA;&#xA;SELECT * FROM @SerialsStaging&#xA;DECLARE @ROW_COUNT INT, @TagForRowID VARCHAR(255), @SerialForRowID VARCHAR(255), @LastSeenDateRowID VARCHAR(255), @LastUserForRowID VARCHAR(255)&#xA;&#xA;SELECT @ROW_COUNT = COUNT(*) FROM @SerialsStaging&#xA;&#xA;DECLARE @INDEX INT = 1&#xA;&#xA;WHILE(@INDEX &lt;= @ROW_COUNT)&#xA;BEGIN&#xA;&#x9;SELECT @TagForRowID = Tag,&#xA;&#x9;&#x9;   @SerialForRowID = Serial,&#xA;&#x9;&#x9;   @LastSeenDateRowID = LastSeenDate,&#xA;&#x9;&#x9;   @LastUserForRowID = LastUser&#xA;&#x9;FROM @SerialsStaging WHERE RowID = @INDEX;&#xA;&#x9;--PRINT 'Tag: ' + @TagForRowID&#xA;&#x9;--PRINT 'Serial: ' + @SerialForRowID&#xA;&#x9;SELECT @InventoryUID = InventoryUID, @EntityUID = EntityUID, @EntityTypeUID = EntityTypeUID, @OriginStatusID = StatusUID , @SiteUID = SiteUID&#xA;&#x9;FROM [TipWebHostedGarlandISD].[dbo].[tblTechInventory]&#xA;&#x9;WHERE [Tag] = @TagForRowID&#xA;&#xA;&#x9;--PRINT '@EntityUID: ' + CONVERT(VARCHAR, @EntityUID)&#xA;&#x9;--PRINT '@OriginStatusID: ' + CONVERT(VARCHAR, @OriginStatusID)&#xA;&#xA;&#x9;IF(@EntityTypeUID = 2)&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;SET @InUseStatusID = 28&#xA;&#x9;END&#xA;&#x9;ELSE IF(@EntityTypeUID = 3)&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;SET @InUseStatusID = 31&#xA;&#x9;END&#xA;&#x9;ELSE IF(@EntityTypeUID = 4)&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;SET @InUseStatusID = 30&#xA;&#x9;END&#x9;&#xA;&#xA;&#x9;PRINT '@InUseStatusID: ' + CONVERT(VARCHAR, @InUseStatusID)&#xA;&#x9;&#xA;&#x9; UPDATE [TipWebHostedGarlandISD].[dbo].[tblTechInventory]&#xA;&#x9; SET StatusUID = @InUseStatusID,&#xA;&#x9; InventoryNotes = InventoryNotes + ': MDM: ' + @LastUserForRowID + ':' +    convert(varchar(10), CAST( SUBSTRING( @LastSeenDateRowID, 0, 11) AS datetime), 101) &#xA;&#x9; WHERE InventoryUID = @InventoryUID --WHERE [Tag] = @TagForRowID&#xA;&#x9;&#xA;&#xA;&#x9;INSERT INTO [TipWebHostedGarlandISD].[dbo].[tblTechInventoryHistory]&#xA;           ([InventoryUID]&#xA;           ,[InventoryTypeUID]&#xA;           ,[SiteUID]&#xA;           ,[EntityUID]&#xA;           ,[EntityTypeUID]&#xA;           ,[StatusUID]&#xA;           ,[OriginSiteUID]&#xA;           ,[OriginStatusUID]&#xA;           ,[OriginEntityUID]&#xA;           ,[OriginEntityTypeUID]&#xA;           ,[InventoryHistoryNotes]&#xA;           ,[CreatedByUserID]&#xA;           ,[CreatedDate]&#xA;           ,[LastModifiedByUserID]&#xA;           ,[LastModifiedDate]&#xA;           ,[BulkEditUID]&#xA;           ,[InventorySourceUID]&#xA;           ,[ContainerUID]&#xA;           ,[OriginContainerUID]&#xA;           ,[ArchiveUID]&#xA;           ,[OriginArchiveUID]&#xA;           ,[OperationUID])&#xA;&#x9;  SELECT [InventoryUID]&#xA;      ,[InventoryTypeUID]&#xA;      ,[SiteUID]&#xA;      ,[EntityUID]&#xA;      ,[EntityTypeUID]&#xA;      ,@InUseStatusID&#xA;      ,[SiteUID]&#xA;&#x9;  ,[StatusUID]&#xA;      ,[EntityUID]&#xA;      ,[EntityTypeUID]&#xA;      , ' MDM: ' + @LastUserForRowID + ':' +    convert(varchar(10), CAST( SUBSTRING( @LastSeenDateRowID, 0, 11) AS datetime), 101) &#xA;      ,@MDMUserID&#xA;      ,GETDATE()&#xA;      ,@MDMUserID&#xA;      ,GETDATE()&#xA;      ,@MDMUserID&#xA;      ,[InventorySourceUID]&#xA;      ,[ContainerUID]&#xA;      ,[ContainerUID]&#xA;      ,[ArchiveUID]&#xA;      ,[ArchiveUID]&#xA;      ,0&#xA;&#x9;  FROM [TipWebHostedGarlandISD].[dbo].[tblTechInventory]&#xA;&#x9;  WHERE InventoryUID = @InventoryUID&#xA;&#xA;&#xA;&#x9;INSERT INTO [TipWebHostedGarlandISD].[dbo].[tblTechInventoryQuickAction]&#xA;           ([InventoryUID]&#xA;           ,[SiteUID]&#xA;           ,[EntityUID]&#xA;           ,[EntityTypeUID]&#xA;           ,[ActionUID]&#xA;           ,[CreatedByUserID]&#xA;           ,[CreatedDate])&#xA;     VALUES&#xA;           (@InventoryUID&#xA;           ,@SiteUID&#xA;           ,@EntityUID&#xA;           ,@EntityTypeUID&#xA;           ,6 -- Found&#xA;           ,@MDMUserID&#xA;           ,@MDMDate)&#xA;&#xA;&#xA;&#xA;&#x9;SET @INDEX = @INDEX + 1&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
