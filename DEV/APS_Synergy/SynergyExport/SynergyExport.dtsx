                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  ts.SiteID, ts.SiteName, ttm.ItemName as ProductName, 
tti.Tag, 
case when tti.EntityTypeUID = 2 then RoomNumber
when tti.EntityTypeUID = 3 then t.TeacherID 
when tti.EntityTypeUID = 4 then s.StudentID
end as LocationID,

case when tti.EntityTypeUID = 2 then 'Room: ' + rm.RoomDescription
when tti.EntityTypeUID = 3 then 'Staff: ' + t.FullName
when tti.EntityTypeUID = 4 then 'Student: ' + s.FullName
end as Location,
convert(varchar(10), tti.LastModifiedDate, 101) as LastScanDate

from tblTechInventory tti
join tblTechItems ttm on tti.ItemUid = ttm.ItemUID
join tblTechSites ts on tti.SiteUID = ts.SiteUID
left outer join tblSTudents s on tti.EntityTypeUID = 4 and tti.EntityUID = s.StudentsUID
left outer join tblTeachers t on tti.EntityTypeUID = 3 and tti.EntityUID = t.TeachersUID
left outer join tblUnvRooms rm on tti.EntityTypeUID = 2 and tti.EntityUID = rm.RoomUID

where inventoryUID &gt; 0
and tti.EntityTypeUID = 4</property>
