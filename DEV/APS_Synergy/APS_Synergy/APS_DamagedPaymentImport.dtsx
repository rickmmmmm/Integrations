      <DTS:Executables>
        <DTS:Executable
          DTS:refId="Package\Import Data into TIPWeb-IM\Data Preparation"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{1D61A4F0-CF28-4EF2-A5DF-836B28D92B59}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Data Preparation"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="UPDATE dbo._ETL_ChargePayments&#xA;SET ChargeUID = X.ChargeUID, ChargeAmount = X.ChargeAmount,&#xA;Copies = SD.Copies, TotalOwed = X.TotalOwed&#xA;FROM _ETL_ChargePayments CP&#xA;JOIN tblStudentsDistribution SD ON CP.Accession = SD.Accession AND CP.StudentID = &#xA;(SELECT TOP 1 StudentID FROM tblStudents WHERE StudentsUID = SD.StudentsUID)&#xA;JOIN tblBookInventory BI ON SD.ISBN = BI.ISBN&#xA;JOIN &#xA;(&#xA;SELECT MIN(CH.ChargeUID) ChargeUID, CH.ItemUID, CH.UniversalID, EntityUID,&#xA;CASE WHEN SUM(CH.ChargeAmount) = SUM(CHP.ChargeAmount) THEN 0&#xA;WHEN SUM(CHP.ChargeAmount) IS NULL THEN SUM(CH.ChargeAmount)&#xA;ELSE SUM(CH.ChargeAmount) - SUM(CHP.ChargeAmount) END [TotalOwed],&#xA;SUM(CH.ChargeAmount) [ChargeAmount]&#xA;FROM tblUnvCharges CH&#xA;LEFT JOIN(&#xA;SELECT ChargeUID, SUM(ChargeAmount) ChargeAmount&#xA;FROM tblUnvChargePayments&#xA;GROUP BY ChargeUID) CHP ON CH.ChargeUID = CHP.ChargeUID&#xA;WHERE CH.Void = 0 AND CH.EntityTypeUID = 4 AND CH.ItemTypeUID = 0&#xA;AND CH.DateSatisfied IS NULL AND CH.ApplicationUID = 1&#xA;GROUP BY EntityUID, CH.ItemUID, CH.UniversalID&#xA;) X ON BI.BookInventoryUID = X.ItemUID AND X.UniversalID = SD.Accession AND X.EntityUID = SD.StudentsUID&#xA;WHERE CP.ChargePaymentUID IS NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE dbo._ETL_ChargePayments&#xA;SET DateSatisfied = ModifiedDate&#xA;WHERE TotalPaid &gt;= TotalOwed&#xA;AND ChargePaymentUID IS NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable
          DTS:refId="Package\Import Data into TIPWeb-IM\INSERT Payments"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{89CAFBDF-CA64-4738-BA73-93F0E32A237A}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="INSERT Payments"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{24159A70-7974-4A6C-9203-44246161B4A6}"
              SQLTask:SqlStatementSource="WHILE (&#xA;SELECT COUNT(*)&#xA;FROM _ETL_ChargePayments CP&#xA;JOIN tblUnvCharges UC&#xA;ON CP.ChargeUID = UC.ChargeUID&#xA;JOIN (&#xA;SELECT ChargeUID, SUM(ChargeAmount) Paid&#xA;FROM tblUnvChargePayments&#xA;GROUP BY ChargeUID&#xA;) UCP ON UC.ChargeUID = UCP.ChargeUID&#xA;JOIN tblUnvCharges UC2&#xA;ON UC.EntityTypeUID = UC2.EntityTypeUID&#xA;AND UC.EntityUID = UC2.EntityUID&#xA;AND UC.ItemUID = UC2.ItemUID&#xA;AND UC.UniversalID = UC2.UniversalID&#xA;WHERE CP.ChargePaymentUID IS NULL&#xA;AND CP.TotalPaid &gt; (UC.ChargeAmount - UCP.Paid)&#xA;AND UC2.DateSatisfied IS NULL&#xA;AND UC2.ChargeUID &gt; UC.ChargeUID&#xA;GROUP BY UC2.EntityTypeUID, UC2.EntityUID,&#xA;UC2.ItemUID, UC2.UniversalID&#xA;) &gt; 0&#xA;BEGIN&#xA;&#x9;UPDATE _ETL_ChargePayments&#xA;&#x9;SET TotalPaid = TotalPaid - C.Owed,&#xA;&#x9;TotalOwed = TotalOwed - C.Owed,&#xA;&#x9;ChargeUID = NextChargeUID,&#xA;&#x9;TempOwed = C.Owed,&#xA;&#x9;TempChargeUID = ChargeUID&#xA;&#x9;FROM _ETL_ChargePayments CP&#xA;&#x9;JOIN (&#xA;&#x9;SELECT MIN(UC.ChargeUID) CurrentChargeUID,&#xA;&#x9;MIN(UC2.ChargeUID) NextChargeUID,&#xA;&#x9;MIN(UC.ChargeAmount) - MIN(UCP.Paid) Owed&#xA;&#x9;FROM _ETL_ChargePayments CP&#xA;&#x9;JOIN tblUnvCharges UC&#xA;&#x9;ON CP.ChargeUID = UC.ChargeUID&#xA;&#x9;JOIN (&#xA;&#x9;SELECT ChargeUID, SUM(ChargeAmount) Paid&#xA;&#x9;FROM tblUnvChargePayments&#xA;&#x9;GROUP BY ChargeUID&#xA;&#x9;) UCP ON UC.ChargeUID = UCP.ChargeUID&#xA;&#x9;JOIN tblUnvCharges UC2&#xA;&#x9;ON UC.EntityTypeUID = UC2.EntityTypeUID&#xA;&#x9;AND UC.EntityUID = UC2.EntityUID&#xA;&#x9;AND UC.ItemUID = UC2.ItemUID&#xA;&#x9;AND UC.UniversalID = UC2.UniversalID&#xA;&#x9;WHERE CP.ChargePaymentUID IS NULL&#xA;&#x9;AND CP.TotalPaid &gt; (UC.ChargeAmount - UCP.Paid)&#xA;&#x9;AND UC2.DateSatisfied IS NULL&#xA;&#x9;AND UC2.ChargeUID &gt; UC.ChargeUID&#xA;&#x9;GROUP BY UC2.EntityTypeUID, UC2.EntityUID,&#xA;&#x9;UC2.ItemUID, UC2.UniversalID&#xA;&#x9;) C ON CP.ChargeUID = C.CurrentChargeUID&#xA;&#xA;&#x9;INSERT INTO tblUnvChargePayments&#xA;&#x9;(ApplicationUID, ChargeUID, PaymentSiteUID,&#xA;&#x9;ChargeAmount, Void, [Description],&#xA;&#x9;Notes, CreatedByUserID, CreatedDate,&#xA;&#x9;LastModifiedByUserID, LastModifiedDate)&#xA;&#x9;SELECT 1, TempChargeUID, NULL,&#xA;&#x9;TempOwed, 0, 'Synergy Integration Payment',&#xA;&#x9;Notes, 0, ModifiedDate, 0, ModifiedDate&#xA;&#x9;FROM _ETL_ChargePayments&#xA;&#x9;WHERE TempChargeUID IS NOT NULL&#xA;&#x9;&#xA;&#x9;UPDATE tblUnvCharges&#xA;&#x9;SET DateSatisfied = CP.ModifiedDate,&#xA;&#x9;LastModifiedByUserID = 0,&#xA;&#x9;LastModifiedDate = CP.ModifiedDate&#xA;&#x9;FROM tblUnvCharges UC&#xA;&#x9;JOIN _ETL_ChargePayments CP&#xA;&#x9;ON UC.ChargeUID = CP.TempChargeUID&#xA;&#x9;&#xA;&#x9;UPDATE _ETL_ChargePayments&#xA;&#x9;SET TempOwed = NULL,&#xA;&#x9;TempChargeUID = NULL&#xA;&#x9;WHERE TempOwed IS NOT NULL&#xA;&#x9;OR TempChargeUID IS NOT NULL&#xA;END&#xA;&#xA;GO&#xA;&#xA;&#xA;INSERT INTO tblUnvChargePayments&#xA;(ApplicationUID, ChargeUID, PaymentSiteUID,&#xA;ChargeAmount, Void, [Description],&#xA;Notes, CreatedByUserID, CreatedDate,&#xA;LastModifiedByUserID, LastModifiedDate)&#xA;SELECT 1, ChargeUID, NULL,&#xA;TotalPaid, 0, 'Synergy Integration Payment',&#xA;Notes, 0, ModifiedDate, 0, ModifiedDate&#xA;FROM _ETL_ChargePayments&#xA;WHERE ChargePaymentUID IS NULL&#xA;AND ChargeUID IS NOT NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE tblUnvCharges&#xA;SET DateSatisfied = CP.DateSatisfied,&#xA;LastModifiedByUserID = 0,&#xA;LastModifiedDate = CP.ModifiedDate&#xA;FROM tblUnvCharges UC&#xA;JOIN _ETL_ChargePayments CP&#xA;ON UC.ChargeUID = CP.ChargeUID&#xA;WHERE CP.ChargePaymentUID IS NULL&#xA;AND CP.DateSatisfied IS NOT NULL&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
      </DTS:Executables>
