= 'Process'&#xA;&#x9;&#xA;&#x9;UPDATE _ETL_StagingChargePayments&#xA;&#x9;SET PaymentNotes = 'Processed'&#xA;&#x9;WHERE PaymentNotes = 'Process'&#xA;END&#xA;&#xA;GO&#xA;&#xA;&#xA;UPDATE _ETL_StagingChargePayments&#xA;SET PaymentAmount = CONVERT(VARCHAR, SJ.Amount)&#xA;FROM _ETL_StagingChargePayments SP&#xA;JOIN (SELECT MAX(ETLStagingChargePaymentUID) AS ID,&#xA;SUM(CONVERT(MONEY, LTRIM(RTRIM(PaymentAmount)))) AS Amount&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE Accepted = 1&#xA;AND PaymentNotes &lt;&gt; 'Existing'&#xA;GROUP BY StudentID, Accession) SJ ON SP.ETLStagingChargePaymentUID = SJ.ID&#xA;&#xA;GO&#xA;&#xA;&#xA;INSERT INTO dbo._ETL_ChargePayments&#xA;(CampusID,StudentID ,ISBN, Accession, Copies, ChargeAmount,&#xA;TotalPaid,TotalOwed, ModifiedDate,Notes,DateSatisfied)&#xA;SELECT LTRIM(RTRIM(CampusID)), LTRIM(RTRIM(StudentID)), LTRIM(RTRIM(ISBN)), LTRIM(RTRIM(Accession)),&#xA;1, NULL, CONVERT(MONEY, LTRIM(RTRIM(PaymentAmount))), NULL,&#xA;CONVERT(DATETIME, LTRIM(RTRIM([TransactionDate]))),&#xA;CASE WHEN LTRIM(RTRIM(ISNULL(_ETL_StagingChargePayments.Notes, ''))) = '' THEN&#xA;PaymentNotes ELSE&#xA;PaymentNotes + '; Notes: ' + _ETL_StagingChargePayments.Notes END,&#xA;NULL&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE ETLStagingChargePaymentUID IN&#xA;(SELECT MAX(ETLStagingChargePaymentUID) AS ID&#xA;FROM _ETL_StagingChargePayments&#xA;WHERE Accepted = 1&#xA;AND PaymentNotes &lt;&gt; 'Existing'&#xA;GROUP BY StudentID, Accession)&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
