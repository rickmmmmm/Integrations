              SQLTask:Connection="{87797675-BCB8-4B66-B49F-7501EEF3FD2B}"
              SQLTask:SqlStatementSource="  ---remove 'QUOTES' from the ProductName name for etl_purchase table(MK 5/6/2014)&#xA;&#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET ProductName = LTRIM(RTRIM(ProductName))&#xA;&#xA;GO &#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET ProductName = CASE WHEN RIGHT(ProductName, 1) = '.' THEN REPLACE(ProductName, '.', '') &#xA;WHEN  RIGHT(ProductName, 1) = '-' THEN REPLACE(ProductName, '-', '') &#xA;WHEN  RIGHT(ProductName, 1) = ',' THEN REPLACE(ProductName, ',', '') &#xA;WHEN  RIGHT(ProductName, 1) = ':' THEN REPLACE(ProductName, ':', '') &#xA;WHEN  RIGHT(ProductName, 1) = ';' THEN REPLACE(ProductName, ':', '') &#xA;ELSE ProductName  END&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET ProductName = LTRIM(RTRIM(ProductName))&#xA;&#xA;GO &#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET ProductName = REPLACE(ProductName ,CHAR(10),'')&#xA;WHERE  LEFT(ProductName, 1) = CHAR(10) ---CHAR(10) = Line Feed (\n) &#xA;&#xA;Go&#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET ProductName = CASE WHEN RIGHT(ProductName, 1) = '.' THEN REPLACE(ProductName, '.', '') &#xA;WHEN  RIGHT(ProductName, 1) = '-' THEN REPLACE(ProductName, '-', '') &#xA;WHEN  RIGHT(ProductName, 1) = ',' THEN REPLACE(ProductName, ',', '') &#xA;WHEN  RIGHT(ProductName, 1) = ':' THEN REPLACE(ProductName, ':', '') &#xA;WHEN  RIGHT(ProductName, 1) = ';' THEN REPLACE(ProductName, ':', '') &#xA;ELSE ProductName  END&#xA;&#xA;&#xA;GO &#xA;&#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET model  = NULL&#xA;WHERE model IN ('',  'N/A' ,'NONE')&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;UPDATE dbo._ETL_Staging&#xA;SET accept = 0,  note = CASE WHEN note IS NULL THEN 'Product Name was not provided.'&#xA; ELSE note + '; ' +  'Product Name was not provided.' END &#xA;WHERE ProductName = ''&#xA;&#xA;Go&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
