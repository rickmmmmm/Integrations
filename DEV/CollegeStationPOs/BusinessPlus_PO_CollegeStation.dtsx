CAST(SiteID AS varchar(255)) AS SiteID,
SUM(CAST(POShipQuantity AS decimal(10,2))) AS TotaQTYShip,
SUM(CAST(POReceiveQuantity AS decimal(10,2))) AS TotalQTYRec,
SUM(CAST(POOrderQuantity AS decimal(10,2))) AS TotalQTYOrderd,
'Hayes Integration BusinessPlus - ' + CAST(CAST(GETDATE() AS DATE) AS VARCHAR(50)) AS Notes

FROM BusinessPlus.StagingCorpChristi
WHERE Exception = 0
AND CAST(ISNULL(PONumber,'') AS varchar(255)) != ''

GROUP BY CAST(PONumber AS varchar(255)),ProductName,CAST(ProductType AS varchar(255)),CAST(Model AS varchar(255)),
