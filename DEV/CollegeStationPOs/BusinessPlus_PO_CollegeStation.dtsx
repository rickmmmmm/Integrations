                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT CAST(PONumber AS varchar(255)) AS PurchaseNumber,
MIN(CAST(PODate AS DATE)) AS PurchaseDate,
CAST(Manufacturer AS varchar(255)) AS Manufacturer,
CAST(ProductName AS varchar(255)) AS ProductName,
CAST(ProductType AS varchar(255)) AS ProductType,
CAST(Model AS varchar(255)) AS Model,
CAST(CASE WHEN ISNULL(ItemSuggestedPrice,'') = '' THEN '0.00' ELSE ItemSuggestedPrice END AS money) AS ItemSuggestedPrice,
CAST(CASE WHEN ISNULL(POAmount,'') = '' THEN '0.00' ELSE POAmount END AS money) AS PurchasePrices,
