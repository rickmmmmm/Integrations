CASE WHEN ISNULL(AccountCode,'') = '' THEN NULL ELSE AccountCode END,FundingSource,DepartmentID,
CAST(CASE WHEN ISNULL(ItemSuggestedPrice,'') = '' THEN '0.00' ELSE ItemSuggestedPrice END AS money),
CAST(SiteID AS varchar(255)),
CAST(Manufacturer AS varchar(255))

HAVING SUM(CAST(POShipQuantity AS decimal(10,2))) &gt; 0</property>
