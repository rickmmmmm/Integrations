                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT CAST(PONumber AS varchar(255)) AS PurchaseNumber,
MIN(CAST(PODate AS DATE)) AS PurchaseDate,
CAST(ProductName AS varchar(255)) AS ProductName,
CAST(CASE WHEN ISNULL(POAmount,'') = '' THEN '0.00' ELSE POAmount END AS money) AS PurchasePrices,
CAST(CASE WHEN ISNULL(AccountCode,'') = '' THEN NULL ELSE AccountCode END AS VARCHAR(255)) AS AccountCode,
CAST(FundingSource AS varchar(255)) AS FundingSource,
CAST(DepartmentID AS varchar(255)) AS DepartmentID,
CAST(SiteID AS varchar(255)) AS SiteID,
COUNT(*) AS TotalQTYRec,
COUNT(*) AS TotalQTYOrderd,
'Hayes Integration eFinancePLUS - ' + CAST(CAST(GETDATE() AS DATE) AS VARCHAR(50)) AS Notes

FROM eFinancePLUS.StagingPO
WHERE Exception = 0
AND CAST(ISNULL(PONumber,'') AS varchar(255)) != ''

GROUP BY CAST(PONumber AS varchar(255)),ProductName,CASE WHEN ISNULL(POAmount,'') = '' THEN '0.00' ELSE POAmount END,
CASE WHEN ISNULL(AccountCode,'') = '' THEN NULL ELSE AccountCode END,FundingSource,DepartmentID,CAST(SiteID AS varchar(255))</property>
