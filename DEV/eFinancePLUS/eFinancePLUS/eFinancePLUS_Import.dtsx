              SQLTask:SqlStatementSource="&#xA;-- Purchase Date&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Date is invalid' ELSE ExceptionReason + ',PO Date is invalid' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.PODate,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='Purchase Date' &#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;&#xA;-- PO Number&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'PO Number is invalid' ELSE ExceptionReason + ',PO Number is invalid' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.PONumber,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='Purchase Order'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Tag is NULL&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Tag is Null' ELSE ExceptionReason + ',Tag is Null' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.Tag,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='Tag - is Null'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Tag is Dup&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Tag is Duplicated' ELSE ExceptionReason + ',Tag is Duplicated' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.Tag,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='Tag - is Dupped'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- DepartmentID does not exists&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Department ID does not exists in TIPWeb' ELSE ExceptionReason + ',Department ID does not exists in TIPWeb' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.DepartmentID,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='Department ID'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- SiteID does not exists&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Site ID does not exists in TIPWeb' ELSE ExceptionReason + ',Site ID does not exists in TIPWeb' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.SiteID,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='SiteID'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- Product Name is not Vaild&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'Product Name is not Valid' ELSE ExceptionReason + ',Product Name is not Valid' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.ProductName,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='Product Name'&#xA;AND A.CustomerCode=B.CustomerCode&#xA;&#xA;-- AssetID is Dup&#xA;UPDATE A&#xA;SET Exception=1,&#xA;    ExceptionReason=CASE WHEN ISNULL(ExceptionReason,'') = '' THEN 'AssetID is Duplicated' ELSE ExceptionReason + ',AssetID is Duplicated' END &#xA;FROM eFinancePLUS.StagingPO AS A&#xA;INNER JOIN  eFinancePLUS.StagingPOExceptions AS B&#xA;ON ISNULL(A.Tag,'')=ISNULL(B.ExceptionValue,'') AND B.ExceptionColumn='AssetID - is Dupped'&#xA;AND A.CustomerCode=B.CustomerCode" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
