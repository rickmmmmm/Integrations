    'To open Help, press F1.

    Public Sub Main()
        Dim ReportPath As String = CType(Dts.Variables("ReportsPath").Value, String)


        Dim httpConn As ConnectionManager = Dts.Connections("ReportServer")
        Dim clientConn As HttpClientConnection = New HttpClientConnection(httpConn.AcquireConnection(Nothing))

        Dim ReportFileName As String = ReportPath + "Oracle_Exception_Report_" + Format(Now, "MMddyyyy") + ".xls"

        clientConn.ServerURL = "http://hayessqlsvr/ReportServer/Pages/ReportViewer.aspx?%2fDataIntergration%2fOracle%2fOracleExceptionNewReport&rs:Command=Render&rs:Format=Excel&rc:Toolbar=False"



        clientConn.DownloadFile(ReportFileName, True)
        Dts.TaskResult = ScriptResults.Success
    End Sub

