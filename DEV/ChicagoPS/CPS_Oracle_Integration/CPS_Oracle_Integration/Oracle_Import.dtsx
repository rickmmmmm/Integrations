                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT CAST(PO_NUMBER AS varchar(50)) AS PO_NUMBER,CAST(LINE_NUM AS INT) AS PO_LINE_ID,
SUBSTRING(LTRIM(RTRIM(PO_ITEM_DESCRIPTION)),1,98) AS PO_ITEMName,
REPLACE(LTRIM(RTRIM(PO_ITEM_DESCRIPTION)),'''','') AS PO_ITEM_DESCRIPTION,
CATEGORY_CODE,CATEGORY_DESCRIPTION,PO_LINE_QUANTITY,
CAST(PO_UNIT_PRICE AS DECIMAL(10,2)) AS PO_UNIT_PRICE,CFDA_NUMBER,
GL_SEGMENTS,'District' AS SiteName,PO_LINE_QUANTITY AS QUANTITYORDERED,PO_LINE_QUANTITY AS QUANTITYRECIEVED,CASE WHEN CLOSED_CODE='CLOSED' THEN 33 ELSE 32 END AS StatusID,
ISNULL(C.TargetValue,CAST(SUBSTRING(GL_SEGMENTS,CHARINDEX('.',GL_SEGMENTS)+1,((CHARINDEX('.',GL_SEGMENTS,CHARINDEX('.',GL_SEGMENTS)+1) - CHARINDEX('.',GL_SEGMENTS)) -1)) AS varchar(50))) AS FundingSource,
CAST(ISNULL(B.TargetValue,SUBSTRING(GL_SEGMENTS,1,CHARINDEX('.',GL_SEGMENTS)-1)) AS VARCHAR(50)) AS DepartmentID,'Unspecified' AS ItemType,
GETDATE() AS CreateDate,GETDATE() AS ModifedDate
FROM Oracle.StagingPO AS A

LEFT JOIN ColumnLookupRemaping AS B
ON SUBSTRING(A.GL_SEGMENTS,1,CHARINDEX('.',A.GL_SEGMENTS)-1)=B.SourceValue AND B.CustomerCode='CPS' AND B.ColumnName='Departments'

LEFT JOIN ColumnLookupRemaping AS C
ON SUBSTRING(GL_SEGMENTS,CHARINDEX('.',GL_SEGMENTS)+1,((CHARINDEX('.',GL_SEGMENTS,CHARINDEX('.',GL_SEGMENTS)+1) - CHARINDEX('.',GL_SEGMENTS)) -1))=C.SourceValue AND C.CustomerCode='CPS' AND C.ColumnName='FundingSource'

WHERE Exception = 0</property>
