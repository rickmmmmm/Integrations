        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            // Paths 
            bool Debug = Convert.ToBoolean(Dts.Variables["Debug"].Value);
            string FilePath = Dts.Variables["FilePath"].Value.ToString();
            string ArchivePath = Dts.Variables["FileArchivePath"].Value.ToString();

            // File Path appending last hash
            if (FilePath.Substring(FilePath.Length - 1, 1) != "\\")
            {
                FilePath = FilePath + "\\";
            }
            if (ArchivePath.Substring(ArchivePath.Length - 1, 1) != "\\")
            {
                ArchivePath = ArchivePath + "\\";
            }

            ArchivePath = FilePath + ArchivePath;

            if (Debug == true)
            {
                MessageBox.Show("FilePath: " + FilePath.ToString());
                MessageBox.Show("ArchivePath: " + ArchivePath.ToString());
            }

            // Does Path exists?
            bool DirectoryExists = Directory.Exists(ArchivePath);


            DirectoryInfo dirInfo = new DirectoryInfo(ArchivePath);
            if (dirInfo.Exists == false)
                Directory.CreateDirectory(ArchivePath);

            List<String> ImportedFiles = Directory
                               .GetFiles(FilePath, "*.*", SearchOption.AllDirectories).ToList();

            foreach (string file in ImportedFiles)
            {
                FileInfo mFile = new FileInfo(file);
                // to remove name collisions
                if (new FileInfo(dirInfo + "\\" + mFile.Name).Exists == false)
                {
                    mFile.MoveTo(dirInfo + "\\" + mFile.Name);
                }
            }

            Dts.TaskResult = (int)ScriptResults.Success;
