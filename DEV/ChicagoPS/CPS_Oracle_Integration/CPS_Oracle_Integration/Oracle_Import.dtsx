                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @CreatedByUserID	INT = ?
DECLARE @ModifiedByUserID	INT = ?
DECLARE @SerialRequired		BIT = ?
DECLARE @ProjectedLife		INT = ?
DECLARE @Active				BIT = ?
DECLARE @ProductName		VARCHAR(100) = ?
DECLARE @ProductDiscript	VARCHAR(100) = ?
DECLARE @ItemTypeUID		INT = ?
DECLARE @ManufacturerUID	INT = ?
DECLARE @ModelNumber		VARCHAR(100) = ?
DECLARE @ItemNotes			VARCHAR(100) = ?
DECLARE @SuggestedPrice		DECIMAL(10,2) = ?
DECLARE @NexitItemNumber	VARCHAR(50)
DECLARE @ItemNumberChecked  BIT = 0

SELECT @NexitItemNumber= VALUE
FROM tblUnvCounter 
WHERE CounterUID = 4

WHILE @ItemNumberChecked = 0
BEGIN
	IF EXISTS (SELECT ItemNumber FROM tblTechItems WITH (NOLOCK) WHERE REPLACE(REPLACE(ItemNumber,'H',''),'DC','') = @NexitItemNumber) 
	BEGIN
		SET @NexitItemNumber=@NexitItemNumber+1
	END
	ELSE
	BEGIN
		SET @ItemNumberChecked = 1
	END
END


INSERT INTO tblTechItems
(ItemNumber,ItemName,ItemDescription,ItemNotes,ItemSuggestedPrice,ItemTypeUID,ModelNumber,ManufacturerUID,SerialRequired,ProjectedLife,Active,CreatedByUserID,LastModifiedByUserID)
VALUES
(@NexitItemNumber,@ProductName,@ProductDiscript,@ItemNotes,@SuggestedPrice,@ItemTypeUID,@ModelNumber,@ManufacturerUID,@SerialRequired,@ProjectedLife,@Active,@CreatedByUserID,@ModifiedByUserID)

UPDATE tblUnvCounter 
SET VALUE = @NexitItemNumber
WHERE CounterUID = 4


