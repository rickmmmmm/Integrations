FROM (
SELECT DISTINCT CAST(PONumber AS varchar(255)) AS PurchaseOrder,
CAST(PODate AS DATE) AS PODate,
CAST(ISNULL(VendorName,'Unassigned') AS varchar(255))  AS VendorName,
POStatus,
CASE WHEN ISNULL(PONotes,'') = '' THEN 'Hayes Integration BusinessPlus - ' + CAST(CAST(GETDATE() AS DATE) AS VARCHAR(50)) ELSE PONotes + ' - Hayes Integration BusinessPlus- ' + CAST(CAST(GETDATE() AS DATE) AS VARCHAR(50)) END AS Notes,
GETDATE() AS CreateDate,
GETDATE() AS ModifiedDate,
ROW_NUMBER() OVER(PARTITION BY CAST(PONumber AS varchar(255)) ORDER BY CAST(PODate AS DATE) ASC) AS RowNumber

FROM BusinessPlus.StagingCorpChristi
WHERE Exception = 0
AND CAST(ISNULL(PONumber,'') AS varchar(255)) != ''


) AS TB

WHERE RowNumber = 1</property>
