              SQLTask:Connection="{F09CF0E7-06B6-4812-AFD7-779F6C51DCCE}"
              SQLTask:SqlStatementSource="DECLARE @body nvarchar(max), @subject NVARCHAR(max), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX), @mailProfile NVARCHAR(MAX)&#xA;DECLARE @rpt Nvarchar(max), @CustomerName Nvarchar(max),  @sLabel nvarchar(max), @logdate NVARCHAR(50) &#xA;DECLARE @ProcessDB NVARCHAR(max), @sSql Nvarchar(max), @total INT, @processed INT, @reject INT &#xA;DECLARE @ParamDef NVARCHAR(max)&#xA;&#xA;SET @ProcessDB = ?&#xA;SET @CustomerName = ?&#xA;SET @email = ?&#xA;SET @ccemail = ? &#xA;SET @rpt = ?&#xA;SET @mailProfile = ?&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'User List Processing Log for ' + @CustomerName + ' (' + @logdate + ')'&#xA;&#xA;&#xA;/*&#xA;Run Total, Accept, Reject for each module as it is developed&#xA;*/&#xA;---------------------- Stats --------------------------&#xA;--Total&#xA;set @sSql = N'SELECT @total = COUNT(*) FROM ' + @ProcessDB + '.dbo.LDAP_UserList '&#xA;set @ParamDef = N'@CustomerName Nvarchar(max), @total int OUTPUT'&#xA;exec sp_executesql @sSql,  @ParamDef, @CustomerName=@CustomerName, @total=@total OUTPUT &#xA;--Processed &#xA;set @sSql = N'SELECT @processed= COUNT(*) FROM ' + @ProcessDB + '.dbo.LDAP_UserList where import = 1'&#xA;set @ParamDef = N'@CustomerName Nvarchar(max), @processed int OUTPUT'&#xA;exec sp_executesql @sSql,  @ParamDef, @CustomerName=@CustomerName, @processed=@processed OUTPUT &#xA;--import &#xA;set @sSql = N'SELECT @reject = COUNT(*) FROM ' + @ProcessDB + '.dbo.LDAP_UserList where import = 0'&#xA;set @ParamDef = N'@CustomerName Nvarchar(max), @reject int OUTPUT'&#xA;exec sp_executesql @sSql,  @ParamDef, @CustomerName=@CustomerName, @reject=@reject OUTPUT &#xA;&#xA;set @sLabel = '&lt;b&gt;&lt;i&gt;User List: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;' Total Records Received: ' + CONVERT(VARCHAR(50), @total) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Processed: ' + CONVERT(VARCHAR(50), @processed) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;' The list of reject records is attached. &lt;br/&gt;&lt;br/&gt;'&#xA;&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;'&lt;p&gt;' + @sLabel + '&lt;/p&gt;&lt;br/&gt;' +&#xA;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9; '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;&#x9; '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;&#xA;EXEC msdb.dbo.sp_send_dbmail&#xA;    @profile_name = @mailProfile,&#xA;    @recipients =@email,&#xA;    @copy_recipients = @ccemail,&#xA;    @body_format = 'HTML',&#xA;    @body = @body, &#xA;    @subject =@subject, &#xA;    @file_attachments = @rpt ;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
