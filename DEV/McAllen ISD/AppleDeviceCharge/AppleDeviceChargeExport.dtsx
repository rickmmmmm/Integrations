                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
cast(chg.ChargeUID as varchar(50)) as FineID, sd.StudentID, items.ItemName, chg.UniversalID as ItemBarCode, 
chgt.Name  as ItemCollection, 
camp.SiteID as FineLocationCode, 
replace(replace(replace(isNull(chg.[Notes], ''), char(10), ''), char(13), ''), char(9), ' ') as Notes,
convert(varchar(10), chg.CreatedDate, 101) as DateCreated,
cast((chg.ChargeAmount - (ISNULL((SELECT SUM(ISNULL(pmt.ChargeAmount,0)) FROM tblUnvChargePayments pmt WHERE pmt.ChargeUID = chg.ChargeUID and void=0),0))) as varchar(50)) as Amount
FROM tblUnvCharges chg 
JOIN tblUnvChargeTypes chgt ON chgt.ChargeTypeUID = chg.[ChargeTypeUID] 
JOIN tblStudents sd on chg.EntityUID = sd.StudentsUID and chg.entitytypeuid = 4 
JOIN tblTechSites camp on camp.SiteID = sd.CampusID 
JOIN tblTechItems items on items.ItemUID = chg.ItemUID 
WHERE chg.ApplicationUID = 2
and EntityTYpeUID = 4
and chg.Void = 0
and chg.DateSatisfied is null
and chg.ChargeAmount &gt; isNull((select sum(ChargeAmount) from tblUnvChargePayments where chargeUID = chg.ChargeUID and void = 0), 0) --how the application does it
and chgt.chargetypeuid in (
	select chargetypeuid 
	from tblUnvChargeTypes

	where name in ('APPLE IPAD ADAPTER', 'APPLE IPAD REPLACEMENT', 'APPLE IPAD USB','APPLE IPAD USB AND ADAPTER',
	'CHROMEBOOK 300E CASE','CHROMEBOOK 300E CHARGER','CHROMEBOOK 300E REPLACEMENT','IPAD AND CASE REPLACEMENT','IPAD CASE REPLACEMENT',
	'APPLE AIR USB (EOL)','APPLE IPAD AIR COMBO USB&amp;ADAPTER (EOL)','APPLE IPAD AIR POWER ADAPTER (EOL)','APPLE IPAD AIR REPLACEMENT (EOL)',
	'APPLE IPAD AIR RUG-ED CASE (EOL)','CHROMEBOOK N-20 CASE (EOL)','CHROMEBOOK N-20 POWER ADAPTER (EOL)','CHROMEBOOK N-20 REPLACEMENT (EOL)',
	'CHROMEBOOK N-20-INTENTIONAL DAMAGE (EOL)','REFUND SECURITY DEPOSIT')
)</property>
