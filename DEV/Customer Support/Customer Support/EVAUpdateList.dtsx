<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{B9BC99FA-EC08-4F11-B05B-2DCF3C89447D}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="&#xA;TRUNCATE TABLE [EZImportProcess].[dbo].[EVAUpdateReport]&#xA;&#xA;/* Start Clean */&#xA;IF OBJECT_ID (N'tempdb.dbo.#TempTblList') IS NOT NULL&#xA;   DROP TABLE #TempTblList&#xA;&#xA;IF OBJECT_ID (N'tempdb.dbo.#TempUpdateReport') IS NOT NULL&#xA;   DROP TABLE #TempUpdateReport&#xA;/*&#xA;Build a Temporary Table of Database Names&#xA;*/&#xA;&#xA;CREATE TABLE #TempTblList&#xA;(&#xA;   DB VARCHAR(250)&#xA;)&#xA;GO&#xA;&#xA;INSERT #TempTblList (DB)&#xA;--List of standard database names where naming convention in TipWeb matches naming convention in MP databases&#xA;SELECT tw.twDBName &#xA;FROM sys.databases mp&#xA;JOIN (SELECT name AS twDBName, CASE WHEN name LIKE  'tipwebIThosted%' THEN &#xA;	SUBSTRING(name, 15, 200)&#xA;	ELSE&#xA;	SUBSTRING(name, 13, 200)&#xA;	END AS sTWClientName&#xA;  FROM sys.databases &#xA;WHERE name LIKE 'tipwebhosted%' OR  name LIKE 'tipwebIThosted%'&#xA;) tw&#xA;	ON sTWClientName =  SUBSTRING(Name, 5, 200)&#xA;WHERE name LIKE 'mpdb%'&#xA;and name not in ('MPdbTest')&#xA;GO&#xA;&#xA;INSERT #TempTblList (DB)&#xA;--List of non-standard database names&#xA;--After break line, db list not in MP&#xA;SELECT name FROM sys.databases WHERE name IN &#xA;	('TipWebHostedAnaheimUnionHSD','TipWebHostedCollegeStationCISD','TipWebHostedCypressFairbanks', 'TipWebHostedCypressFairbanksIM',&#xA;	'TipWebSingleSiteDenisonHS', 'TipWebHostedFtSamHouston', 'TipWebHostedHaysConsolidated', 'TipWebHostedLittleRock','TipWebSingleSiteMartinsvilleISD',&#xA;	'TipWebHostedMesaCounty','TipWebITHostedMiddletown','TipWebITHostedNeoshoR5Schools','TipWebSingleSiteNewDianaHS','TipWebHostedPharrSanJuan',&#xA;	'TipWebITHostedPulaskiCS','TipWebSingleSiteRandolphFieldsISD', 'TipWebHostedSpringBranchISD','TipWebHostedWhitehouseISD',&#xA;	--&#xA;	'TipWebHostedAnthonyISD', 'TipWebHostedBorgerISD', 'TipWebHostedBrownwoodISD', 'TipWebITHostedBryanISD', 'TipWebHostedCalhounCountyISD',&#xA;	'TipWebITHostedCotullaISD', 'TipWebSingleSiteCurtisHS', 'TipWebITHostedFulton58', 'TipWebITHostedGalvestonBrazoria','TipWebHostedGladeWaterISD',&#xA;	'TipWebSingleSiteKennedaleHS', 'TipWebHostedPoteetISD', 'TipWebHostedWarrenCitySchools', 'TipWebSingleSiteRalphMcKeeHS'	&#xA;	) &#xA;&#xA;&#xA;DELETE #TempTblList WHERE DB IN ('TipWebHostedCypressFairbanksIM')  -- they use EZImport for IM&#xA;	&#xA;--SELECT * FROM #TempTblList&#xA;--GO&#xA;&#xA;CREATE TABLE #TempUpdateReport&#xA;(      [LatestDate] DATETIME&#xA;		,[DistDistrDate] datetime&#xA;		,[StuDistrDate] DATETIME&#xA;		,[TechInvDate] DATETIME&#xA;	   ,DB VARCHAR(250)&#xA;)&#xA;GO	&#xA;--		&#xA;&#xA;--- Create Updated Query&#xA;&#xA;DECLARE @TableName NVARCHAR(50) &#xA;SELECT @TableName = '[dbo].[tblStudents]'&#xA;&#xA;declare @ModiDate NVarchar(50)&#xA;SELECT @ModiDate = ' (select top 1 modifieddate '&#xA;&#xA;declare @DistrDist NVarchar(75)&#xA;SELECT @DistrDist = '[dbo].[tblDistrictDistributions] order by modifieddate desc) '&#xA;&#xA;declare @StuDist NVarchar(75)&#xA;SELECT @StuDist = '[dbo].[tblDistrictDistributions] order by modifieddate desc) '&#xA;&#xA;declare @TechInv1 NVarchar(50)&#xA;select @TechInv1 = ' (select top 1 LastModifiedDate ' &#xA;&#xA;declare @TechInv2 NVarchar(75)&#xA;select @TechInv2 = '[dbo].[tblTechInventory] order by LastModifiedDate desc) '&#xA;&#xA;&#xA;&#xA;&#xA;DECLARE @SQL NVARCHAR(MAX)&#xA;SELECT @SQL = STUFF((&#xA;    SELECT CHAR(13) +&#xA;    &#xA;'SELECT top 1 ''' + name + ''', modifiedDate, ' +&#xA;@ModiDate + 'FROM [' + Name + '].' + @DistrDist + ' as DistDistrDate, ' +&#xA;@ModiDate + 'FROM [' + Name + '].' + @StuDist + ' as StudDistrDate, ' +&#xA;@TechInv1 + 'FROM [' + Name + '].' + @TechInv2 + ' as TechInvDate ' +&#xA;' FROM [' + Name + '].' + @TableName + ' where UserID = 3 order by ModifiedDate desc '&#xA;&#xA;FROM sys.databases WHERE name IN (SELECT DB FROM #TempTblList)&#xA;FOR XML PATH(''), TYPE).value('.', 'NVARCHAR(MAX)'), 1, 1, '')&#xA;&#xA;INSERT INTO #TempUpdateReport (DB, [LatestDate], [DistDistrDate], [StuDistrDate], [TechInvDate])              &#xA;EXEC sys.sp_executesql @SQL&#xA;&#xA;INSERT INTO [EZImportProcess].[dbo].[EVAUpdateReport]&#xA;           ([EVADBName],[StudentUpdate],[NumDays],[DistDistrDate],[StuDistrDate],[TechInvDate])&#xA;(SELECT DB, LatestDate, DATEDIFF(day, LatestDate, GETDATE()) AS DaysPassed, [DistDistrDate], [StuDistrDate], [TechInvDate]&#xA;FROM #TempUpdateReport t&#xA;)&#xA;&#xA;&#xA;DROP TABLE #TempTblList&#xA;DROP TABLE #TempUpdateReport&#xA;&#xA;	" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
