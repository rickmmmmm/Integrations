                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select AuditName, SiteID, SiteName, ApprovedDate, FirstScanDate, LastScanDate, numEntities AS numLocations, numNewEntities AS numNewLocations, numInProgressEntities AS numInProgress, 
numFinalizedEntities AS numFinalizedLocations, numClosedEntities AS numClosedLocations, 
numTags, numVerified, numMissing, numMisplaced,
	(CASE	WHEN audList.Approved = 1 THEN 101 + (
		(CASE WHEN (numMisplaced + numMissing) &gt; 100 THEN FLOOR(((CONVERT(Float,(numMisplaced + numMissing))) / (CONVERT(Float,numMisplaced + numMissing))) * 100) ELSE 100  END))
		WHEN COALESCE(numEntities, 0) &lt;&gt; 0 THEN round((((CONVERT(Float,COALESCE(numClosedEntities,0))) + (CONVERT(Float,COALESCE(numFinalizedEntities,0)))) / (CONVERT(Float,numEntities))) * 100, 0)
		ELSE 0 END) AS perCentage
from (
	select aud.AuditUID, aud.AuditName, aud.Approved, det.SiteUID, s.SiteID, s.SiteName, aud.ApprovedDate, MIN(cnt.ScanDate) AS [FirstScanDate], MAX(cnt.ScanDate) AS LastScanDate,
		(SELECT COUNT(*) FROM [dbo].[tblTechAuditDetails] WHERE ([AuditUID] = aud.[AuditUID])) as numEntities,
		(SELECT COUNT(*) FROM [dbo].[tblTechAuditDetails] WHERE ([StatusID] = 53) AND ([AuditUID] = aud.[AuditUID])) as numNewEntities, --New Entities (53)
		(SELECT COUNT(*) FROM [dbo].[tblTechAuditDetails] WHERE ([StatusID] = 54) AND ([AuditUID] = aud.[AuditUID])) AS numInProgressEntities, --In Progress Entities (54)
		(SELECT COUNT(*) FROM [dbo].[tblTechAuditDetails] WHERE ([StatusID] = 55) AND ([AuditUID] = aud.[AuditUID]) ) AS numFinalizedEntities, -- Finalized Entities (55)
		(SELECT COUNT(*) FROM [dbo].[tblTechAuditDetails] WHERE ([StatusID] = 56) AND ([AuditUID] = aud.[AuditUID])) AS numClosedEntities, --Closed Entities (56)

		(SELECT COUNT(*) FROM dbo.tblTechAuditDetailInventoryCounts c
		JOIN dbo.tblTechAuditDetails d ON c.AuditDetailUID = d.AuditDetailUID
		JOIN dbo.tblUnvAudits a ON a.AuditUID = d.AuditUID
		WHERE (c.StatusID &lt;&gt; 98) AND (a.AuditUID = aud.AuditUID) AND d.SIteUID = det.SIteUID) as numTags,
		(SELECT COUNT(*) FROM dbo.tblTechAuditDetailInventoryCounts c
		JOIN dbo.tblTechAuditDetails d ON c.AuditDetailUID = d.AuditDetailUID
		JOIN dbo.tblUnvAudits a ON a.AuditUID = d.AuditUID
		WHERE (c.StatusID = 52) AND (a.AuditUID = aud.AuditUID)and d.SIteUID = det.SIteUID) as numVerified,
		(SELECT COUNT(*) FROM dbo.tblTechAuditDetailInventoryCounts c
		JOIN dbo.tblTechAuditDetails d ON c.AuditDetailUID = d.AuditDetailUID
		JOIN dbo.tblUnvAudits a ON a.AuditUID = d.AuditUID
		WHERE (c.StatusID = 50) AND (a.AuditUID = aud.AuditUID)and d.SIteUID = det.SIteUID) as numMissing,
		(SELECT COUNT(*) FROM dbo.tblTechAuditDetailInventoryCounts c
		JOIN dbo.tblTechAuditDetails d ON c.AuditDetailUID = d.AuditDetailUID
		JOIN dbo.tblUnvAudits a ON a.AuditUID = aud.AuditUID
		WHERE (c.StatusID = 51) AND (a.AuditUID = d.AuditUID)and d.SIteUID = det.SIteUID) as numMisplaced
	from tblUnvAudits aud
	join tblTechAuditDetails det on aud.AuditUID = det.AuditUID
	join tblTechSites s on det.SiteUID = s.SiteUID
	join tblTechAuditDetailInventoryCounts cnt on det.AuditDetailUID = cnt.AuditDetailUID
	WHERE (aud.CreatedDate &gt; '04/16/2019') 
	group by aud.AuditUID, aud.AuditName, aud.Approved, det.SiteUID, s.SiteID, s.SiteName, aud.ApprovedDate
	) audList</property>
