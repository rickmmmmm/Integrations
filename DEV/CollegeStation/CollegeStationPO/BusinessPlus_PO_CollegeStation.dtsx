                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT CAST(csspo.PO_NUMBER AS varchar(255)) AS PurchaseNumber,
MIN(CAST(csspo.PO_DATE AS DATE)) AS PurchaseDate,
CAST(csspo.MANUFACTURER AS varchar(255)) AS Manufacturer,
CAST(csspo.PRODUCT_NAME AS varchar(255)) AS ProductName,
CAST(csspo.PRODUCT_TYPE AS varchar(255)) AS ProductType,
CAST(csspo.MODEL AS varchar(255)) AS Model,
CAST(CASE WHEN ISNULL(csspo.PURCHASE_PRICE,'') = '' THEN '0.00' ELSE csspo.PURCHASE_PRICE END AS money) AS ItemSuggestedPrice,
CAST(CASE WHEN ISNULL(csspo.PURCHASE_PRICE,'') = '' THEN '0.00' ELSE csspo.PURCHASE_PRICE END AS money) AS PurchasePrices,
CAST(CASE WHEN ISNULL(csspo.ACCOUNT_CODE,'') = '' THEN NULL ELSE csspo.ACCOUNT_CODE END AS VARCHAR(255)) AS AccountCode,
CAST(csspo.FUNDING_SOURCE AS varchar(255)) AS FundingSource,
CAST(csspo.SHIPPEDTOSITE AS varchar(255)) AS DepartmentID,
CAST(CASE WHEN ISNULL(csspo.SHIPPEDTOSITE, '') = '' THEN 'District' ELSE csspo.SHIPPEDTOSITE END AS varchar(255)) AS SiteID,
SUM(CAST(csspo.QUANTITYSHIPPED AS decimal(10,2))) AS TotaQTYShip,
SUM(CAST(csspo.QUANTITY AS decimal(10,2))) AS TotalQTYRec,
SUM(CAST(0 AS decimal(10,2))) AS TotalQTYOrderd,
'Hayes Integration Purchasing - ' + CAST(CAST(GETDATE() AS DATE) AS VARCHAR(50)) AS Notes

FROM [dbo].[_ETL_StagingPurchaseOrders] as csspo
WHERE Exception = 0
AND CAST(ISNULL(csspo.PO_NUMBER,'') AS varchar(255)) != ''

GROUP BY CAST(csspo.PO_NUMBER AS varchar(255)),CAST(csspo.PRODUCT_NAME AS varchar(255)),CAST(csspo.PRODUCT_TYPE AS varchar(255)),
CAST(csspo.MODEL AS varchar(255)),
CAST(CASE WHEN ISNULL(csspo.PURCHASE_PRICE,'') = '' THEN '0.00' ELSE csspo.PURCHASE_PRICE END AS money),
CAST(CASE WHEN ISNULL(csspo.ACCOUNT_CODE,'') = '' THEN NULL ELSE csspo.ACCOUNT_CODE END AS VARCHAR(255)),
CAST(csspo.FUNDING_SOURCE AS varchar(255)),
CAST(CASE WHEN ISNULL(csspo.SHIPPEDTOSITE, '') = '' THEN 'District' ELSE csspo.SHIPPEDTOSITE END AS varchar(255)),
CAST(CASE WHEN ISNULL(csspo.PURCHASE_PRICE,'') = '' THEN '0.00' ELSE csspo.PURCHASE_PRICE END AS money),
CAST(csspo.SHIPPEDTOSITE AS varchar(255)),CAST(csspo.MANUFACTURER AS varchar(255))

HAVING SUM(CAST(csspo.QUANTITY AS decimal(10,2))) &gt; 0</property>
