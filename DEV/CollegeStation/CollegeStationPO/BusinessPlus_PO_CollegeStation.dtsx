GETDATE() AS CreateDate,
GETDATE() AS ModifiedDate,
ROW_NUMBER() OVER(PARTITION BY CAST(csspo.PO_NUMBER AS varchar(255)) ORDER BY CAST(csspo.PO_DATE AS DATE) ASC) AS RowNumber
FROM [dbo].[_ETL_StagingPurchaseOrders] as csspo
WHERE Exception = 0
AND CAST(ISNULL(csspo.PO_NUMBER,'') AS varchar(255)) != ''
) AS TB
WHERE RowNumber = 1</property>
