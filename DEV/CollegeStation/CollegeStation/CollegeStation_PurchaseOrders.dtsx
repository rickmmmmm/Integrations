          SQLTask:SqlStatementSource="UPDATE PO&#xA;SET&#xA;PO.PurchaseUID = TTP.PurchaseUID&#xA;FROM _ETL_StagingPurchases PO LEFT JOIN tblTechPurchases TTP on (PO.PO_NUMBER = TTP.OrderNumber)&#xA;&#xA;&#xA;UPDATE PO&#xA;SET&#xA;PO.VendorUID = VDR.VendorID&#xA;FROM _ETL_StagingPurchases PO LEFT JOIN tblVendor VDR on (PO.VENDOR_NAME = VDR.VendorName)&#xA;&#xA;UPDATE PO&#xA;SET&#xA;PO.ItemUID = TTI.ItemUID&#xA;FROM _ETL_StagingPurchases PO LEFT JOIN tblTechItems TTI on (PO.PRODUCT_NAME = TTI.ItemName)&#xA;&#xA;UPDATE PO&#xA;SET&#xA;PO.PurchaseItemDetailUID = TPID.PurchaseItemDetailUID&#xA;FROM _ETL_StagingPurchases PO LEFT JOIN tblTechPurchaseItemDetails TPID on (PO.PurchaseUID = TPID.PurchaseUID and PO.ItemUID = TPID.ItemUID)&#xA;&#xA;UPDATE PO&#xA;SET&#xA;PO.FundingSourceUID = FS.FundingSourceUID&#xA;FROM _ETL_StagingPurchases PO LEFT JOIN tblFundingSources FS on (PO.FUNDING_SOURCE = FS.FundingSource)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
