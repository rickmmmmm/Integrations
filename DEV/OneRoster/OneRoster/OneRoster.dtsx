		ContentType	    = 'application/x-www-form-urlencoded'
		UseBasicParsing = $true
	}
	
	$access = Invoke-RestMethod @paramInvokeRestMethod
	
	if ($access.access_token)
	{
		return $access
	}
	else
	{
		throw "No access token returned"
	}
}

