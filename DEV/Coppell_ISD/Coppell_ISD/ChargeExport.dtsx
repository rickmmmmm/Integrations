          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{232AD013-DF13-4B89-81A9-1E7DF618229B}"
              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo._ETL_ChargeExport', 'U') IS NOT NULL DROP TABLE dbo._ETL_ChargeExport;&#xA;&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;&#xA;GO&#xA;&#xA;SET QUOTED_IDENTIFIER ON&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;&#xA;CREATE TABLE [dbo]._ETL_ChargeExport (&#xA; [ETLChargeExportUID] [int] IDENTITY(1,1) NOT NULL,&#xA; ChargeUID [varchar](50) NOT NULL,&#xA; StudentID [varchar](50) NOT NULL,&#xA; ItemName [varchar](50) NOT NULL,&#xA; ItemBarCode [varchar](50) NOT NULL,&#xA; ItemCollection [varchar](50) NOT NULL,&#xA; FineLocationCode [varchar](50) NOT NULL,&#xA; Notes [varchar](500) NOT NULL,&#xA; CreatedDate [varchar](50) NOT NULL,&#xA; [ChargeAmount] [varchar](50) NOT NULL,&#xA; CONSTRAINT [PK__ETL_ChargeExport] PRIMARY KEY CLUSTERED &#xA;(&#xA; [ETLChargeExportUID] ASC&#xA;)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
