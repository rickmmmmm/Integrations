        <DTS:Executable
          DTS:refId="Package\Get Data\Insert Data"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{C818EEEA-0606-4AE2-8A17-108156A5DD13}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Insert Data"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{232AD013-DF13-4B89-81A9-1E7DF618229B}"
              SQLTask:SqlStatementSource="&#xA;INSERT INTO [dbo].[_ETL_ChargeExport]&#xA;           ([ChargeUID]&#xA;           ,[StudentID] --50&#xA;&#x9;&#x9;   ,[ItemName] --50&#xA;           ,[ItemBarCode] --50&#xA;           ,[ItemCollection]  --50&#xA;           ,[FineLocationCode] --100&#xA;           ,[Notes]  --max&#xA;           ,[CreatedDate]&#xA;           ,[ChargeAmount]&#xA;&#x9;&#x9;   )&#xA;SELECT &#xA;cast(chg.ChargeUID as varchar(50)) as FineID, sd.StudentID, items.ItemName, chgt.Name as ItemBarCode, &#xA;chgt.Name  as ItemCollection, &#xA;camp.SiteName as FineLocationCode, &#xA;replace(replace(replace(isNull(chg.[Notes], ''), char(10), ''), char(13), ''), char(9), ' ') as Notes,&#xA;convert(varchar(10), chg.CreatedDate, 101) as DateCreated,&#xA;cast((chg.ChargeAmount - (ISNULL((SELECT SUM(ISNULL(pmt.ChargeAmount,0)) FROM tblUnvChargePayments pmt WHERE pmt.ChargeUID = chg.ChargeUID and void=0),0))) as varchar(50)) as Amount&#xA;FROM tblUnvCharges chg &#xA;JOIN tblUnvChargeTypes chgt ON chgt.ChargeTypeUID = chg.[ChargeTypeUID] &#xA;JOIN tblStudents sd on chg.EntityUID = sd.StudentsUID and chg.entitytypeuid = 4 &#xA;JOIN tblTechSites camp on camp.SiteID = sd.CampusID &#xA;JOIN tblTechItems items on items.ItemUID = chg.ItemUID &#xA;WHERE chg.ApplicationUID = 2&#xA;and EntityTYpeUID = 4&#xA;and chg.Void = 0&#xA;and chg.DateSatisfied is null&#xA;and chg.ChargeAmount &gt; isNull((select sum(ChargeAmount) from tblUnvChargePayments where chargeUID = chg.ChargeUID and void = 0), 0) --how the application does it&#xA;&#xA;UNION&#xA;Select distinct 'B'+Cast(DistributionID as varchar(30)) as FineID, StudentID, ItemName, ItemBarCode, ItemCollection,&#xA;&#x9;FineLocationCode, Notes, DateCreated, ChargeAmount as Amount&#xA;from (&#xA;&#x9;SELECT SD.Accession, sd.DistributionID, S.StudentID, substring(BI.Title, 1, 50) as ItemName, SD.Accession as ItemBarcode, 'Textbook' as ItemCollection,&#xA;&#x9;SUBSTRING(S.CampusID, PATINDEX('%[^0]%', S.CampusID),8000) as FineLocationCode, 'Lost Textbook' as Notes,&#xA;&#x9;CONVERT(VARCHAR(10), SD.ModifiedDate, 101) as DateCreated, CONVERT(varchar,BI.Price) as ChargeAmount&#xA;&#x9;FROM tblStudentsDistribution SD&#xA;&#x9;JOIN tblStudents S ON SD.StudentsUID = S.StudentsUID&#xA;&#x9;JOIN tblBookInventory BI ON SD.ISBN = BI.ISBN&#xA;&#x9;LEFT JOIN tblPaymentsCollected PC ON SD.Accession = PC.Accession&#xA;&#x9;AND SD.StudentsUID = PC.ID&#xA;&#x9;LEFT JOIN dbo._ETL_ChargeExport EF ON 'B'+SD.Accession = EF.ChargeUID&#xA;&#x9;WHERE EF.ETLChargeExportUID IS NULL&#xA;&#x9;AND PC.PaymentID IS NULL&#xA;&#x9;AND SD.Code = 'LOST'&#xA;&#x9;and BI.Price &gt; 0&#xA;&#xA;&#x9;UNION&#xA;&#x9;SELECT SD.Accession, sd.DistributionID, S.StudentID, substring(BI.Title, 1, 50), SD.Accession, 'Textbook',&#xA;&#x9;SUBSTRING(S.CampusID, PATINDEX('%[^0]%', S.CampusID),8000), 'Lost Textbook',&#xA;&#x9;CONVERT(VARCHAR(10), SD.ModifiedDate, 101), CONVERT(varchar,PC.TotalOwed - PC.TotalPaid)&#xA;&#x9;FROM tblStudentsDistribution SD&#xA;&#x9;JOIN tblStudents S ON SD.StudentsUID = S.StudentsUID&#xA;&#x9;JOIN tblBookInventory BI ON SD.ISBN = BI.ISBN&#xA;&#x9;JOIN tblPaymentsCollected PC ON SD.Accession = PC.Accession&#xA;&#x9;AND SD.StudentsUID = PC.ID&#xA;&#x9;WHERE SD.Code = 'PRTL'&#xA;&#x9;) as Books&#xA;order by Amount&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
