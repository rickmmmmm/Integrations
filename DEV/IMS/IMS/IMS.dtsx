                  SQLTask:SqlStatementSource="&#xA;&#xA;--select *&#xA;update cca set TeacherEnrollment = ts.cnt&#xA;from tblCampusCoursesAssigned cca&#xA;join (&#xA;&#x9;select CampusCoursesAssignedUID, count(distinct teachersuid) as cnt&#xA;&#x9;from _IMS_TeacherSchedule&#xA;&#x9;where import = 1&#xA;&#x9;and CampusCoursesAssignedUID is not null&#xA;&#x9;group by CampusCoursesAssignedUID&#xA;&#x9;) ts on cca.CampusCoursesAssignedUID = ts.CampusCoursesAssignedUID&#xA;;&#xA;GO&#xA;--select *&#xA;update cca set StudentEnrollment = ss.cnt&#xA;from tblCampusCoursesAssigned cca&#xA;join (&#xA;&#x9;select CampusCoursesAssignedUID, count(distinct StudentsUID) as cnt&#xA;&#x9;from _IMS_StudentSchedule&#xA;&#x9;where import = 1&#xA;&#x9;and CampusCoursesAssignedUID is not null&#xA;&#x9;group by CampusCoursesAssignedUID&#xA;&#x9;) ss on cca.CampusCoursesAssignedUID = ss.CampusCoursesAssignedUID&#xA;;&#xA;GO&#xA;&#xA;&#xA;--declare @tblUpdate int&#xA;--select @tblUpdate = (select count(*)&#xA;--from sys.columns c&#xA;&#x9;&#x9;--join sys.tables t on c.object_id = t.object_id&#xA;&#x9;&#x9;--where c.name = 'teachercount'&#xA;&#x9;&#x9;--and t.name = '_IMS_CampusCourse'&#xA;&#x9;&#x9;--)&#xA;&#xA;--if @tblUpdate &gt; 0 &#xA;--BEGIN&#xA;&#xA;&#x9;--select cc.campusuid, cc.MasterCourseUID, cc.TeacherCount, cc.StudentCount, cca.StudentEnrollment, cca.TeacherEnrollment &#xA;&#x9;Update cca set TeacherEnrollment = cc.TeacherCount&#xA;&#x9;from _IMS_CampusCourse cc&#xA;&#x9;join tblCampusCoursesAssigned cca&#xA;&#x9;&#x9;on cc.MasterCourseUID = cca.MasterCourseUID&#xA;&#x9;&#x9;and cc.CampusUID = cca.CampusUID&#xA;&#x9;&#x9;where cc.TeacherCount is not null&#xA;&#x9;&#x9;and isNumeric(cc.TeacherCount) &gt; 0&#xA;&#x9;&#x9;and cc.TeacherCount &gt; 0&#xA;GO&#xA;&#xA;&#x9;--select cc.campusuid, cc.MasterCourseUID, cc.TeacherCount, cc.StudentCount, cca.StudentEnrollment, cca.TeacherEnrollment &#xA;&#x9;Update cca set StudentEnrollment = cc.StudentCount&#xA;&#x9;from _IMS_CampusCourse cc&#xA;&#x9;join tblCampusCoursesAssigned cca&#xA;&#x9;&#x9;on cc.MasterCourseUID = cca.MasterCourseUID&#xA;&#x9;&#x9;and cc.CampusUID = cca.CampusUID&#xA;&#x9;&#x9;where cc.StudentCount is not null&#xA;&#x9;&#x9;and isNumeric(cc.StudentCount) &gt; 0&#xA;&#x9;&#x9;and cc.StudentCount &gt; 0&#xA;&#xA;GO&#xA;&#xA;--END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
