            <SQLTask:SqlTaskData
              SQLTask:Connection="{392AD231-BF1F-4AF0-8055-B59EFA3B3BF4}"
              SQLTask:SqlStatementSource="DECLARE @ClientUID int, @rpt Nvarchar(max), @mailProfile NVARCHAR(MAX), @ZipExists int, @ZipFileSize NVARCHAR(MAX), @ZipFileSizeOver int &#xA;&#xA;DECLARE @body nvarchar(max), @subject NVARCHAR(max), @sText nvarchar(max), @logdate NVARCHAR(50) &#xA;DECLARE @ClientCatalog NVARCHAR(max), @CustomerName Nvarchar(max), @email NVARCHAR(MAX), @ccemail NVARCHAR(MAX)&#xA;DECLARE @StuDemog int, @TeachDemog int, @Staff int, @MasterCourse int, @CampusCourse int, @ClassList int, @StuSched int, @TeachSched int&#xA;declare @EmailAttachment int&#xA;&#xA;&#xA;DECLARE @sSqlBase Nvarchar(max), @sSql Nvarchar(max), @ParamDef NVARCHAR(max)&#xA;DECLARE @total int, @Filetotal INT, @Update INT, @New int, @reject INT &#xA;&#xA;set @ClientUID  = ?&#xA;SET @mailProfile = ?&#xA;set @rpt = ?&#xA;set @ZipExists = ?&#xA;set @ZipFileSize = ?&#xA;set @EmailAttachment = ?&#xA;&#xA;set @ZipFileSizeOver = 0&#xA;if cast(@ZipFileSize as int) &gt; 1000000 &#xA;&#x9;set @ZipFileSizeOver = 1&#x9;&#xA;&#xA;&#xA;&#xA;&#xA;SELECT @CustomerName = CustomerName, &#xA;@ClientCatalog = InitialCatalog, &#xA;@email  = RejectEmail, &#xA;@ccemail = RejectEmailCC, &#xA;@StuDemog = isNull(StudentDemog, 0),&#xA;@TeachDemog = isNull(TeacherDemog, 0), &#xA;@Staff = isNull(Staff, 0), &#xA;@MasterCourse = isNull(MasterCourse, 0),&#xA;@CampusCourse = isNull(CampusCourse, 0),&#xA;@ClassList = isNull(ClassList, 0),&#xA;@StuSched = isNull(StudentSchedule, 0),&#xA;@TeachSched = isNull(TeacherSchedule, 0)&#xA;FROM ImportManagementSystem.dbo.CustomerList &#xA;where CustomersUID = @ClientUID &#xA;and Active = 1&#xA;&#xA;&#xA;SELECT @logdate = CONVERT(VARCHAR(10), GETDATE(),101)&#xA;SET @subject= 'SIS Import Processing Log for ' + @CustomerName + ' (' + @logdate + ')'&#xA;&#xA;set @sSqlBase = N'SELECT @total = COUNT(*) FROM ' + @ClientCatalog + N'.dbo.'&#xA;set @ParamDef = N'@total int OUTPUT'&#xA;&#xA;&#xA;/*&#xA;Run Total, Accept, Reject for each module as it is developed&#xA;select @total = count(*) from _IMS_Students&#xA;select @total = count(*) from _IMS_Students where import = 0&#xA;select @total = count(*) from _IMS_Students where import = 1 and New = 1&#xA;select @total = count(*) from _IMS_Students where import = 1 and New = 0&#xA;&#xA;&#xA;*/&#xA;set @sText = ''&#xA;----------------------STUDENT DEMOGRAPHIC---------------------------&#xA;if @StuDemog &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Students where import = 1 and New = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Student Demographic: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;----------------------TEACHER DEMOGRAPHIC---------------------------&#xA;if @TeachDemog &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Teachers where import = 1 and New = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Teacher Demographic: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------STAFF---------------------------&#xA;if @Staff &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Staff '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Staff where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Staff where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Staff where import = 1 and New = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Staff: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------MASTER COURSE---------------------------&#xA;if @MasterCourse &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_MasterCourse '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_MasterCourse where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_MasterCourse where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_MasterCourse where import = 1 and New = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Master Course: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------CAMPUS COURSE---------------------------&#xA;if @CampusCourse &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_CampusCourse '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_CampusCourse where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_CampusCourse where import = 1 and New = 1 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_CampusCourse where import = 1 and New = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Campus Course: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------Class List---------------------------&#xA;if @ClassList &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Classes '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Classes where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_Classes where import = 1  '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Class List: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;&#xA;----------------------TEACHER SCHEDULE---------------------------&#xA;if @TeachSched &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_TeacherSchedule '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_TeacherSchedule where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_TeacherSchedule where import = 1 and New = 1 and TeachersUID is not null and MasterCourseUID is not null'&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_TeacherSchedule where import = 1 and New = 0 and TeachersUID is not null and MasterCourseUID is not null'&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Teacher Schedule: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------STUDENT SCHEDULE---------------------------&#xA;if @StuSched &gt; 0&#xA;BEGIN&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_StudentSchedule '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Filetotal = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_StudentSchedule where import = 0 '&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @reject = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_StudentSchedule where import = 1 and New = 1 and StudentsUID is not null and MasterCourseUID is not null'&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @New = @total&#xA;&#xA;&#x9;set @sSql = @sSqlBase + N'_IMS_StudentSchedule where import = 1 and New = 0 and StudentsUID is not null and MasterCourseUID is not null'&#xA;&#x9;exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA;&#x9;set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Student Schedule: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA;&#x9;'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA;&#x9;'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA;&#x9;'   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA;&#x9;' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;IF @ZipExists &gt; 0&#xA;BEGIN&#xA;&#x9;IF @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;set @sText = @sText + 'A zipped copy of the reject file(s) is attached.'  + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#x9;END&#xA;&#x9;ELSE&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;if @EmailAttachment &gt; 0&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;set @sText = @sText + 'The zipped copy of the reject file(s) was too large to attach.'  + '&lt;br/&gt;'  +&#xA; &#x9;&#x9;&#x9;&#x9;&#x9;'You can find the file in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#x9;&#x9;END&#xA;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;set @sText = @sText + 'You can find Reject Files (if any) in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#x9;&#x9;END&#xA;&#xA;&#x9;END&#xA;END&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;'&lt;p&gt;' + @sText + '&lt;/p&gt;&lt;br/&gt;' +&#xA;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9; '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;&#x9; '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;if @ZipExists &gt; 0 and @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;BEGIN&#xA;&#x9;--print @sText  + ' ' + 'attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject,&#xA;&#x9;&#x9;@file_attachments = @rpt;&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;&#xA;&#x9;--print @sText  + ' ' + 'no attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;&#x9;@profile_name = @mailProfile,&#xA;&#x9;&#x9;@recipients =@email,&#xA;&#x9;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;&#x9;@body_format = 'HTML',&#xA;&#x9;&#x9;@body = @body, &#xA;&#x9;&#x9;@subject = @subject;&#xA;END&#xA;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
