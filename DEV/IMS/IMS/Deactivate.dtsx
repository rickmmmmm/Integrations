LSE&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;if @EmailAttachment &gt; 0&#xA;&#x9;&#x9;&#x9;set @sText = @sText + 'The zipped copy of the reort file(s) was too large to attach.'  + '&lt;br/&gt;'  +&#xA;&#x9;&#x9;&#x9;'You can find the file in the &quot;Deactivated&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;&#x9;set @sText = @sText + 'You can find Report Files (if any) in the &quot;Deactivated&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;&#x9;END&#xA;END&#xA;&#xA;&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA;&#x9;&#x9;'&lt;p&gt;' + @sText + '&lt;/p&gt;&lt;br/&gt;' +&#xA;&#x9;&#x9; 'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;&#x9;&#x9;'&lt;p&gt;Hayes Software Systems&lt;/p&gt;' +&#xA;&#x9;&#x9;'&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;&#xA;if @ZipExists &gt; 0 and @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;BEGIN&#xA;&#x9;--print @sText  + ' ' + 'attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;@profile_name = @mailProfile,&#xA;&#x9;@recipients =@email,&#xA;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;@body_format = 'HTML',&#xA;&#x9;@body = @body, &#xA;&#x9;@subject = @subject,&#xA;&#x9;@file_attachments = @rpt;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA;&#x9;--print @sText  + ' ' + 'no attach'&#xA;&#x9;EXEC msdb.dbo.sp_send_dbmail&#xA;&#x9;@profile_name = @mailProfile,&#xA;&#x9;@recipients =@email,&#xA;&#x9;@copy_recipients = @ccemail,&#xA;&#x9;@body_format = 'HTML',&#xA;&#x9;@body = @body, &#xA;&#x9;@subject = @subject;&#xA;END&#xA;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
