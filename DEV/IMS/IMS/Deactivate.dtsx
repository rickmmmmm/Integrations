                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select t.TeacherID, t.CampusID, t.FirstName, t.Middlename, t.LastName, t.TeachersUID
from tblTeachers t
left outer join 
	(
	select distinct teachersuid, teacherid, campusid, lastname, firstname
	from _IMS_Teachers
	union
	select distinct teachersuid, staffID, campusID, lastname, firstname
	from _IMS_Staff
	) allStaff on t.teacherid = allStaff.teacherid and t.CampusID = allStaff.CampusID
where allStaff.teachersuid is null
and (activeit = 1)
and getdate() &gt; dateadd(d, 7, t.ModifiedDate)
