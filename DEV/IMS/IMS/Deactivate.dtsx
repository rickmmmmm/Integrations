                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select s.StudentID, s.CampusID, s.FirstName, s.Middlename, s.LastName, s.StudentsUID,
	(select count(*) from tblTechInventory where entityTypeUID = 4  and entityuid = s.studentsuid) as InventoryCount,
	(select count(*) from tblUnvCharges where entityTypeUID = 4 and ApplicationUID = 2 and entityuid = s.studentsuid and ChargeAmount &gt; 0 and DateSatisfied is null  and void &lt;&gt; 0) as ChargesCount,
