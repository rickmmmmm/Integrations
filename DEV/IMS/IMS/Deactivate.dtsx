rom _IMS_Students&#xA;select @total = count(*) from _IMS_Students where import = 0&#xA;select @total = count(*) from _IMS_Students where import = 1 and New = 1&#xA;select @total = count(*) from _IMS_Students where import = 1 and New = 0&#xA;&#xA;&#xA;*/&#xA;set @sText = ''&#xA;----------------------STUDENT DEMOGRAPHIC---------------------------&#xA;if @StuDemog &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_Students '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Students where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Students where import = 1 and New = 1 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Students where import = 1 and New = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Student Demographic: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;----------------------TEACHER DEMOGRAPHIC---------------------------&#xA;if @TeachDemog &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_Teachers '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Teachers where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Teachers where import = 1 and New = 1 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Teachers where import = 1 and New = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Teacher Demographic: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------STAFF---------------------------&#xA;if @Staff &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_Staff '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Staff where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Staff where import = 1 and New = 1 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Staff where import = 1 and New = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Staff: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------MASTER COURSE---------------------------&#xA;if @MasterCourse &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_MasterCourse '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_MasterCourse where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_MasterCourse where import = 1 and New = 1 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_MasterCourse where import = 1 and New = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Master Course: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------CAMPUS COURSE---------------------------&#xA;if @CampusCourse &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_CampusCourse '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_CampusCourse where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_CampusCourse where import = 1 and New = 1 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_CampusCourse where import = 1 and New = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Campus Course: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------Class List---------------------------&#xA;if @ClassList &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_Classes '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Classes where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_Classes where import = 1  '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Class List: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;&#xA;----------------------TEACHER SCHEDULE---------------------------&#xA;if @TeachSched &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_TeacherSchedule '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_TeacherSchedule where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_TeacherSchedule where import = 1 and New = 1 and TeachersUID is not null and MasterCourseUID is not null'&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_TeacherSchedule where import = 1 and New = 0 and TeachersUID is not null and MasterCourseUID is not null'&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Teacher Schedule: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;----------------------STUDENT SCHEDULE---------------------------&#xA;if @StuSched &gt; 0&#xA;BEGIN&#xA; set @sSql = @sSqlBase + N'_IMS_StudentSchedule '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Filetotal = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_StudentSchedule where import = 0 '&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @reject = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_StudentSchedule where import = 1 and New = 1 and StudentsUID is not null and MasterCourseUID is not null'&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @New = @total&#xA;&#xA; set @sSql = @sSqlBase + N'_IMS_StudentSchedule where import = 1 and New = 0 and StudentsUID is not null and MasterCourseUID is not null'&#xA; exec sp_executesql @sSql,  @ParamDef, @total=@total OUTPUT &#xA; set @Update = @total&#xA;&#xA;set @sText = @sText +'&lt;b&gt;&lt;i&gt;Student Schedule: &lt;/i&gt;&lt;/b&gt;&lt;br/&gt;&lt;br/&gt;' +&#xA; 'Total Records Received: ' + CONVERT(VARCHAR(50), @Filetotal) + '&lt;br/&gt;' +&#xA; 'Total Records Rejected: ' + CONVERT(VARCHAR(50), @reject) + '&lt;br/&gt;' +&#xA; '   Total Records Added: ' + CONVERT(VARCHAR(50), @New) + '&lt;br/&gt;' +&#xA; ' Total Records Updated: ' + CONVERT(VARCHAR(50), @Update) + '&lt;br/&gt;&lt;br/&gt;' &#xA;END&#xA;&#xA;IF @ZipExists &gt; 0&#xA;BEGIN&#xA; IF @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA; BEGIN&#xA;  set @sText = @sText + 'A zipped copy of the reject file(s) is attached.'  + '&lt;br/&gt;&lt;br/&gt;' &#xA; END&#xA; ELSE&#xA; BEGIN&#xA;  if @EmailAttachment &gt; 0&#xA;  BEGIN&#xA;   set @sText = @sText + 'The zipped copy of the reject file(s) was too large to attach.'  + '&lt;br/&gt;'  +&#xA;      'You can find the file in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;  END&#xA;  ELSE&#xA;  BEGIN&#xA;   set @sText = @sText + 'You can find Reject Files (if any) in the &quot;Reject&quot; folder of your SFTP location.' + '&lt;br/&gt;&lt;br/&gt;' &#xA;  END&#xA;&#xA; END&#xA;END&#xA;&#xA;SET @body = '&lt;html&gt;&lt;body&gt;' + @subject + &#xA; '&lt;p&gt;' + @sText + '&lt;/p&gt;&lt;br/&gt;' +&#xA;  'If you have any questions, please contact Customer Support at 1-800-495-5993.&lt;/p&gt;&lt;br&gt;' +&#xA;  '&lt;p&gt;Hayes Software Systems&lt;br&gt;1-800-495-5993&lt;/br&gt;&lt;/p&gt;' +&#xA;  '&lt;/body&gt;&lt;html&gt;'&#xA;&#xA;--print @body &#xA;&#xA;if @ZipExists &gt; 0 and @ZipFileSizeOver &lt; 1 and @EmailAttachment &gt; 0&#xA;BEGIN&#xA; --print @sText  + ' ' + 'attach'&#xA; EXEC msdb.dbo.sp_send_dbmail&#xA;  @profile_name = @mailProfile,&#xA;  @recipients =@email,&#xA;  @copy_recipients = @ccemail,&#xA;  @body_format = 'HTML',&#xA;  @body = @body, &#xA;  @subject = @subject,&#xA;  @file_attachments = @rpt;&#xA;&#xA;END&#xA;ELSE&#xA;BEGIN&#xA; &#xA; --print @sText  + ' ' + 'no attach'&#xA; EXEC msdb.dbo.sp_send_dbmail&#xA;  @profile_name = @mailProfile,&#xA;  @recipients =@email,&#xA;  @copy_recipients = @ccemail,&#xA;  @body_format = 'HTML',&#xA;  @body = @body, &#xA;  @subject = @subject;&#xA;END&#xA;&#xA;    &#xA;    &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
