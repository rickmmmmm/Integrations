          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{99615FE6-9ACE-4F0D-BE21-CD086645F00A}"
              SQLTask:SqlStatementSource="DECLARE&#xA;&#x9;@clientname NVARCHAR(100),&#xA;&#x9;@jobname NVARCHAR(100),&#xA;&#x9;@totcount int,&#xA;&#x9;@filename NVARCHAR(1000),&#xA;&#x9;@packagename NVARCHAR(100)&#xA;&#xA;SET @clientname = ?&#xA;SET @jobname = ?&#xA;SET @filename = ?&#xA;SET @packagename = ?&#xA;&#xA;SET @totcount = (SELECT COUNT(*) AS totalcount&#xA;FROM _ETL_Export)&#xA;&#xA;DELETE FROM _IntegrationDashboard.dbo.ErrorCount&#xA;WHERE Client = @clientname AND JobName = @jobname&#xA;&#xA;DELETE FROM _IntegrationDashboard.dbo.FileExist&#xA;WHERE client = @clientname AND job_name = @jobname&#xA;&#xA;DELETE FROM _IntegrationDashboard.dbo.TaskOnError&#xA;WHERE client = @clientname AND JobName = @jobname&#xA;&#xA;INSERT INTO _IntegrationDashboard.dbo.FileExist&#xA;(client, job_name, filecheck, filename, filesize, file_rcvd_date, category)&#xA;VALUES (@clientname, @jobname, 1, @filename, -1, GETDATE(), 'Fines')&#xA;&#xA;INSERT INTO _IntegrationDashboard.dbo.ErrorCount&#xA;(Client, JobName, TotalRecord, ErrorCount, ErrorRptPath, RptDate, Category)&#xA;VALUES (@clientname, @jobname , @totcount,  0, '', GETDATE(), 'Fines')&#xA;&#xA;IF EXISTS (SELECT * FROM sys.tables WHERE name = 'sysssislog')&#xA;BEGIN&#xA;&#x9;INSERT INTO _IntegrationDashboard.dbo.TaskOnError&#xA;&#x9;(Client, JobName, error_id, starttime, endtime, [MESSAGE], taskname, category)&#xA;&#x9;SELECT @clientname, @jobname, id AS error_id,&#xA;&#x9;starttime, endtime, [message], [source] AS taskname, 'Fines'&#xA;&#x9;FROM sysssislog&#xA;&#x9;WHERE (EVENT IN ('OnError'))&#xA;&#x9;AND source != @packagename&#xA;&#x9;ORDER BY id&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
