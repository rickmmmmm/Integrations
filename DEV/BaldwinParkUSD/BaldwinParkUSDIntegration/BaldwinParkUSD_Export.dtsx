            <SQLTask:SqlTaskData
              SQLTask:Connection="{99615FE6-9ACE-4F0D-BE21-CD086645F00A}"
              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo._ETL_Export', 'U') IS NOT NULL DROP TABLE dbo._ETL_Export;&#xA;&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;&#xA;GO&#xA;&#xA;SET QUOTED_IDENTIFIER ON&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[_ETL_Export](&#xA;&#x9;[ETLExportUID] [int] IDENTITY(1,1) NOT NULL,&#xA;&#x9;[AssetID] [varchar](100) NOT NULL,&#xA;&#x9;[SiteID] [varchar](100) NOT NULL,&#xA;&#x9;[Location] [varchar](50) NOT NULL,&#xA;&#x9;[Tag] [varchar](50) NOT NULL,&#xA;&#x9;[Serial] [varchar](50) NOT NULL,&#xA;&#x9;[ProductName] [varchar](100) NOT NULL,&#xA;&#x9;[Manufacturer] [varchar](100) NOT NULL,&#xA;&#x9;[Model] [varchar](100) NOT NULL,&#xA;&#x9;[ProductType] [varchar](50) NOT NULL,&#xA;&#x9;[ReceivedDate] [varchar](50) NOT NULL,&#xA;&#x9;[PurchaseOrder] [varchar](50) NOT NULL,&#xA;&#x9;[PurchasePrice] [varchar](50) NOT NULL,&#xA;&#x9;[AccountCode] [varchar](50) NOT NULL,&#xA;&#x9;[RetiredStatus] [varchar](50) NOT NULL,&#xA;&#x9;[RetiredDate] [varchar](50) NOT NULL,&#xA; CONSTRAINT [PK__ETL_Export] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[ETLExportUID] ASC&#xA;)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;SET ANSI_PADDING ON&#xA;&#xA;GO&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
